aV\u098f\u0996\u09be\u09a8\u0995\u09be\u09b0 \u09b8\u09c1\u09a8\u09cd\u09a6\u09b0 \u0997\u09cd\u09b0\u09be\u09ae\u09c7\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 \u09ac\u09c7\u09b0\u09cb\u0997 \u0993 \u0995\u09c1\u099f\u09be\u0999\u09cd\u0997\u09a3 \u0986\u09ae\u09be\u0995\u09c7 \u09b8\u09ac\u09a5\u09c7\u0995\u09c7 \u09ac\u09c7\u09b6\u09bf \u0986\u0995\u09b0\u09cd\u09b7\u09bf\u09a4 \u0995\u09b0\u09c7\u099b\u09c7 
p381
aV\u0995\u09bf\u09a8\u09cd\u09a4\u09c1 \u098f\u09b0 \u099a\u09bf\u0995\u09bf\u09a4\u09cd\u09b8\u09be \u09a8\u09be \u0995\u09b0\u09be \u09b9\u09b2\u09c7 , \u098f\u099f\u09be \u09b8\u09ae\u09df\u09c7\u09b0 \u09b8\u09be\u09a5\u09c7 \u09a7\u09c0\u09b0\u09c7 \u09a7\u09c0\u09b0\u09c7 \u09ac\u09c7\u09a1\u09bc\u09c7 \u09af\u09be\u09df ( \u0996\u09be\u09a8\u09bf\u0995\u099f\u09be \u09ac\u09c7\u09b6\u09bf \u09b8\u0982\u0995\u09cd\u09b0\u09ae\u09a3 ) \u0986\u09b0 \u09ad\u09ac\u09bf\u09b7\u09cd\u09af\u09a4\u09c7 \u099a\u09cb\u0996\u09c7\u09b0 \u0993\u09aa\u09b0\u09c7\u09b0 \u099d\u09bf\u09b2\u09cd\u09b2\u09c0\u0995\u09c7 ( \u0995\u09b0\u09cd\u09a8\u09bf\u09df\u09be ) \u0985\u09b8\u09cd\u09ac\u099a\u09cd\u099b \u0995\u09b0\u09c7 \u09a6\u09c7\u09df \u0986\u09b0 \u098f\u09b0 \u09ab\u09b2\u09c7 \u0985\u09a8\u09cd\u09a7\u09a4\u09cd\u09ac \u09b8\u09c3\u09b7\u09cd\u099f\u09bf \u09b9\u09df 
p382
aV\u09b9\u09cd\u09af\u09be\u0981 , \u099f\u09cd\u09b0\u09c7\u09a8\u09c7 \u09ac\u09bf\u09af\u09bc\u09c7 \u0993 \u09b0\u09af\u09bc\u09be\u09b2 \u09b0\u09be\u099c\u09b8\u09cd\u09a5\u09be\u09a8 \u0985\u09a8 \u09b9\u09c1\u0987\u09b2\u09b8\u09c7 \u09ae\u09a7\u09c1\u099a\u09a8\u09cd\u09a6\u09cd\u09b0\u09bf\u09ae\u09be\u09b0 \u09aa\u09cd\u09af\u09be\u0995\u09c7\u099c \u098f\u0987 \u0997\u09be\u09a1\u09bc\u09bf\u0997\u09c1\u09b2\u09cb\u09b0 \u09b8\u099e\u09cd\u099a\u09be\u09b2\u09a8\u09c7\u09b0 \u09a6\u09be\u09af\u09bc\u09bf\u09a4\u09cd\u09ac\u09c7 \u09a5\u09be\u0995\u09be \u09b0\u09be\u099c\u09b8\u09cd\u09a5\u09be\u09a8 \u09aa\u09b0\u09cd\u09af\u099f\u09a8 \u09ac\u09bf\u0995\u09be\u09b6 \u09a8\u09bf\u0997\u09ae ( \u0986\u099f\u09bf\u09a1\u09bf\u09b8\u09bf\u09b0 ) \u09af\u09cb\u099c\u09a8\u09be\u09b0 \u0985\u09a8\u09cd\u09a4\u09b0\u09cd\u09ad\u09c1\u0995\u09cd\u09a4 
p383
aV\u098f\u099f\u09be \u09ac\u09bf\u09a6\u09cd\u09af\u09be\u09b0\u09cd\u09a5\u09c0\u09a6\u09c7\u09b0 \u09ac\u09be \u09ac\u09c1\u09a6\u09cd\u09a7\u09bf\u099c\u09c0\u09ac\u09bf\u09a6\u09c7\u09b0 \u099c\u09a8\u09cd\u09af \u098f\u0995\u099f\u09be \u0989\u09a4\u09cd\u09a4\u09ae \u099f\u09a8\u09bf\u0995 
p384
aV\u09e7 \u09a5\u09c7\u0995\u09c7 \u09e8 \u099a\u09be\u09ae\u099a \u0985\u09b0\u09cd\u09a5\u09be\u09a4 \u09e7\u09e6 \u09a5\u09c7\u0995\u09c7 \u09e8\u09e6 \u0997\u09cd\u09b0\u09be\u09ae \u09b8\u0995\u09be\u09b2 - \u09ac\u09bf\u0995\u09c7\u09b2 \u09a6\u09c1\u09a7\u09c7\u09b0 \u09b8\u09be\u09a5\u09c7 \u09ac\u09be \u09b0\u09c1\u099f\u09bf \u0987\u09a4\u09cd\u09af\u09be\u09a6\u09bf\u09b0 \u09b8\u09be\u09a5\u09c7 \u099a\u09be\u099f\u09a8\u09bf\u09b0 \u09ae\u09a4\u09cb \u0996\u09c7\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7\u09a8 
p385
aV\u098f\u099f\u09be \u09ae\u09c7\u09a7\u09be\u09ac\u09b0\u09cd\u09a7\u0995 ( \u09ac\u09c1\u09a6\u09cd\u09a7\u09bf\u09ac\u09b0\u09cd\u09a7\u0995 ) , \u09a0\u09be\u09a8\u09cd\u09a1\u09be , \u09b6\u09b0\u09c0\u09b0\u09c7\u09b0 \u09b8\u09ac \u0985\u0999\u09cd\u0997\u0995\u09c7 \u09b6\u0995\u09cd\u09a4\u09bf , \u09aa\u09c1\u09b7\u09cd\u099f\u09bf \u0993 \u0986\u09b0\u09cb\u0997\u09cd\u09af \u09aa\u09cd\u09b0\u09a6\u09be\u09a8 \u0995\u09b0\u09c7 
p386
aV\u09b6\u09b0\u09c0\u09b0 \u09aa\u09c1\u09b7\u09cd\u099f \u0995\u09b0\u09c7 \u09ac\u09b2 , \u0995\u09be\u09a8\u09cd\u09a4\u09bf\u09ac\u09b0\u09cd\u09a7\u0995 , \u099a\u09cb\u0996\u09c7\u09b0 \u099c\u09a8\u09cd\u09af \u09ad\u09be\u09b2\u09cb , \u09a0\u09be\u09a8\u09cd\u09a1\u09be \u09b0\u09b8\u09be\u09df\u09a8 \u09af\u09be \u0997\u09cd\u09b0\u09c0\u09b7\u09cd\u09ae\u0995\u09be\u09b2\u09c7 \u09ac\u09bf\u09b6\u09c7\u09b7\u09ad\u09be\u09ac\u09c7 \u09b8\u09c7\u09ac\u09a8\u09c0\u09df 
p387
aV\u098f\u099f\u09be \u09b6\u09b0\u09c0\u09b0\u09c7\u09b0 \u09ae\u09c7\u09a6 \u0997\u09b2\u09bf\u09df\u09c7 \u09b9\u09be\u09a1\u09bc , \u09ae\u099c\u09cd\u099c\u09be \u0993 \u09b6\u09c1\u0995\u09cd\u09b0\u09be\u09a6\u09bf \u09a7\u09be\u09a4\u09c1\u0997\u09c1\u09b2\u09cb \u09aa\u09c1\u09b7\u09cd\u099f \u0995\u09b0\u09c7 
p388
aV\u0986\u099f\u09bf\u09a1\u09bf\u09b8\u09bf \u09af\u09a5\u09be\u0995\u09cd\u09b0\u09ae\u09c7 \u09a8\u09bf\u099c\u09b8\u09cd\u09ac \u098f\u099c\u09c7\u09a8\u09cd\u099f\u09a6\u09c7\u09b0 \u0986\u09a6\u09c7\u09b6 \u09a6\u09c7\u09af\u09bc \u09a4\u09be\u0981\u09b0\u09be \u09af\u09c7\u09a8 \u09b9\u09b2\u09bf\u0989\u09a1\u0995\u09c7 \u098f\u0987 \u09aa\u09cd\u09af\u09be\u0995\u09c7\u099c\u09b0 \u09b2\u0995\u09cd\u09b7\u09cd\u09af \u09ac\u09be\u09a8\u09be\u09af\u09bc 
p389
aV\u09ae\u09c7\u09a6\u09c7 \u09b0\u09c2\u09aa\u09be\u09a8\u09cd\u09a4\u09b0\u09bf\u09a4 \u0995\u09b0\u09c7 \u09b6\u09b0\u09c0\u09b0 \u09ae\u09be\u0982\u09b8\u09aa\u09c7\u09b6\u09c0\u09ac\u09b9\u09c1\u09b2 \u09ac\u09be\u09a8\u09be\u09df 
p390
aV\u09a6\u09bf\u09ac\u09cd\u09af \u09ae\u09c7\u09a6\u09cb\u09b9\u09b0 \u09ac\u099f\u09bf ( \u0993\u099c\u09a8\u09b9\u09c0\u09a8 ) - \u098f\u099f\u09be \u09aa\u09be\u099a\u09a8\u09a4\u09a8\u09cd\u09a4\u09cd\u09b0\u09c7\u09b0 \u09ac\u09bf\u0995\u09c3\u09a4\u09bf \u09a6\u09c2\u09b0 \u0995\u09b0\u09c7 \u09b6\u09b0\u09c0\u09b0\u09c7\u09b0 \u0985\u09a4\u09cd\u09af\u09a7\u09bf\u0995 \u09ae\u09c7\u09a6 \u0995\u09ae \u0995\u09b0\u09c7 \u09b6\u09b0\u09c0\u09b0\u0995\u09c7 \u09b8\u09c1\u09a8\u09cd\u09a6\u09b0 , \u09b8\u09c1\u09a1\u09cc\u09b2 , \u0995\u09be\u09a8\u09cd\u09a4\u09bf\u09ae\u09df \u0993 \u09b8\u09cd\u09ab\u09c1\u09b0\u09cd\u09a4\u09bf\u09ae\u09df \u09ac\u09be\u09a8\u09be\u09df 
p391
aV\u098f\u099f\u09be \u09a5\u09be\u0987\u09b0\u09a1\u09c7\u09b0 \u09ac\u09bf\u0995\u09c3\u09a4\u09bf , \u09b8\u09a8\u09cd\u09a7\u09bf\u09ac\u09be\u09a4 , \u0997\u09be\u0981\u099f\u09c7\u09b0 \u09ac\u09cd\u09af\u09be\u09a5\u09be , \u0995\u09cb\u09ae\u09b0 \u09ac\u09cd\u09af\u09be\u09a5\u09be , \u09b9\u09be\u0981\u099f\u09c1 \u09ac\u09cd\u09af\u09be\u09a5\u09be\u09df\u0993 \u09ac\u09bf\u09b6\u09c7\u09b7 \u09b2\u09be\u09ad\u09a6\u09be\u09df\u0995 
p392
aV\u09a6\u09cd\u09ac\u09bf\u09a4\u09c0\u09af\u09bc \u09ac\u09be\u09a3- \u0997\u0999\u09cd\u0997\u09be- \u09a6\u09af\u09bc\u09be\u09b2\u09aa\u09c1\u09b0\u09c7- \u0985\u09ac\u09b8\u09cd\u09a5\u09bf\u09a4 \u09b0\u09af\u09bc\u09c7\u099b\u09c7 
p393
aV\u09a8\u09be\u0997\u09be\u09b2\u09cd\u09af\u09be\u09a8\u09cd\u09a1\u09c7- \u09ac\u09af\u09bc\u09c7 \u09af\u09be\u0993\u09af\u09bc\u09be \u09aa\u09cd\u09b0\u09a7\u09be\u09a3 \u09a8\u09a6\u09c0 \u09b9\u09b2 \u09a7\u09a8\u09b6\u09cd\u09b0\u09c0- , \u09a6\u09cb\u09af\u09bc\u09be\u0999\u09cd\u0997- \u0986\u09b0 \u099d\u09be\u0981\u099c\u09bf- 
p394
aV\u09a8\u09be\u0997\u09be\u09b2\u09cd\u09af\u09be\u09a8\u09cd\u09a1\u09c7\u09b0- \u09aa\u09c1\u09b0\u09cb \u099c\u09a8\u09b8\u0982\u0996\u09cd\u09af\u09be\u0987 \u09b9\u09b2 \u0989\u09aa\u099c\u09be\u09a4\u09bf\u09b8\u09ae\u09cd\u09aa\u09cd\u09b0\u09a6\u09be\u09af\u09bc\u09ad\u09c1\u0995\u09cd\u09a4 
p395
aV` \u09ae\u09b9\u09be\u09ae\u09b9\u09bf\u09ae \u09b0\u09be\u09b7\u09cd\u099f\u09cd\u09b0\u09aa\u09a4\u09bf ` \u0985\u09b8\u09ae\u09c7\u09b0- \u09b0\u09be\u099c\u09cd\u09af\u09aa\u09be\u09b2\u09c7\u09b0 \u09ae\u09be\u09a7\u09cd\u09af\u09be\u09ae\u09c7 \u09a8\u09be\u0997\u09be\u09b2\u09cd\u09af\u09be\u09a8\u09cd\u09a1- \u09b6\u09be\u09b8\u09a8 \u0995\u09b0\u09a4\u09c7\u09a8 
p396
aV\u09e7\u09ef\u09ec\u09e7 \u09b8\u09be\u09b2\u09c7 \u09ad\u09be\u09b0\u09a4- \u09b8\u09b0\u0995\u09be\u09b0- \u09a8\u09be\u0997\u09be\u09b2\u09cd\u09af\u09be\u09a8\u09cd\u09a1\u0995\u09c7- \u09b0\u09be\u099c\u09cd\u09af\u09c7\u09b0 \u09a4\u0995\u09ae\u09be \u09a6\u09c7\u09a8 
p397
aV\u0997\u09c1\u09b0\u09c1\u09a4\u09cd\u09ac\u09aa\u09c2\u09b0\u09cd\u09a3 \u0990\u09a4\u09bf\u09b9\u09be\u09b8\u09bf\u0995 \u099c\u09be\u09af\u09bc\u0997\u09be\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 \u09a8\u09be\u0997\u09be\u09b2\u09cd\u09af\u09be\u09a8\u09cd\u09a1- \u09b0\u09be\u099c\u09cd\u09af\u09c7 \u0985\u09b8\u09ae\u09c7\u09b0- ` \u09ad\u09c2\u09a4\u09aa\u09c2\u09b0\u09cd\u09ac \u09a8\u09be\u0997\u09be- \u09aa\u09be\u09b9\u09be\u09a1\u09bc ` \u0986\u09b0 \u09aa\u09c2\u09b0\u09cd\u09ac\u09cb\u09a4\u09cd\u09a4\u09b0 \u09b8\u09c0\u09ae\u09be\u09a8\u09cd\u09a4\u09c7\u09b0 ` \u09ad\u09c2\u09a4\u09aa\u09c2\u09b0\u09cd\u09ac \u09a4\u09c1\u09af\u09bc\u09c7\u09a8\u09b8\u09be\u0999\u09cd\u0997- \u09b8\u09c0\u09ae\u09be\u09a8\u09cd\u09a4 \u09a1\u09bf\u09ad\u09bf\u09b6\u09a8\u0993 ` \u09aa\u09a1\u09bc\u09c7 
p398
aV\u09e7\u09ef\u09eb\u09ed \u09b8\u09be\u09b2\u09c7 \u09a8\u09be\u0997\u09be\u09b2\u09cd\u09af\u09be\u09a8\u09cd\u09a1\u0995\u09c7- \u0995\u09c7\u09a8\u09cd\u09a6\u09cd\u09b0\u09b6\u09be\u09b8\u09bf\u09a4 \u09aa\u09cd\u09b0\u09a6\u09c7\u09b6 \u0998\u09cb\u09b7\u09a3\u09be \u0995\u09b0\u09be \u09b9\u09af\u09bc\u09c7\u099b\u09bf\u09b2 
p399
aV\u09a8\u09be\u0997\u09be\u09b2\u09cd\u09af\u09be\u09a8\u09cd\u09a1\u09c7\u09b0- \u09b0\u09be\u099c\u09cd\u09af \u0985\u09b8\u09ae\u09c7\u09b0- \u09ac\u09cd\u09b0\u09b9\u09cd\u09ae\u09aa\u09c1\u09a4\u09cd\u09b0- \u0998\u09be\u099f \u0986\u09b0 \u09ac\u09b0\u09cd\u09ae\u09be\u09b0- \u09ae\u09be\u099d\u09c7 \u09aa\u09be\u09b9\u09be\u09a1\u09bc\u09c0 \u098f\u09b2\u09be\u0995\u09be\u09b0 \u09ac\u09c3\u0995\u09cd\u09b7\u09b6\u09cb\u09ad\u09bf\u09a4 \u0997\u09ad\u09c0\u09b0 \u0989\u09aa\u09a4\u09cd\u09af\u0995\u09be\u09b0 \u09ac\u09b2\u09af\u09bc\u09c7 \u0985\u09ac\u09b8\u09cd\u09a5\u09bf\u09a4 \u09b0\u09af\u09bc\u09c7\u099b\u09c7 
p400
aV\u09a8\u09be\u0997\u09be\u09b2\u09cd\u09af\u09be\u09a8\u09cd\u09a1\u09c7\u09b0- \u09aa\u09c2\u09b0\u09cd\u09ac\u09c7 \u09ad\u09be\u09b0\u09a4- \u0986\u09b0 \u09ac\u09b0\u09cd\u09ae\u09be\u09b0- \u0986\u09a8\u09cd\u09a4\u09b0\u09cd\u099c\u09be\u09a4\u09bf\u0995 \u09b8\u09c0\u09ae\u09be \u0986\u099b\u09c7 
p401
aV\u09e7\u09ef\u09ec\u09e9 \u09b8\u09be\u09b2\u09c7 \u09e7\u0987 \u09a1\u09bf\u09b8\u09c7\u09ae\u09cd\u09ac\u09b0- \u09a8\u09be\u0997\u09be\u09b2\u09cd\u09af\u09be\u09a8\u09cd\u09a1- \u09b0\u09be\u099c\u09cd\u09af\u09c7\u09b0 \u09ac\u09bf\u09a7\u09bf\u09ae\u09a4 \u0989\u09a6\u09cd\u0998\u09be\u099f\u09a8 \u09b9\u09af\u09bc 
p402
aV\u09a8\u09be\u0997\u09be\u09b2\u09cd\u09af\u09be\u09a8\u09cd\u09a1\u09c7\u09b0- \u09ac\u09bf\u0996\u09cd\u09af\u09be\u09a4 \u09a6\u09b0\u09cd\u09b6\u09a8\u09c0\u09af\u09bc \u09b8\u09cd\u09a5\u09be\u09a8\u09c7\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 \u09aa\u09a1\u09bc\u09c7 \u09af\u09c1\u09a6\u09cd\u09a7- \u0995\u09ac\u09cd\u09b0\u09bf\u09b8\u09cd\u09a4\u09be\u09a8- , \u09b8\u0982\u0997\u09cd\u09b0\u09b9\u09be\u09b2\u09af\u09bc , \u09aa\u09cd\u09b0\u09be\u09a3\u09c0 \u0989\u09a6\u09cd\u09af\u09be\u09a8 , \u0995\u09cd\u09af\u09be\u09a5\u09b2\u09bf\u0995 \u0997\u09c0\u09b0\u09cd\u099c\u09be\u0998\u09b0 
p403
aV\u09af\u09c7 \u09b8\u09ac \u099c\u09c7\u09b2\u09be\u09df \u09ac\u09bf\u09b6\u09c7\u09b7 \u09ac\u09bf\u09a6\u09cd\u09af\u09be\u09b2\u09df \u09a8\u09c7\u0987 , \u09b8\u09c7\u0996\u09be\u09a8\u09c7 \u09aa\u09cd\u09b0\u09be\u09a5\u09ae\u09bf\u0995 \u09b8\u09cd\u09a4\u09b0\u09c7 \u09b8\u09be\u09b9\u09be\u09af\u09cd\u09af \u0995\u09b0\u09be \u09b9\u09df 
p404
aV\u09af\u09c7 \u09b8\u0982\u09b8\u09cd\u09a5\u09be\u0997\u09c1\u09b2\u09bf \u09ac\u09bf\u09ad\u09bf\u09a8\u09cd\u09a8 \u09a7\u09b0\u09a8\u09c7\u09b0 \u09ac\u09bf\u0995\u09b2\u09be\u0999\u09cd\u0997\u09a6\u09c7\u09b0 \u099c\u09a8\u09cd\u09af \u09ac\u09bf\u09b6\u09c7\u09b7 \u09b8\u09cd\u0995\u09c1\u09b2 \u099a\u09be\u09b2\u09be\u09df \u09a4\u09be\u09a6\u09c7\u09b0 \u09ef\u09e6 \u09b6\u09a4\u09be\u0982\u09b6 \u09aa\u09b0\u09cd\u09af\u09a8\u09cd\u09a4 \u0986\u09b0\u09cd\u09a5\u09bf\u0995 \u09b8\u09be\u09b9\u09be\u09af\u09cd\u09af \u0995\u09b0\u09be \u09b9\u09df 
p405
aV\u09ac\u09bf\u0995\u09b2\u09be\u0999\u09cd\u0997 \u09ac\u09cd\u09af\u0995\u09cd\u09a4\u09bf\u09a6\u09c7\u09b0 \u09b8\u09be\u09a7\u09be\u09b0\u09a3 \u0995\u09be\u099c \u0995\u09b0\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u09b8\u09b9\u09be\u09df\u0995 \u0989\u09aa\u0995\u09b0\u09a3\u09c7\u09b0 \u09aa\u09cd\u09b0\u09df\u09cb\u099c\u09a8 \u09aa\u09a1\u09bc\u09c7 
p406
aV\u09ac\u09bf\u0995\u09b2\u09be\u0999\u09cd\u0997\u09b0\u09be \u09af\u09be\u09a4\u09c7 \u09ac\u09c7\u09b6\u09c0 \u09b0\u09cb\u099c\u0997\u09be\u09b0 \u0995\u09b0\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7 \u09a4\u09be\u09a6\u09c7\u09b0 \u09a1\u09bf\u09b2\u09be\u09b0\u09b6\u09bf\u09aa\u09c7 \u09ed \u09b6\u09a4\u09be\u0982\u09b6 \u0986\u09b0\u0995\u09cd\u09b7\u09a3 \u09aa\u09cd\u09b0\u09a6\u09be\u09a8 \u0995\u09b0\u09be \u09b9\u09df 
p407
aV\u09ac\u09bf\u09ae\u09be\u09a8 \u09af\u09be\u09a4\u09cd\u09b0\u09be\u09df \u09b6\u09c1\u09a7\u09c1 \u0987\u09a3\u09cd\u09a1\u09bf\u09df\u09be\u09a8- \u098f\u09df\u09be\u09b0\u09b2\u09be\u0987\u09a8\u09cd\u09b8- \u09eb\u09e6 \u09b6\u09a4\u09be\u0982\u09b6 \u099b\u09be\u09a1\u09bc \u09a6\u09c7\u09df \u09a6\u09c3\u09b7\u09cd\u099f\u09bf\u09b9\u09c0\u09a8\u09a6\u09c7\u09b0 \u099c\u09a8\u09cd\u09af 
p408
aV\u09ae\u09c2\u0995 - \u09ac\u09cb\u09a7\u09bf\u09b0\u09a6\u09c7\u09b0 \u099b\u09c7\u09a1\u09bc\u09c7 \u09ac\u09be\u0995\u09bf \u09b8\u09ac \u09a7\u09b0\u09a8\u09c7\u09b0 \u09ac\u09bf\u0995\u09b2\u09be\u0999\u09cd\u0997\u09a6\u09c7\u09b0 \u09b8\u09be\u09a5\u09c7 \u09a5\u09be\u0995\u09be \u09b8\u09b9\u09be\u09df\u0995\u09b0\u09be\u0993 \u098f\u09a4\u099f\u09be\u0987 \u099b\u09be\u09a1\u09bc \u09aa\u09be\u09df 
p409
aV\u098f\u0987 \u09ad\u09be\u09ac\u09c7 \u0995\u09c1\u09b7\u09cd\u09a0\u09b0\u09cb\u0997 \u099a\u09bf\u0995\u09bf\u09a4\u09cd\u09b8\u09be\u09b0 \u09ab\u09b2\u09c7 \u09a0\u09bf\u0995 \u09b9\u09df\u09c7 \u09af\u09be\u0993\u09df\u09be \u09ac\u09cd\u09af\u0995\u09cd\u09a4\u09bf\u09a6\u09c7\u09b0 \u09aa\u09c1\u09a8\u09b0\u09cd\u09ac\u09be\u09b8 \u0995\u09b0\u09a4\u09c7 \u0986\u09b0 \u09a4\u09be\u09a6\u09c7\u09b0 \u09b8\u09ae\u09be\u099c\u09c7\u09b0 \u09ae\u09c2\u09b2\u09b8\u09cd\u09b0\u09cb\u09a4\u09c7 \u09ab\u09bf\u09b0\u09bf\u09df\u09c7 \u0986\u09a8\u09a4\u09c7 \u09b8\u099a\u09c7\u09b7\u09cd\u099f \u09b8\u0982\u09b8\u09cd\u09a5\u09be\u0997\u09c1\u09b2\u09bf\u0995\u09c7\u0993 \u09ef\u09e6 \u09b6\u09a4\u09be\u0982\u09b6 \u09aa\u09b0\u09cd\u09af\u09a8\u09cd\u09a4 \u0986\u09b0\u09cd\u09a5\u09bf\u0995 \u09b8\u09b9\u09be\u09df\u09a4\u09be \u0995\u09b0\u09be \u09b9\u09df 
p410
aV\u09b8\u09c7\u09b0\u09bf\u09ac\u09cd\u09b0\u09be\u09b2 \u09aa\u0995\u09cd\u09b7\u09be\u0998\u09be\u09a4\u09c7\u09b0 \u09a6\u09cd\u09ac\u09be\u09b0\u09be \u0986\u0995\u09cd\u09b0\u09be\u09a8\u09cd\u09a4 \u0986\u09b0 \u09ae\u09be\u09a8\u09b8\u09bf\u0995\u09ad\u09be\u09ac\u09c7 \u0985\u09aa\u09b0\u09bf\u09a3\u09a4 \u09ac\u09be\u099a\u09cd\u099a\u09be\u09a6\u09c7\u09b0 \u09b6\u09bf\u0995\u09cd\u09b7\u09bf\u09a4 \u0995\u09b0\u09a4\u09c7 \u0986\u09b0 \u09a4\u09be\u09b0 \u09aa\u09c1\u09a8\u09b0\u09cd\u09ac\u09be\u09b8 \u0995\u09b0\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u09b6\u09bf\u0995\u09cd\u09b7\u0995 \u0986\u09b0 \u0985\u09a8\u09cd\u09af \u09ac\u09bf\u09b6\u09c7\u09b7\u099c\u09cd\u099e \u09a4\u09c8\u09b0\u09bf \u0995\u09b0\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u09b8\u09cd\u09ac\u09df\u0982\u09b8\u09c7\u09ac\u09c0 \u09b8\u0982\u09b8\u09cd\u09a5\u09be\u09a6\u09c7\u09b0 \u09b8\u09b9\u09be\u09df\u09a4\u09be \u0995\u09b0\u09be \u09b9\u09df 
p411
aV\u09ac\u09be\u09b0\u09b6\u09cb \u099f\u09be\u0995\u09be \u09aa\u09b0\u09cd\u09af\u09a8\u09cd\u09a4 \u09ae\u09be\u09b8\u09bf\u0995 \u0986\u09df\u0993\u09df\u09be\u09b2\u09be \u09ac\u09cd\u09af\u0995\u09cd\u09a4\u09bf\u09a6\u09c7\u09b0 \u09e9\u09ec\u09e6\u09e6 \u099f\u09be\u0995\u09be \u09aa\u09b0\u09cd\u09af\u09a8\u09cd\u09a4 \u09b8\u09b9\u09be\u09df\u0995 \u0989\u09aa\u0995\u09b0\u09a3 \u09ac\u09bf\u09a8\u09be\u09ae\u09c2\u09b2\u09cd\u09af \u09a6\u09c7\u0993\u09df\u09be \u09b9\u09df 
p412
aV\u0985\u09a4\u0983\u09aa\u09b0 \u09a8\u09bf\u09b0\u09cd\u09a7\u09a8 \u09ac\u09bf\u0995\u09b2\u09be\u0999\u09cd\u0997\u09a6\u09c7\u09b0 \u09b8\u09b9\u09be\u09df\u0995 \u0989\u09aa\u0995\u09b0\u09a3 \u0995\u09c7\u09a8\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u09b8\u09b9\u09be\u09df\u09a4\u09be \u0995\u09b0\u09be \u09b9\u09df 
p413
aV\u09e7\u09e7\u09ed\u09ea - \u09e7\u09e9\u09eb\u09e6\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 \u09b8\u09be\u09a6\u09be \u09ae\u09cb\u099c\u09be\u0987\u0995\u09c7\u09b0 \u09a4\u09c8\u09b0\u09bf \u099a\u09be\u09b0\u09cd\u099a\u09c7\u09b0 \u09b8\u09be\u09a5\u09c7 \u2018 \u0995\u09cd\u09b2\u09be\u0999\u09cd\u0995- \u099f\u09be\u0993\u09af\u09bc\u09be\u09b0\u09c7\u09b0- \u2019 \u09b0\u09c2\u09aa\u09c7 \u09e7\u09ea\u0987 \u09b6\u09a4\u09be\u09ac\u09cd\u09a6\u09c0\u09a4\u09c7 \u098f\u09b0 \u09a8\u09bf\u09b0\u09cd\u09ae\u09be\u09a3 \u0995\u09b0\u09be \u09b9\u09af\u09bc 
p414
aV\u099b\u09cb\u099f \u098f\u0995\u099f\u09be \u09ae\u09af\u09bc\u09a6\u09be\u09a8\u09c7 \u099a\u09be\u09b0\u09cd\u099a\u09c7\u09b0 \u09aa\u09bf\u099b\u09a8\u09c7 \u09a4\u09c8\u09b0\u09bf \u09e7\u09ee\u09e6 \u09ab\u09c1\u099f \u0989\u0981\u099a\u09c1 \u2018 \u099d\u09cb\u0981\u0995\u09be\u09a8\u09cb- \u09ae\u09c0\u09a8\u09be\u09b0- \u2019 \u09a6\u09c7\u0996\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u09b8\u09be\u09b0\u09be \u09ac\u09bf\u09b6\u09cd\u09ac \u09a5\u09c7\u0995\u09c7 \u09aa\u09b0\u09cd\u09af\u099f\u0995\u09b0\u09be \u098f\u09b8\u09c7 \u09ad\u09bf\u09a1\u09bc \u0995\u09b0\u09c7 
p415
aV\u09aa\u09b0\u09cd\u09af\u099f\u09a8 \u09ac\u09bf\u09ad\u09be\u0997 \u098f\u0987 \u09ac\u09cd\u09af\u09ac\u09b8\u09be\u09af\u09bc \u0989\u09a4\u09b8\u09be\u09b9 \u09a6\u09c7\u0993\u09af\u09bc\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u09af\u09c1\u09ac\u0995\u09a6\u09c7\u09b0 \u0986\u09ac\u09b6\u09cd\u09af\u0995 \u09aa\u09cd\u09b0\u09b6\u09bf\u0995\u09cd\u09b7\u09a3 \u09aa\u09cd\u09b0\u09a6\u09be\u09a8 \u0995\u09b0\u099b\u09c7 
p416
aV\u09a4\u09be\u09b0\u09be \u09ac\u09b2\u09c7\u09a8 \u09af\u09c7 \u09ae\u09be\u09a8\u09c1\u09b7 \u098f\u0996\u09a8 \u09b8\u09cd\u09ac\u09be\u09b8\u09cd\u09a5\u09cd\u09af\u09c7\u09b0 \u09aa\u09cd\u09b0\u09a4\u09bf \u09ac\u09c7\u09b6\u09bf \u09b8\u099a\u09c7\u09a4\u09a8 \u09b9\u09af\u09bc\u09c7 \u0997\u09c7\u099b\u09c7 
p417
aV\u09aa\u09b0\u09cd\u09af\u099f\u0995 \u09aa\u09cd\u09b0\u0995\u09c3\u09a4\u09bf\u09b0 \u09af\u09be\u09a4\u09c7 \u09aa\u09c1\u09b0\u09cb \u09b2\u09be\u09ad \u0993\u09a0\u09be\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7 , \u09a4\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u09ac\u09bf\u09b6\u09c7\u09b7 \u09b8\u09c1\u09ac\u09bf\u09a7\u09be \u09aa\u09cd\u09b0\u09a6\u09be\u09a8 \u0995\u09b0\u09be\u09b0 \u09a6\u09bf\u0995\u09c7 \u09b0\u09be\u099c\u09cd\u09af \u09b8\u09b0\u0995\u09be\u09b0 \u09aa\u09cd\u09b0\u099a\u09c7\u09b7\u09cd\u099f\u09be \u099a\u09be\u09b2\u09be\u099a\u09cd\u099b\u09c7 
p418
aV\u098f\u09b0 \u09ab\u09b2\u09c7 \u09b8\u09bf\u0995\u09cd\u0995\u09bf\u09ae\u09c7\u09b0 \u09ae\u09a4\u09cb \u09aa\u09be\u09b9\u09be\u09a1\u09bc\u09bf \u09b0\u09be\u099c\u09cd\u09af\u09c7 \u09aa\u09cd\u09b0\u09be\u0995\u09c3\u09a4\u09bf\u0995 \u099a\u09bf\u0995\u09bf\u09a4\u09b8\u09be\u09b0 \u0996\u09c1\u09ac \u09ad\u09be\u09b2\u09cb \u09ac\u09cd\u09af\u09ac\u09b8\u09be \u09b9\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7 
p419
aV\u09b0\u09be\u099c\u09cd\u09af\u09c7\u09b0 \u09ac\u09a8 \u0993 \u09aa\u09b0\u09cd\u09af\u099f\u09a8 \u09ae\u09a8\u09cd\u09a4\u09cd\u09b0\u09c0 \u09ad\u09c0\u09ae \u09a1\u09c1\u0999\u09cd\u0997\u09c7\u09b2 \u09af\u09be\u09a4\u09cd\u09b0\u09be\u09b0 \u09ac\u09bf\u09b7\u09af\u09bc\u09c7 \u0986\u09b2\u09cb\u099a\u09a8\u09be\u09af\u09bc \u09ac\u09b2\u09c7\u09a8 \u09af\u09c7 \u09b0\u09be\u099c\u09cd\u09af \u09aa\u09cd\u09b0\u09be\u0995\u09c3\u09a4\u09bf\u0995 \u09b8\u0982\u09b8\u09be\u09a7\u09a8\u09c7\u09b0 \u0995\u09cd\u09b7\u09c7\u09a4\u09cd\u09b0\u09c7 \u0985\u09a4\u09bf \u09b8\u09ae\u09cd\u09aa\u09a8\u09cd\u09a8 
p420
aV\u098f\u0987 \u09ac\u09bf\u09b6\u09c7\u09b7\u09a4\u09be\u0993 \u09aa\u09b0\u09cd\u09af\u099f\u09a8\u09c7\u09b0 \u09b8\u09be\u09a5\u09c7 \u09b8\u0982\u09af\u09c1\u0995\u09cd\u09a4 \u0995\u09b0\u09be\u09b0 \u09af\u09cb\u099c\u09a8\u09be \u0986\u099b\u09c7 
p421
aV\u09b8\u09bf\u0995\u09cd\u0995\u09bf\u09ae\u09c7 \u0986\u099c \u0985\u09ac\u09a7\u09bf \u0998\u09c1\u09b0\u09c7\u0987 \u09ae\u09be\u09a8\u09b8\u09bf\u0995 \u09b6\u09be\u09a8\u09cd\u09a4\u09bf \u09aa\u09be\u0993\u09af\u09bc\u09be \u09af\u09be\u099a\u09cd\u099b\u09bf\u09b2 \u0995\u09bf\u09a8\u09cd\u09a4\u09c1 \u098f\u0996\u09a8 \u09ac\u09bf\u09b6\u09cd\u09ac\u09b8\u09cd\u09a4\u09b0\u09c0\u09af\u09bc \u09aa\u09cd\u09b0\u09be\u0995\u09c3\u09a4\u09bf\u0995 \u099a\u09bf\u0995\u09bf\u09a4\u09b8\u09be \u0993 \u09ae\u09be\u09b2\u09bf\u09b6 \u09aa\u09be\u09b0\u09cd\u09b2\u09be\u09b0 \u0996\u09cb\u09b2\u09be\u09b0 \u09af\u09cb\u099c\u09a8\u09be \u09a4\u09c8\u09b0\u09bf \u0995\u09b0\u09be \u09b9\u099a\u09cd\u099b\u09c7 
p422
aV\u098f\u099f\u09be\u0995\u09c7 \u09aa\u09b0\u09cd\u09af\u099f\u09a8\u09c7\u09b0 \u09b8\u09be\u09a5\u09c7 \u099c\u09cb\u09a1\u09bc\u09be\u09b0 \u09aa\u09cd\u09b0\u09b8\u09cd\u09a4\u09c1\u09a4\u09bf \u099a\u09b2\u099b\u09c7 
p423
aV\u09a4\u09c3\u09a4\u09c0\u09af\u09bc ` \u09ac\u09be\u09a3- \u0997\u0999\u09cd\u0997\u09be- ` \u0995\u09c1\u09b0\u09c1\u0995\u09cd\u09b7\u09c7\u09a4\u09cd\u09b0- \u09b0\u09c7\u09b2\u0993\u09af\u09bc\u09c7 \u09b8\u09cd\u099f\u09c7\u09b6\u09a8 \u09a5\u09c7\u0995\u09c7 \u09ef \u0995\u09bf\u09b2\u09cb\u09ae\u09bf\u099f\u09be\u09b0 \u09a6\u09c2\u09b0\u09c7 ` \u099c\u09ae\u09c0\u09a8- ` \u0997\u09cd\u09b0\u09be\u09ae\u09c7\u09b0 \u0995\u09be\u099b\u09c7\u0987 \u0986\u099b\u09c7 
p424
aV\u09af\u09c7\u09b9\u09c7\u09a4\u09c1 \u09b8\u09bf\u0995\u09cd\u0995\u09bf\u09ae\u09c7 \u0994\u09b7\u09a7\u09c0\u09af\u09bc \u0997\u09be\u099b\u0997\u09be\u099b\u09be\u09b2\u09bf \u09aa\u09be\u0993\u09af\u09bc\u09be \u09af\u09be\u09af\u09bc , \u09a4\u09be\u0987 \u09ac\u09bf\u09ad\u09bf\u09a8\u09cd\u09a8 \u09ac\u09cd\u09af\u09ac\u09b8\u09be\u09af\u09bc\u09bf\u0995 \u0995\u09cd\u09b7\u09c7\u09a4\u09cd\u09b0\u09c7 \u098f\u09b0 \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0 \u09ae\u09be\u09a5\u09be\u09af\u09bc \u09b0\u09c7\u0996\u09c7 \u09ac\u09bf\u09b6\u09cd\u09ac\u09b8\u09cd\u09a4\u09b0\u09c0\u09af\u09bc \u09ac\u09bf\u09b6\u09c7\u09b7\u099c\u09cd\u099e\u09a6\u09c7\u09b0 \u09a5\u09c7\u0995\u09c7 \u098f\u0987 \u09ac\u09cd\u09af\u09be\u09aa\u09be\u09b0\u09c7 \u09aa\u09b0\u09be\u09ae\u09b0\u09cd\u09b6 \u09a8\u09c7\u0993\u09af\u09bc\u09be \u09b9\u099a\u09cd\u099b\u09c7 
p425
aV\u098f\u0995\u0987 \u09ad\u09be\u09ac\u09c7 \u0995\u09c7\u09b0\u09b2\u09c7\u09b0 \u09ae\u09a4\u09cb \u09aa\u09cd\u09b0\u09be\u0995\u09c3\u09a4\u09bf\u0995 \u0994\u09b7\u09a7\u09c0 \u09a4\u09c7\u09b2\u09c7\u09b0 \u09ae\u09be\u09b2\u09bf\u09b6 , \u099c\u09c8\u09ac\u09bf\u0995 \u09ac\u09b0\u09cd\u099c\u09cd\u09af \u09aa\u09a6\u09be\u09b0\u09cd\u09a5 \u09a5\u09c7\u0995\u09c7 \u0989\u09a4\u09aa\u09be\u09a6\u09bf\u09a4 , \u09b8\u09ac\u09c1\u099c \u09b8\u09ac\u09cd\u099c\u09bf \u0993 \u09b0\u09be\u09a8\u09cd\u09a8 \u0987\u09a4\u09cd\u09af\u09be\u09a6\u09bf \u0989\u09aa\u09b2\u09ac\u09cd\u09a7\u09bf \u0995\u09b0\u09be\u09a8\u09cb \u09af\u09cb\u099c\u09a8\u09be \u09a4\u09c8\u09b0\u09bf \u0995\u09b0\u09be \u09b9\u099a\u09cd\u099b\u09c7 
p426
aV\u09a8\u09bf\u099c\u09c7\u09b0 \u09a1\u09be\u09df\u09c7\u099f\u09c7 \u09ae\u09be\u099b\u09c7\u09b0 \u09a4\u09c7\u09b2 \u09b0\u09be\u0996\u09c1\u09a8 
p427
aV\u098f\u09a4\u09c7 \u0985\u09a8\u09c7\u0995 \u09ab\u09cd\u09af\u09be\u099f\u09bf \u0985\u09cd\u09af\u09be\u09b8\u09bf\u09a1\u09b8 \u09a5\u09be\u0995\u09c7 , \u09af\u09be \u09b9\u09be\u09b0\u09cd\u099f \u0985\u09cd\u09af\u09be\u099f\u09be\u0995 \u0986\u09b0 \u0995\u09cd\u09af\u09be\u09a8\u09b8\u09be\u09b0\u09c7\u09b0 \u09b8\u09be\u09a5\u09c7 \u09b2\u09a1\u09bc\u09be\u0987 \u0995\u09b0\u09a4\u09c7 \u09b8\u09be\u09b9\u09be\u09af\u09cd\u09af \u0995\u09b0\u09c7 
p428
aV\u09ab\u09b2 \u0986\u09b0 \u09b8\u09ac\u099c\u09bf\u09a4\u09c7 \u09a5\u09be\u0995\u09be \u099a\u09b0\u09cd\u09ac\u09bf \u09b9\u09b2 \u09b6\u09b0\u09c0\u09b0\u09c7\u09b0 \u09aa\u0995\u09cd\u09b7\u09c7 \u0989\u09aa\u0995\u09be\u09b0\u09c0 
p429
aV\u098f\u09ae\u09a8\u0995\u09bf \u0986\u09aa\u09a8\u09bf \u09b8\u09a0\u09bf\u0995\u09ad\u09be\u09ac\u09c7 \u09a8\u09bf\u09b6\u09cd\u09ac\u09be\u09b8 \u09a8\u09c7\u0993\u09df\u09be \u0985\u09ad\u09cd\u09af\u09be\u09b8 \u0995\u09b0\u09c7\u0993 \u09aa\u09c7\u099f\u09c7\u09b0 \u099a\u09b0\u09cd\u09ac\u09bf \u0995\u09ae \u0995\u09b0\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7\u09a8 
p430
aV\u098f\u09b0 \u09ab\u09b2\u09c7 \u09ab\u09c1\u09b8\u09ab\u09c1\u09b8 \u09aa\u09b0\u09bf\u09b7\u09cd\u0995\u09be\u09b0 \u09b9\u09df\u09c7 \u09af\u09be\u09ac\u09c7 , \u09af\u09c7\u0995\u09cb\u09a8\u0993 \u099c\u09bf\u09a8\u09bf\u09b8\u09c7\u09b0 \u0993\u09aa\u09b0\u09c7 \u09ae\u09a8\u09cb\u09af\u09cb\u0997 \u0995\u09c7\u09a8\u09cd\u09a6\u09cd\u09b0\u09c0\u09ad\u09c2\u09a4 \u0995\u09b0\u09a4\u09c7 \u09b8\u09be\u09b9\u09be\u09af\u09cd\u09af \u09aa\u09be\u0993\u09df\u09be \u09af\u09be\u09ac\u09c7 
p431
aV\u0997\u09ad\u09c0\u09b0 \u09a8\u09bf\u09b6\u09cd\u09ac\u09be\u09b8 \u09a8\u09bf\u09a8 
p432
aV\u0996\u09be\u09ac\u09be\u09b0\u09c7\u09b0 \u09b8\u09cd\u09ac\u09be\u09a6 \u09ad\u09be\u09b2\u09cb \u0995\u09b0\u09be\u09b0 \u09b8\u09be\u09a5\u09c7 \u09b8\u09be\u09a5\u09c7 \u09a4\u09c7\u099c\u09aa\u09be\u09a4\u09be \u0985\u09a8\u09c7\u0995 \u09a7\u09b0\u09a3\u09c7\u09b0 \u09b8\u09cd\u09ac\u09be\u09b8\u09cd\u09a5\u09cd\u09af\u09b8\u0982\u0995\u09cd\u09b0\u09be\u09a8\u09cd\u09a4 \u09b8\u09ae\u09b8\u09cd\u09af\u09be \u09a5\u09c7\u0995\u09c7 \u0986\u09b0\u09be\u09ae \u09a6\u09c7\u09df 
p433
aV\u09af\u09c7\u0995\u09cb\u09a8\u0993 \u09a7\u09b0\u09a3\u09c7\u09b0 \u099a\u09cb\u099f \u09b2\u09be\u0997\u09b2\u09c7 , \u09a4\u09c7\u099c\u09aa\u09be\u09a4\u09be \u09ac\u09c7\u099f\u09c7 \u09b8\u09c7\u0987 \u09b8\u09cd\u09a5\u09be\u09a8\u09c7 \u09b2\u09be\u0997\u09bf\u09df\u09c7 \u0986\u09b0 \u09a4\u09be\u09b0 \u0993\u09aa\u09b0\u09c7 \u0995\u09be\u09aa\u09a1\u09bc \u09ac\u09c7\u0981\u09a7\u09c7 \u09a6\u09c7\u0993\u09df\u09be\u09b0 \u09ab\u09b2\u09c7 \u0995\u09bf\u099b\u09c1 \u09a6\u09bf\u09a8\u09c7\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7\u0987 \u0986\u09b0\u09be\u09ae \u09aa\u09be\u0993\u09df\u09be \u09af\u09be\u09df 
p434
aV\u09ac\u09be\u0987\u09b0\u09c7 \u09ac\u09be\u09b0 \u0995\u09b0\u09c7 \u09ae\u09c2\u09b2\u09ac\u09a8\u09cd\u09a7\u09a8 , \u0989\u09a1\u09cd\u09a1\u09c0\u09df\u09be\u09a8 \u09ac\u09a8\u09cd\u09a7\u09a8 \u098f\u09ac\u0982 \u099c\u09be\u09b2\u09a8\u09cd\u09a7\u09b0 \u09ac\u09a8\u09cd\u09a7\u09a8 \u09b2\u09be\u0997\u09bf\u09df\u09c7 \u09a8\u09bf\u09b6\u09cd\u09ac\u09be\u09b8\u0995\u09c7 \u09af\u09a4\u099f\u09be \u09aa\u09be\u09b0\u09c7\u09a8 \u09ac\u09be\u0987\u09b0\u09c7\u0987 \u0986\u099f\u0995\u09c7 \u09b0\u09be\u0996\u09c1\u09a8 
p435
aV\u09ac\u09be\u09b9\u09cd\u09af\u09ac\u09c3\u09a4\u09cd\u09a4\u09bf \u09aa\u09cd\u09b0\u09be\u09a3\u09be\u09df\u09be\u09ae \u09ac\u09bf\u09a7\u09bf\u09a4\u09c7 \u09b8\u09bf\u09a6\u09cd\u09a7\u09be\u09b8\u09a8 \u09ac\u09be \u09aa\u09a6\u09cd\u09ae\u09be\u09b8\u09a8\u09c7 \u09ac\u09bf\u09a7\u09bf\u09ae\u09a4 \u09ac\u09b8\u09c7 \u09b6\u09cd\u09ac\u09be\u09b8\u0995\u09c7 \u098f\u0995\u099f\u09bf \u09ac\u09be\u09b0\u09c7\u0987 \u09af\u09a4\u099f\u09be \u09aa\u09be\u09b0\u09c7\u09a8 \u09ac\u09be\u0987\u09b0\u09c7 \u09ac\u09be\u09b0 \u0995\u09b0\u09c7 \u09a6\u09bf\u09a8 
p436
aV\u09a8\u09bf\u09b6\u09cd\u09ac\u09be\u09b8 \u09ad\u09c7\u09a4\u09b0\u09c7 \u09a8\u09bf\u09df\u09c7 \u09a4\u09be\u0995\u09c7 \u09a8\u09be \u0986\u099f\u0995\u09bf\u09df\u09c7\u0987 \u0986\u09ac\u09be\u09b0 \u09b6\u09cd\u09ac\u09b8\u09a8 - \u0995\u09cd\u09b0\u09bf\u09df\u09be \u0995\u09b0\u09c1\u09a8 
p437
aV\u09af\u0996\u09a8 \u09a8\u09bf\u09b6\u09cd\u09ac\u09be\u09b8 \u09a8\u09bf\u09a4\u09c7 \u0987\u099a\u09cd\u099b\u09be \u09b9\u09ac\u09c7 , \u09a4\u0996\u09a8 \u09ac\u09a8\u09cd\u09a7\u09a8 \u09b8\u09b0\u09bf\u09df\u09c7 \u09a7\u09c0\u09b0\u09c7 - \u09a7\u09c0\u09b0\u09c7 \u09a8\u09bf\u09b6\u09cd\u09ac\u09be\u09b8 \u09a8\u09bf\u09a8 
p438
aV\u09aa\u09cd\u09b0\u09a3\u09ac- \u098f\u09ac\u0982 \u0997\u09be\u09df\u09a4\u09cd\u09b0\u09c0- \u0987\u09a4\u09cd\u09af\u09be\u09a6\u09bf \u09ae\u09a8\u09cd\u09a4\u09cd\u09b0 \u09a6\u09c0\u09b0\u09cd\u0998\u0995\u09cd\u09b7\u09a3 \u09b6\u09cd\u09b0\u09a6\u09cd\u09a7\u09be\u09aa\u09c2\u09b0\u09cd\u09ac\u0995 \u099c\u09aa \u0995\u09b0\u09b2\u09c7 \u09a4\u09be \u09b6\u0995\u09cd\u09a4\u09bf , \u09b6\u09be\u09a8\u09cd\u09a4\u09bf \u0993 \u0986\u09a8\u09a8\u09cd\u09a6\u09aa\u09cd\u09b0\u09a6\u09be\u09a8 \u0995\u09b0\u09c7 
p439
aV\u09b6\u09cc\u099a , \u09b8\u09cd\u09a8\u09be\u09a8 , \u0986\u09b8\u09a8\u09be\u09a6\u09bf \u09a8\u09bf\u09a4\u09cd\u09af\u0995\u09b0\u09cd\u09ae \u09a5\u09c7\u0995\u09c7 \u09a8\u09bf\u09ac\u09c3\u09a4\u09cd\u09a4 \u09b9\u09df\u09c7 \u09b8\u09c1\u0996 , \u09b6\u09be\u09a8\u09cd\u09a4\u09bf \u098f\u09ac\u0982 \u0986\u09a8\u09a8\u09cd\u09a6 \u0995\u09be\u09ae\u09a8\u09be \u0995\u09b0\u09c7\u09a8 \u098f\u09ae\u09a8 \u09ac\u09cd\u09af\u09be\u0995\u09cd\u09a4\u09bf\u09a6\u09c7\u09b0 \u09aa\u09cd\u09b0\u09a4\u09bf\u09a6\u09bf\u09a8 \u0995\u09ae \u0995\u09b0\u09c7 \u09e7\u09eb \u09ae\u09bf\u09a8\u09bf\u099f \u09a5\u09c7\u0995\u09c7 \u09b6\u09c1\u09b0\u09c1 \u0995\u09b0\u09c7 \u098f\u0995 \u0998\u09a8\u09cd\u099f\u09be \u09aa\u09b0\u09cd\u09af\u09a8\u09cd\u09a4 \u09ad\u0997\u09ac\u09be\u09a8\u09c7\u09b0 \u09a7\u09cd\u09af\u09be\u09a8 \u0989\u09aa\u09be\u09b8\u09a8\u09be \u0985\u09ac\u09b6\u09cd\u09af\u0987 \u0995\u09b0\u09be \u0989\u099a\u09bf\u09a4 
p440
aV\u098f\u099f\u09be \u09a8\u09bf\u09df\u09ae\u09bf\u09a4 \u09b8\u09c7\u09ac\u09a8 \u0995\u09b0\u09b2\u09c7 \u09ab\u09bf\u099f \u09a5\u09be\u0995\u09ac\u09c7\u09a8 \u0986\u09b0 \u09ac\u09df\u09b8\u09c7\u09b0 \u099b\u09be\u09aa\u0993 \u0986\u09aa\u09a8\u09be\u09b0 \u09a5\u09c7\u0995\u09c7 \u09a6\u09c2\u09b0\u09c7 \u09a5\u09be\u0995\u09ac\u09c7 
p441
aV\u09ae\u09b9\u09bf\u09b2\u09be\u09a6\u09c7\u09b0 \u099c\u09a8\u09cd\u09af \u0995\u09cd\u09af\u09be\u09b2\u09b6\u09bf\u09df\u09be\u09ae \u09aa\u09cd\u09b0\u09df\u09cb\u099c\u09a8\u09c0\u09df , \u098f\u099f\u09be \u09b6\u09c1\u09a7\u09c1\u09ae\u09be\u09a4\u09cd\u09b0 \u09b9\u09be\u09a1\u09bc \u09ae\u099c\u09ac\u09c1\u09a4 \u0995\u09b0\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u09a8\u09df , \u09ac\u09b0\u0982 \u0997\u09cb\u099f\u09be \u09b6\u09b0\u09c0\u09b0\u0995\u09c7 \u09b8\u09c1\u09b8\u09cd\u09a5 \u09b0\u09be\u0996\u09be\u09b0 \u099c\u09a8\u09cd\u09af\u09c7\u0993 
p442
aV\u09ab\u09bf\u099c\u09bf\u0993\u09a5\u09c7\u09b0\u09be\u09aa\u09c0 \u09b8\u09cd\u099f\u09c7\u09a8\u09cd\u09a5\u09c7\u09b0 \u09b8\u09be\u09a5\u09c7 \u09b8\u09cd\u099f\u09cd\u09af\u09be\u09ae\u09bf\u09a8\u09be\u0993 \u09a6\u09c7\u09df 
p443
aV\u0986\u099c\u0995\u09be\u09b2 \u09ac\u09be\u099a\u09cd\u099a\u09be\u09b0\u09be \u099a\u09ac\u09cd\u09ac\u09bf\u09b6 \u0998\u09a8\u09cd\u099f\u09be \u099f\u09bf\u09ad\u09bf\u09b0 \u09b8\u09be\u09ae\u09a8\u09c7 \u09ac\u09b8\u09c7 \u09a5\u09be\u0995\u09c7 \u09af\u09be\u09b0 \u09ab\u09b2\u09c7 \u0985\u09a8\u09c7\u0995 \u09a7\u09b0\u09a8\u09c7\u09b0 \u09b8\u09ae\u09b8\u09cd\u09af\u09be \u09b9\u09df 
p444
aV\u0986\u09ae\u09b0\u09be \u09ac\u09bf\u09b6\u09cd\u09ac\u09be\u09b8 \u0995\u09b0\u09bf \u09af\u09c7 \u0995\u09be\u09b0\u09c1\u09b0 \u0995\u09be\u09b0\u09c1\u09b0 \u09a8\u09bf\u099c\u09b8\u09cd\u09ac \u0995\u09bf\u099b\u09c1 \u09ac\u09cd\u09af\u09be\u09aa\u09be\u09b0 \u09a5\u09be\u0995\u09c7 \u09a4\u09be\u0987 \u09aa\u09cd\u09b0\u09a5\u09ae\u09c7 \u0986\u09ae\u09b0\u09be \u09b0\u09cb\u0997\u09c0\u0995\u09c7 \u09ab\u09bf\u099c\u09bf\u0995\u09cd\u09af\u09be\u09b2\u09bf \u09aa\u09b0\u09c0\u0995\u09cd\u09b7\u09be \u0995\u09b0\u09bf \u0993 \u09a4\u09be\u09b0\u09aa\u09b0\u09c7 \u09b8\u09c7 \u0985\u09a8\u09c1\u09af\u09be\u09df\u09c0 \u098f\u0995\u09cd\u09b8\u09b0\u09b8\u09be\u0987\u099c \u098f\u09ac\u0982 \u099a\u09bf\u0995\u09bf\u09a4\u09cd\u09b8\u09be \u0995\u09b0\u09bf 
p445
aV\u09af\u09cb\u0997\u09be\u09b0 \u09a6\u09cd\u09ac\u09be\u09b0\u09be \u0986\u09aa\u09a8\u09be\u09b0 \u09b6\u09b0\u09c0\u09b0 \u09ac\u09b2 \u09a4\u09cb \u09aa\u09be\u09ac\u09c7 \u0995\u09bf\u09a8\u09cd\u09a4\u09c1 \u09a4\u09be \u09a7\u09b0\u09c7 \u09b0\u09be\u0996\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u09ab\u09bf\u099c\u09bf\u0993\u09a5\u09c7\u09b0\u09be\u09aa\u09c0 \u0985\u09a4\u09cd\u09af\u09a8\u09cd\u09a4 \u099c\u09b0\u09c1\u09b0\u09c0 
p446
aV\u098f\u0987 \u09aa\u09cd\u09b0\u09b6\u09cd\u09a8\u09c7\u09b0 \u099c\u09ac\u09be\u09ac\u09c7 \u09a1\u0983 \u0995\u09be\u099e\u09cd\u099a\u09a8- \u09ac\u09b2\u09c7\u09a8 \u09af\u09c7 \u09ab\u09bf\u099c\u09bf\u0993\u09a5\u09c7\u09b0\u09be\u09aa\u09c0\u09b0 \u09aa\u09cd\u09b0\u09a7\u09be\u09a8 \u0989\u09a6\u09cd\u09a6\u09c7\u09b6\u09cd\u09af \u09b9\u09b2 \u09ae\u09be\u0982\u09b8\u09aa\u09c7\u09b6\u09c0\u0997\u09c1\u09b2\u09bf\u09b0 \u09b6\u0995\u09cd\u09a4\u09bf \u09ac\u09be\u09a1\u09bc\u09bf\u09df\u09c7 \u09b9\u09be\u09a1\u09bc\u0995\u09c7 \u09b8\u09b9\u09be\u09df\u09a4\u09be \u0995\u09b0\u09be 
p447
aV\u09af\u09cb\u0997\u09be \u09af\u09be\u09b0\u09be \u0995\u09b0\u09be\u09a8 \u09a4\u09be\u09b0\u09be \u098f\u0995\u099f\u09be \u0995\u09a5\u09be\u09b0 \u0989\u09aa\u09b0\u0987 \u099c\u09cb\u09b0 \u09a6\u09c7\u09a8 \u09af\u09c7 \u098f\u099f\u09be \u09b8\u09ac \u09ac\u09df\u09b8\u09c0 \u09b2\u09cb\u0995\u09c7\u09b0\u09be\u0987 \u0995\u09b0\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7 \u0995\u09bf\u09a8\u09cd\u09a4\u09c1 \u09ab\u09bf\u099c\u09bf\u0993\u09a5\u09c7\u09b0\u09be\u09aa\u09c0\u09a4\u09c7 \u098f\u0987 \u09b0\u0995\u09ae \u0995\u09bf\u099b\u09c1 \u09a8\u09c7\u0987 
p448
aV\u0985\u09cd\u09af\u09be\u0995\u099f\u09bf\u09ad \u09ae\u09c1\u09ad\u09ae\u09c7\u09a8\u09cd\u099f \u09b8\u09c7\u0987 \u09b8\u09ac \u09b0\u09cb\u0997\u09c0\u0995\u09c7 \u09a6\u09c7\u0993\u09df\u09be \u09b9\u09df \u09af\u09be\u09b0\u09be \u09a8\u09bf\u099c\u09c7 \u09a5\u09c7\u0995\u09c7 \u098f\u0995\u09cd\u09b8\u09b0\u09b8\u09be\u0987\u099c \u0995\u09b0\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7 \u09a8\u09be 
p449
aV\u09ab\u09bf\u099c\u09bf\u0993\u09a5\u09c7\u09b0\u09be\u09aa\u09c0 \u0986\u09b0 \u09af\u09cb\u0997\u09be\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 \u0995\u09bf \u09a4\u09ab\u09be\u09a4\u09cd ?
p450
aV\u09aa\u09cd\u09af\u09be\u09b8\u09bf\u09ad \u09ae\u09c1\u09ad\u09ae\u09c7\u09a8\u09cd\u099f\u09c7\u09b0 \u09b8\u09be\u09b9\u09be\u09af\u09cd\u09af\u09c7 \u09b0\u09cb\u0997\u09c0\u0995\u09c7 \u09b8\u09c7\u0987 \u09b8\u09ae\u09df \u098f\u0995\u099f\u09c1 \u09b9\u09bf\u099f \u09a6\u09bf\u09df\u09c7 \u098f\u0995\u09cd\u09b8\u09b0\u09b8\u09be\u0987\u099c \u0995\u09b0\u09be\u09a8\u09cb \u09b9\u09df 
p451
aV\u0995\u09a8\u09cd\u099f\u09bf\u09a8\u09bf\u0989\u09df\u09be\u09ae \u09aa\u09cd\u09af\u09be\u09b8\u09bf\u09ad \u09ae\u09c1\u09ad\u09ae\u09c7\u09a8\u09cd\u099f\u09c7 \u09b0\u09cb\u0997\u09c0\u0995\u09c7 \u0985\u09a8\u09ac\u09b0\u09a4 \u09a8\u09be\u09a8\u09be \u09a7\u09b0\u09a8\u09c7\u09b0 \u099a\u09bf\u0995\u09bf\u09a4\u09cd\u09b8\u09be \u0995\u09b0\u09be\u09a8\u09cb \u09b9\u09df 
p452
aV\u0995\u09cb\u09b7\u09c7 \u0986\u09df\u09b0\u09a8 \u09ac\u09c7\u09b6\u09bf \u09a5\u09be\u0995\u09be\u09b0 \u09ab\u09b2\u09c7 \u09a4\u09c8\u09b0\u09bf \u09b9\u0993\u09df\u09be \u09ab\u09cd\u09b0\u09bf \u09b0\u09c7\u09a1\u09bf\u0995\u09cd\u09af\u09be\u09b2\u09c7\u09b0 \u09aa\u09cd\u09b0\u09ad\u09be\u09ac\u09c7 \u09a4\u09be \u0995\u09cd\u09b7\u09a4\u09bf\u0997\u09cd\u09b0\u09b8\u09cd\u09a5 \u09b9\u09df 
p453
aV\u09af\u09a6\u09bf \u09a5\u09cd\u09af\u09be\u09b2\u09be\u09b8\u09c7\u09ae\u09bf\u09df\u09be- , \u09b8\u09bf\u0995\u09bf\u09b2\u09b8\u09c7\u09b2- \u0985\u09cd\u09af\u09be\u09a8\u09bf\u09ae\u09bf\u09df\u09be- \u0987\u09a4\u09cd\u09af\u09be\u09a6\u09bf\u09b0 \u09ab\u09b2\u09c7 \u09a8\u09bf\u09df\u09ae\u09bf\u09a4 \u0985\u09a8\u09cd\u09a4\u09b0\u09be\u09b2\u09c7 \u09b0\u0995\u09cd\u09a4 \u09a6\u09bf\u09a4\u09c7 \u09b9\u09df , \u09a4\u0996\u09a8 \u09b6\u09b0\u09c0\u09b0\u09c7 \u09ac\u09c7\u09b6\u09bf \u09aa\u09b0\u09bf\u09ae\u09be\u09a3\u09c7 \u0986\u09df\u09b0\u09a8 \u099c\u09ae\u09be \u09b9\u09a4\u09c7 \u09a5\u09be\u0995\u09c7 
p454
aV\u0997\u09cd\u09b0\u09c7\u099f- \u09b9\u09bf\u09ae\u09be\u09b2\u09af\u09bc\u09be\u09a8- \u09b0\u09be\u09b7\u09cd\u099f\u09cd\u09b0\u09c0\u09af\u09bc- \u0989\u09a6\u09cd\u09af\u09be\u09a8- \u0995\u09c1\u09b2\u09cd\u09b2\u09c1- \u099c\u09c7\u09b2\u09be\u09af\u09bc \u09e7\u09ef\u09eb\u09ea \u09b8\u09be\u09b2\u09c7 \u09ed\u09ec\u09eb \u09ac\u09b0\u09cd\u0997 \u0995\u09bf\u09b2\u09cb\u09ae\u09bf\u099f\u09be\u09b0 \u099c\u09ae\u09bf\u09b0 \u0989\u09aa\u09b0 \u09b8\u09cd\u09a5\u09be\u09aa\u09a8 \u0995\u09b0\u09be \u09b9\u09af\u09bc 
p455
aV\u098f\u0987 \u0989\u09a6\u09cd\u09af\u09be\u09a8\u09c7\u09b0 \u099c\u09a8\u09cd\u09af \u09aa\u09cd\u09b0\u09a7\u09be\u09a8 \u09b0\u09c7\u09b2\u09b8\u09cd\u099f\u09c7\u09b6\u09a8 \u099c\u09cb\u0997\u09c7\u09a8\u09cd\u09a6\u09b0- \u09a8\u0997\u09b0 \u0986\u09b0 \u09ac\u09bf\u09ae\u09be\u09a8\u09ac\u09a8\u09cd\u09a6\u09b0 \u0995\u09b2\u09cd\u09b2\u09c1\u09a4\u09c7- \u0985\u09ac\u09b8\u09cd\u09a5\u09bf\u09a4 \u09b0\u09af\u09bc\u09c7\u099b\u09c7 
p456
aV\u099c\u09cb\u0997\u09c7\u09a8\u09cd\u09a6\u09b0- \u09a8\u0997\u09b0 , \u0997\u09cd\u09b0\u09c7\u099f- \u09b9\u09bf\u09ae\u09be\u09b2\u09af\u09bc\u09be\u09a8- \u09b0\u09be\u09b7\u09cd\u099f\u09cd\u09b0\u09c0\u09af\u09bc- \u0989\u09a6\u09cd\u09af\u09be\u09a8- \u09a5\u09c7\u0995\u09c7 \u09e7\u09e6\u09e6 \u0995\u09bf\u09b2\u09cb\u09ae\u09bf\u099f\u09be\u09b0 \u09ae\u09a4\u09cb \u0986\u09b0 \u0995\u09b2\u09cd\u09b2\u09c1- \u09ac\u09bf\u09ae\u09be\u09a8 \u09ac\u09a8\u09cd\u09a6\u09b0 \u09a5\u09c7\u0995\u09c7 \u09eb\u09e6 \u0995\u09bf\u09b2\u09cb\u09ae\u09bf\u099f\u09be\u09b0 \u09a6\u09c2\u09b0\u09c7 \u0985\u09ac\u09b8\u09cd\u09a5\u09bf\u09a4 \u09b0\u09af\u09bc\u09c7\u099b\u09c7 
p457
aV\u0997\u09cd\u09b0\u09c7\u099f- \u09b9\u09bf\u09ae\u09be\u09b2\u09af\u09bc\u09be\u09a8- \u09b0\u09be\u09b7\u09cd\u099f\u09cd\u09b0\u09c0\u09af\u09bc- \u0989\u09a6\u09cd\u09af\u09be\u09a8\u09c7\u09b0 \u099c\u09b2\u09ac\u09be\u09af\u09bc\u09c1 \u09b9\u09b2 \u09b6\u09c0\u09a4\u09cb\u09b7\u09cd\u09a3 \u09aa\u09be\u09b0\u09cd\u09ac\u09a4\u09c0\u09af\u09bc 
p458
aV\u0997\u09cd\u09b0\u09c7\u099f- \u09b9\u09bf\u09ae\u09be\u09b2\u09af\u09bc\u09be\u09a8- \u09b0\u09be\u09b7\u09cd\u099f\u09cd\u09b0\u09c0\u09af\u09bc- \u0989\u09a6\u09cd\u09af\u09be\u09a8\u09c7\u09b0- \u09b8\u09ae\u09c1\u09a6\u09cd\u09b0\u09a4\u09b2 \u09a5\u09c7\u0995\u09c7 \u09b8\u09be\u09a7\u09be\u09b0\u09a3 \u0989\u099a\u09cd\u099a\u09a4\u09be \u09b9\u09b2 \u09e8\u09eb\u09e6\u09e6 \u09ae\u09bf\u099f\u09be\u09b0 
p459
aV\u0997\u09cd\u09b0\u09c7\u099f- \u09b9\u09bf\u09ae\u09be\u09b2\u09af\u09bc\u09be\u09a8- \u09b0\u09be\u09b7\u09cd\u099f\u09cd\u09b0\u09c0\u09af\u09bc- \u0989\u09a6\u09cd\u09af\u09be\u09a8\u09c7- \u09aa\u09be\u0987\u09a8 \u0993 \u09a6\u09c7\u09ac\u09a6\u09be\u09b0\u09c1 \u0997\u09be\u099b \u09aa\u09be\u0993\u09af\u09bc\u09be \u09af\u09be\u09af\u09bc 
p460
aV\u098f\u0996\u09be\u09a8\u0995\u09be\u09b0 \u09ac\u09a8\u09cd\u09af \u09aa\u09cd\u09b0\u09be\u09a3\u09c0\u09a6\u09c7\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 \u099a\u09bf\u09a4\u09be , \u09ad\u09c7\u09a1\u09bc\u09be , \u09b9\u09bf\u09ae\u09be\u09b2\u09af\u09bc\u09be\u09a8 \u09a5\u09be\u09b0 , \u09b8\u09c7\u09b0\u09be\u09ac , \u0995\u09b8\u09cd\u09a4\u09c1\u09b0\u09c0 \u09ae\u09c3\u0997 , \u09ad\u09b0\u09b2 \u0986\u09b0 \u0995\u09be\u09a0\u09ac\u09c7\u09a1\u09bc\u09be\u09b2\u09c0 \u0986\u099b\u09c7 
p461
aV\u0997\u09cd\u09b0\u09c7\u099f- \u09b9\u09bf\u09ae\u09be\u09b2\u09af\u09bc\u09be\u09a8- \u09b0\u09be\u09b7\u09cd\u099f\u09cd\u09b0\u09c0\u09af\u09bc- \u0989\u09a6\u09cd\u09af\u09be\u09a8\u09c7- \u09aa\u09be\u0996\u09bf\u09a6\u09c7\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 \u09ab\u09c0\u099c\u09c7\u09a8\u09cd\u099f - \u099f\u09cd\u09b0\u09c7\u0997\u09cb\u09aa\u09a8 , \u09ae\u09cb\u09a8\u09be\u09b2 , \u099a\u09c0\u09b2 \u0986\u09b0 \u09ac\u09be\u099c \u0987\u09a4\u09cd\u09af\u09be\u09a6\u09bf \u09aa\u09a1\u09bc\u09c7 
p462
aV\u09aa\u09bf\u09a8- \u0998\u09be\u099f\u09bf \u0997\u09cd\u09b0\u09c7\u099f- \u09b0\u09be\u09b7\u09cd\u099f\u09cd\u09b0\u09c0\u09af\u09bc- \u0989\u09a6\u09cd\u09af\u09be\u09a8\u09c7 \u09b9\u09bf\u09ae\u09be\u099a\u09b2- \u09aa\u09cd\u09b0\u09a6\u09c7\u09b6\u09c7\u09b0- \u09b2\u09be\u09b9\u09cb\u09b2- \u0986\u09b0 \u09b8\u09cd\u09aa\u09c0\u09a4\u09bf\u099c\u09cb\u09b2\u09cb\u09b0- \u09aa\u09bf\u09a8- \u0998\u09be\u099f\u09bf\u09a4\u09c7- \u09ec\u09ed\u09eb \u09ac\u09b0\u09cd\u0997 \u0995\u09bf\u09b2\u09cb\u09ae\u09bf\u099f\u09be\u09b0 \u0995\u09cd\u09b7\u09c7\u09a4\u09cd\u09b0\u09c7\u09b0 \u0989\u09aa\u09b0 \u09b8\u09cd\u09a5\u09be\u09aa\u09a8 \u0995\u09b0\u09be \u09b9\u09af\u09bc\u09c7\u099b\u09c7 
p463
aV\u09aa\u09bf\u09a8- \u0998\u09be\u099f\u09bf- \u09b0\u09be\u09b7\u09cd\u099f\u09cd\u09b0\u09c0\u09af\u09bc- \u0989\u09a6\u09cd\u09af\u09be\u09a8- \u09a5\u09c7\u0995\u09c7 \u09a8\u09bf\u0995\u099f\u09a4\u09ae \u09aa\u09cd\u09b0\u09a7\u09be\u09a8 \u09b0\u09c7\u09b2\u09b8\u09cd\u099f\u09c7\u09b6\u09a8 \u098f\u09ac\u0982 \u09ac\u09bf\u09ae\u09be\u09a8\u09ac\u09a8\u09cd\u09a6\u09b0 \u09b9\u09b2 \u09b6\u09bf\u09ae\u09b2\u09be\u09af\u09bc \u09af\u09c7\u099f\u09bf \u0989\u09a6\u09cd\u09af\u09be\u09a8 \u09a5\u09c7\u0995\u09c7 \u09e9\u09ed\u09eb \u0995\u09bf\u09b2\u09cb\u09ae\u09bf\u099f\u09be\u09b0 \u09a6\u09c2\u09b0\u09c7 \u0985\u09ac\u09b8\u09cd\u09a5\u09bf\u09a4 \u09b0\u09af\u09bc\u09c7\u099b\u09c7 
p464
aV\u09b0\u0995\u09cd\u09a4\u09c7\u09b0 \u099a\u09be\u09aa \u09ac\u09c7\u09a1\u09bc\u09c7 \u09af\u09be\u0993\u09df\u09be\u09b0 \u09ab\u09b2\u09c7 \u098f\u099f\u09be \u09ab\u09c7\u099f\u09c7 \u09af\u09be\u09df \u09af\u09be\u09b0 \u09ab\u09b2\u09c7 \u09b0\u0995\u09cd\u09a4\u09b8\u09cd\u09b0\u09be\u09ac \u09b9\u09df 
p465
aV\u09ae\u09b8\u09cd\u09a4\u09bf\u09b7\u09cd\u0995\u09c7 \u09b0\u0995\u09cd\u09a4\u09b8\u09cd\u09b0\u09be\u09ac \u09b9\u0993\u09df\u09be\u09b0 \u09ab\u09b2\u09c7 \u09b0\u09cb\u0997\u09c0 \u09a8\u09df \u09b9\u09a0\u09be\u09a4 \u0985\u099c\u09cd\u099e\u09be\u09a8 \u09b9\u09df\u09c7 \u09aa\u09a1\u09bc\u09c7 \u09af\u09be\u09df \u0986\u09b0 \u09a8\u09df\u09a4\u09cb \u0986\u0997\u09c7 \u09ae\u09be\u09a5\u09be\u09df \u09a4\u09c0\u09ac\u09cd\u09b0 \u09ac\u09cd\u09af\u09be\u09a5\u09be \u09b9\u09df \u098f\u09ac\u0982 \u0986\u09b8\u09cd\u09a4\u09c7 \u0986\u09b8\u09cd\u09a4\u09c7 \u09b8\u09c7 \u0985\u099c\u09cd\u099e\u09be\u09a8 \u09b9\u09df\u09c7 \u09af\u09be\u09df 
p466
aV\u09aa\u09cd\u09b0\u09cb\u09b8\u09cd\u099f\u09bf\u0997\u09ae\u09bf\u09a8 ( prostigmin ) \u098f\u09b0 \u09a4\u09bf\u09a8\u099f\u09bf \u0995\u09b0\u09c7 \u099f\u09cd\u09af\u09be\u09ac\u09b2\u09c7\u099f \u09ac\u09be \u098f\u09b0 \u0987\u099e\u09cd\u099c\u09c7\u0995\u09b6\u09be\u09a8 ( \u09b8\u09aa\u09cd\u09a4\u09be\u09b9\u09c7 \u098f\u0995 \u09ac\u09be\u09b0 ) \u09a6\u09bf\u09b2\u09c7\u0993 \u09b2\u09be\u09ad \u09b9\u09df 
p467
aV\u09b8\u09bf\u0995\u09cd\u09ad\u09bf\u09b2 \u0993 \u09b8\u09cd\u099f\u09c7\u09ae\u09c7\u099f\u09bf\u09b2\u09c7\u09b0 \u09ac\u09a1\u09bc\u09bf\u0993 \u0995\u09be\u09b0\u09cd\u09af\u0995\u09b0\u09c0 \u09b9\u09df 
p468
aV\u09ae\u09be\u09b0\u09cd\u099c\u09bf\u09a8\u09c7\u09b0 ( margine ) \u098f\u0995 \u09a5\u09c7\u0995\u09c7 \u09a4\u09bf\u09a8\u099f\u09c7 \u09ac\u09a1\u09bc\u09bf \u09a6\u09bf\u09a8 
p469
aV\u09ae\u09b8\u09cd\u09a4\u09bf\u09b7\u09cd\u0995\u09c7 \u09b0\u0995\u09cd\u09a4\u09bf\u09b8\u09cd\u09b0\u09be\u09ac ( erebral haemorrhage ) - \u098f\u0987 \u09b0\u09cb\u0997 \u09ac\u09c7\u09b6\u09bf\u09b0\u09ad\u09be\u0997 \u09ea\u09e6 \u09ac\u099b\u09b0\u09c7\u09b0 \u0985\u09a7\u09bf\u0995 \u0986\u09df\u09c1\u09b0 \u09b2\u09cb\u0995\u09c7\u09a6\u09c7\u09b0 \u09b9\u09df 
p470
aV\u098f\u0987 \u09b0\u09cb\u0997 \u09b8\u09cd\u09a4\u09cd\u09b0\u09c0\u09a6\u09c7\u09b0 \u09a5\u09c7\u0995\u09c7 \u09aa\u09c1\u09b0\u09c1\u09b7\u09a6\u09c7\u09b0 \u09ac\u09c7\u09b6\u09bf \u09b9\u09df 
p471
aV\u09ac\u09c7\u09b6\u09bf\u09b0\u09ad\u09be\u0997 \u09b8\u09ae\u09df \u098f\u0987 \u0993\u09b7\u09c1\u09a7 \u09b8\u09c7\u0987 \u09ac\u09cd\u09af\u09be\u0995\u09cd\u09a4\u09bf\u09a6\u09c7\u09b0 \u09b9\u09df \u09af\u09be\u09a6\u09c7\u09b0 \u09b9\u09c3\u09a6\u09b0\u09cb\u0997 \u09ac\u09be \u09ac\u09c3\u0995\u09cd\u0995\u09c7\u09b0 \u09b0\u09cb\u0997 \u0986\u099b\u09c7 \u09ac\u09be \u09af\u09be\u09a6\u09c7\u09b0 \u09a7\u09ae\u09a8\u09c0 \u0997\u09c7\u0981\u099f\u09c7\u09ac\u09be\u09a4 , \u09ae\u09a6\u09cd\u09af\u09aa\u09be\u09a8 \u09ac\u09be \u0989\u09aa\u09a6\u0982\u09b6\u09c7\u09b0 \u0995\u09be\u09b0\u09a3\u09c7 \u09b6\u0995\u09cd\u09a4 \u09b9\u09df\u09c7 \u0997\u09c7\u099b\u09c7 
p472
aV\u09af\u09be\u09a6\u09c7\u09b0 \u09ae\u09b8\u09cd\u09a4\u09bf\u09b7\u09cd\u0995\u09c7\u09b0 \u09a7\u09ae\u09a8\u09c0\u09a4\u09c7 \u0985\u09cd\u09af\u09be\u09a8\u09bf\u0989\u09b0\u09bf\u099c\u09ae ( neurism ) , \u0985\u09cd\u09af\u09be\u09ae\u09cd\u09ac\u09cb\u09b2\u09bf\u099c\u09ae ( mbolism ) \u09ac\u09be \u0995\u09cb\u09a8\u0993 \u099f\u09bf\u0989\u09ae\u09be\u09b0 \u09b9\u09df 
p473
aV\u0985\u09cd\u09af\u09be\u09a8\u09bf\u0989\u09b0\u09bf\u099c\u09ae \u0993 \u0985\u09cd\u09af\u09be\u09ae\u09cd\u09ac\u09cb\u09b2\u09bf\u099c\u09ae\u09c7\u09b0 \u0995\u09be\u099b\u09c7 \u09a7\u09ae\u09a8\u09c0 \u09a8\u09b0\u09ae \u0993 \u09a6\u09c1\u09b0\u09cd\u09ac\u09b2 \u09b9\u09df\u09c7 \u09af\u09be\u09df 
p474
aV\u098f\u0987 \u099b\u09cb\u099f\u09cb \u09a6\u09c7\u09b6\u09c7 \u098f\u09a4 \u09b8\u09cc\u09a8\u09cd\u09a6\u09b0\u09cd\u09af \u09a7\u09b0\u09c7 \u09a8\u09be \u09ae\u09a8\u09c7 \u09b9\u09af\u09bc 
p475
aV\u09ac\u09be\u099c\u09be\u09b0\u09c7\u09b0 \u09b8\u09c7\u0987 \u09ad\u09cd\u09b0\u09ae\u09a3 \u09a5\u09c7\u0995\u09c7 \u0986\u09ac\u09be\u09b0 \u0995\u09cd\u09b0\u09c1\u099c\u09c7 \u09ab\u09c7\u09b0\u09a4 \u09af\u09be\u0993\u09af\u09bc\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u099c\u09c7\u099f\u09bf\u09b0 \u09a6\u09bf\u0995\u09c7 \u09af\u09be\u0993\u09af\u09bc\u09be\u09b0 \u09b8\u09ae\u09af\u09bc \u09af\u0996\u09a8 \u09a6\u09c3\u09b7\u09cd\u099f\u09bf \u09ac\u09be\u09b0\u09ac\u09be\u09b0 \u09b0\u09be\u09b8\u09cd\u09a4\u09be \u09a7\u09b0\u09c7 \u099a\u09b2\u09be \u09b8\u09ae\u09c1\u09a6\u09cd\u09b0\u09c7\u09b0 \u099c\u09b2\u09c7\u09b0 \u09a6\u09bf\u0995\u09c7 \u09af\u09be\u0993\u09af\u09bc\u09be\u09a4\u09c7 \u09ac\u09c1\u099d\u09a4\u09c7 \u09aa\u09be\u09b0\u09b2\u09be\u09ae \u09af\u09c7 \u098f\u0996\u09be\u09a8\u0995\u09be\u09b0 \u09b2\u09cb\u0995 \u098f\u0987 \u09b8\u09cc\u09a8\u09cd\u09a6\u09b0\u09cd\u09af\u09c7\u09b0 \u09ae\u09c2\u09b2\u09cd\u09af \u0995\u09a4\u0996\u09be\u09a8\u09bf \u09ac\u09cb\u099d\u09c7 
p476
aV\u09b8\u09a4\u09ad\u09be\u09ac\u09c7 \u09ac\u09b2\u09b2\u09c7 \u0986\u09ae\u09bf \u0995\u09cb\u09a8\u0993 \u09b9\u09bf\u09a8\u09cd\u09a6\u09c1\u09b8\u09cd\u09a4\u09be\u09a8\u09bf\u0995\u09c7 \u09b0\u09be\u09b7\u09cd\u099f\u09cd\u09b0\u09aa\u09a4\u09bf \u09ac\u09be \u09aa\u09cd\u09b0\u09a7\u09be\u09a8\u09ae\u09a8\u09cd\u09a4\u09cd\u09b0\u09c0\u09b0 \u09ad\u09be\u09b7\u09a3 \u098f\u09a4 \u09ae\u09a8 \u09a6\u09bf\u09af\u09bc\u09c7 \u09b6\u09c1\u09a8\u09a4\u09c7 \u09a6\u09c7\u0996\u09bf\u09a8\u09bf 
p477
aV\u0995\u09bf\u09a8\u09cd\u09a4\u09c1 \u09af\u0996\u09a8 \u098f\u0987 \u09ae\u09be\u099f\u09bf\u09a4\u09c7 \u098f\u0995\u099f\u09be \u09a8\u09a4\u09c1\u09a8 \u0985\u09b8\u09cd\u09a4\u09bf\u09a4\u09cd\u09ac \u09b0\u099a\u09a8\u09be\u09b0 \u09aa\u09cd\u09b0\u09b6\u09cd\u09a8 \u0993\u09a0\u09c7 \u09a4\u0996\u09a8 \u098f\u09a4\u099f\u09be \u0997\u09ae\u09cd\u09ad\u09c0\u09b0\u09a4\u09be \u09a5\u09be\u0995\u09be\u099f\u09be \u09a6\u09b0\u0995\u09be\u09b0 
p478
aV\u0985\u09a8\u09c7\u0995 \u09b2\u09cb\u0995 \u0985\u0995\u09cd\u099f\u09cb\u09ac\u09b0\u09c7 \u09ae\u09b2\u09a6\u09c0\u09ad\u09c7\u09b0 \u0995\u09cd\u09af\u09be\u09ac\u09bf\u09a8\u09c7\u099f \u09a6\u09cd\u09ac\u09be\u09b0\u09be \u099c\u09b2\u09c7\u09b0 \u09a4\u09b2\u09be\u09af\u09bc \u0995\u09b0\u09be \u09ac\u09c8\u09a0\u0995 \u099f\u09cb\u099f\u0995\u09be \u09ad\u09be\u09ac\u09b2\u09c7\u0993 \u09ae\u09b2\u09a6\u09c0\u09ad \u0997\u09bf\u09af\u09bc\u09c7 \u09ae\u09a8\u09c7 \u09b9\u09ac\u09c7 \u09af\u09a6\u09bf \u09aa\u09cd\u09b0\u0995\u09c3\u09a4\u09bf\u09b0 \u0995\u09cb\u09aa \u09af\u09a6\u09bf \u0986\u09ae\u09be\u09a6\u09c7\u09b0 \u09a5\u09c7\u0995\u09c7 \u098f\u099f\u09be \u099b\u09bf\u09a8\u09bf\u09af\u09bc\u09c7 \u09a8\u09c7\u09af\u09bc \u09a4\u09be\u09b9\u09b2\u09c7 \u09aa\u09c3\u09a5\u09bf\u09ac\u09c0 \u0995\u09a4 \u09a6\u09b0\u09bf\u09a6\u09cd\u09b0 \u09b9\u09af\u09bc\u09c7 \u09af\u09be\u09ac\u09c7 
p479
aV\u0993\u0996\u09be\u09a8\u0995\u09be\u09b0 \u09b8\u09cc\u09a8\u09cd\u09a6\u09b0\u09cd\u09af\u09c7 \u098f\u0995\u099f\u09be \u0986\u09b2\u09be\u09a6\u09be \u09ac\u09bf\u09b6\u09c7\u09b7 \u09ac\u09cd\u09af\u09be\u09aa\u09be\u09b0 \u0986\u099b\u09c7 
p480
aV\u0986\u09b0 \u09b8\u09c7\u099f\u09be \u098f\u09ae\u09a8 \u098f\u0995\u099f\u09bf \u09b8\u09ab\u09b0 \u09af\u09be \u0986\u09aa\u09a8\u09bf \u0995\u0996\u09a8\u0993\u0987 \u099a\u09be\u0987\u09ac\u09c7\u09a8 \u09a8\u09be \u09af\u09c7 \u09b6\u09c7\u09b7 \u09b9\u09af\u09bc\u09c7 \u09af\u09be\u0995 
p481
aV\u09b8\u09bf \u09aa\u09cd\u09b2\u09c7\u09a8 \u09e7\u09e8 \u0995\u09bf\u09b2\u09cb\u09ae\u09bf\u099f\u09be\u09b0 \u09a5\u09c7\u0995\u09c7 \u09b8\u09c1\u09b0\u09c1 \u0995\u09b0\u09c7 \u09e7\u09ee\u09e9 \u0995\u09bf\u09b2\u09cb\u09ae\u09bf\u099f\u09be\u09b0 ( \u0989\u09a4\u09cd\u09a4\u09ae\u09c7 \u09ae\u09be\u09ad\u09bf\u09b2\u09c7\u09ad\u09be\u09b0\u09c1 ) \u0985\u09ac\u09a7\u09bf \u09af\u09be\u09a4\u09cd\u09b0\u09be \u09ae\u09b2\u09a6\u09c0\u09ad\u09c7\u09b0 \u09a6\u09cd\u09ac\u09c0\u09aa\u09c7 \u0995\u09b0\u09be \u09b9\u09af\u09bc 
p482
aV\u0986\u09ae\u09b0\u09be \u09ae\u09be\u09b2\u09c7\u09b0 \u0986\u09b9\u09ae\u09c7\u09a6\u09bf \u09ac\u09be\u099c\u09be\u09b0\u09c7 \u09b0\u09be\u099c\u09a7\u09be\u09a8\u09c0\u09b0 \u09b8\u09ac\u09a5\u09c7\u0995\u09c7 \u09ac\u09a1\u09bc\u09cb \u0985\u09cd\u09af\u09be\u09a8\u09cd\u099f\u09bf\u0995 \u0993 \u09b8\u09c1\u09ad\u09c7\u09a8\u09bf\u09b0\u09c7\u09b0 \u09a6\u09cb\u0995\u09be\u09a8\u09c7 \u099b\u09bf\u09b2\u09be\u09ae 
p483
aV\u09b8\u09ac \u09b8\u09c7\u09b2\u09b8\u09ae\u09cd\u09af\u09be\u09a8 \u09ae\u09b2\u09a6\u09c0\u09ad\u09c7\u09b0 \u09b0\u09be\u09b7\u09cd\u099f\u09cd\u09b0\u09aa\u09a4\u09bf \u09ae\u09cb\u09b9\u09be\u09ae\u09cd\u09ae\u09a6 \u09a8\u09be\u09b6\u09c0\u09a6\u09c7\u09b0 \u09a6\u09cd\u09ac\u09be\u09b0\u09be \u098f\u0995 \u09a6\u09bf\u09a8 \u0986\u0997\u09c7\u0987 \u0995\u09cb\u09aa\u09c7\u09a8\u09b9\u09c7\u0997\u09c7\u09a8\u09c7 \u09ac\u09bf\u09b6\u09cd\u09ac \u099c\u09b2\u09ac\u09be\u09af\u09bc\u09c1 \u09b8\u09ae\u09cd\u09ae\u09c7\u09b2\u09a8\u09c7 \u09a6\u09c7\u0993\u09af\u09bc\u09be \u09ad\u09be\u09b7\u09a3 \u0996\u09c1\u09ac \u09ae\u09a8 \u09a6\u09bf\u09af\u09bc\u09c7 \u09b6\u09c1\u09a8\u099b\u09bf\u09b2\u09c7\u09a8 
p484
aV\u09b8\u09cd\u09a4\u09a8 \u0995\u09cd\u09af\u09be\u09a8\u09b8\u09be\u09b0\u09c7 \u09ac\u09c7\u09b6\u09bf\u09b0 \u09ad\u09be\u0997\u0987 \u09ae\u09be\u09a8\u09b8\u09bf\u0995 \u098f\u09ac\u0982 \u0986\u09b0\u09cd\u09a5\u09bf\u0995 \u09a6\u09bf\u0995 \u09a6\u09bf\u09df\u09c7 \u09ad\u09c7\u0999\u09c7 \u09aa\u09a1\u09bc\u09c7 
p485
aV\u09b6\u09b2\u09cd\u09af - \u099a\u09bf\u0995\u09bf\u09a4\u09cd\u09b8\u09be\u09b0 \u0997\u09c1\u09b0\u09c1\u09a4\u09cd\u09ac \u09ac\u09be\u09a1\u09bc\u09be\u09a4\u09c7 , \u09a6\u09c7\u09b0\u09bf\u09a4\u09c7 \u099c\u09c7\u09a8\u09c7 \u099c\u09c0\u09ac\u09a8 \u09ac\u09be\u0981\u099a\u09be\u09a8\u09cb\u09b0 \u09aa\u09b0\u0993 \u09b0\u09cb\u0997\u09c0 \u09b6\u09be\u09b0\u09c0\u09b0\u09bf\u0995 \u09ac\u09bf\u0995\u09c3\u09a4\u09bf\u09b0 \u09ad\u09df \u098f\u09ac\u0982 \u09aa\u09c1\u09a8\u09b0\u09be\u09df \u0986\u0995\u09cd\u09b0\u09be\u09a8\u09cd\u09a4 \u09b9\u0993\u09df\u09be\u09b0 \u09b8\u09ae\u09cd\u09ad\u09be\u09ac\u09a8\u09be\u09b0 \u0995\u09a5\u09be \u09ad\u09c7\u09ac\u09c7 \u09a4\u09cd\u09b0\u09b8\u09cd\u09a4 \u09b9\u09df\u09c7 \u09a5\u09be\u0995\u09c7 
p486
aV\u098f\u09b0\u09be \u0995\u09cb\u09a8 \u09ac\u09bf\u09b7\u09df\u09c7 \u0985\u09a7\u09bf\u0995 \u09b8\u0982\u0996\u09cd\u09af\u09be \u09aa\u09be\u09df ?
p487
aV\u09b8\u09be\u09b0\u09be \u09b8\u09cd\u0995\u09c1\u09b2 \u09a6\u09c7\u0996\u09b2\u09c7 \u099b\u09c7\u09b2\u09c7 \u0986\u09b0 \u09ae\u09c7\u09df\u09c7\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 , \u099b\u09cb\u099f \u09ac\u09be\u099a\u09cd\u099a\u09be \u0986\u09b0 \u09ac\u09c3\u09a6\u09cd\u09a7\u09c7\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 \u098f\u0987 \u09a6\u09c3\u09b7\u09cd\u099f\u09bf\u0995\u09cb\u09a3 \u09a5\u09c7\u0995\u09c7 \u0995\u09cb\u09a8\u09cb \u09a4\u09ab\u09be\u09a4 \u09aa\u09be\u0993\u09df\u09be \u09af\u09be\u09df \u0995\u09bf ?
p488
aV\u0995\u09cd\u09b0\u09bf\u0995\u09c7\u099f\u09c7 \u09a4\u09cb \u098f\u0987 \u09ac\u09be\u0981-\u09b9\u09be\u09a4\u09bf \u0996\u09c7\u09b2\u09cb\u09df\u09be\u09a1\u09bc\u09c7\u09b0 \u09b8\u09cd\u09a5\u09be\u09a8\u0987 \u0986\u09b2\u09be\u09a6\u09be \u09b9\u09df 
p489
aV\u0986\u09aa\u09a8\u09be\u09a6\u09c7\u09b0 \u09b8\u09cd\u0995\u09c1\u09b2 \u09ac\u09be \u0995\u0995\u09cd\u09b7\u09c7 \u098f\u09a6\u09c7\u09b0 \u0985\u09a8\u09c1\u09aa\u09be\u09a4\u099f\u09be \u0995\u09a4 ?
p490
aV\u09b8\u09cd\u09a4\u09a8 \u0995\u09cd\u09af\u09be\u09a8\u09b8\u09be\u09b0 \u09ae\u09b9\u09bf\u09b2\u09be\u09a6\u09c7\u09b0 \u099c\u09a8\u09cd\u09af \u098f\u0995\u099f\u09be \u09ad\u09df\u09be\u09ac\u09b9 \u09a8\u09be\u09ae 
p491
aV\u0986\u0997\u09c7\u0995\u09be\u09b0 \u09a6\u09bf\u09a8\u09c7 \u09b8\u09cd\u09a4\u09a8 \u0995\u09cd\u09af\u09be\u09a8\u09b8\u09be\u09b0 \u0985\u099c\u09be\u09a8\u09be \u09a5\u09be\u0995\u09be\u09b0 \u09ab\u09b2\u09c7 \u09b9\u09be\u099c\u09be\u09b0 \u09b9\u09be\u099c\u09be\u09b0 \u09ae\u09be\u09a8\u09c1\u09b7\u09c7\u09b0 \u09ae\u09c3\u09a4\u09cd\u09af\u09c1 \u09b9\u09a4 
p492
aV\u0995\u09bf\u09a8\u09cd\u09a4\u09c1 \u09aa\u09cd\u09b0\u09a4\u09bf\u099f\u09bf \u099c\u09bf\u09a8\u09bf\u09b8\u0995\u09c7 \u09ae\u09be\u09aa\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u0995\u09bf\u099b\u09c1 \u09a8\u09be \u0995\u09bf\u099b\u09c1 \u09ad\u09cc\u09a4\u09bf\u0995 \u09ae\u09be\u09aa\u09a6\u09a3\u09cd\u09a1 \u09a4\u09c8\u09b0\u09bf \u0995\u09b0\u09a4\u09c7\u0987 \u09b9\u09df 
p493
aV\u098f\u099f\u09be \u099b\u09be\u09a1\u09bc\u09be \u0995\u09cb\u09a8\u09cb \u09ac\u09c8\u099c\u09cd\u099e\u09be\u09a8\u09bf\u0995\u09c7\u09b0 \u0995\u09cb\u09a8\u09cb \u09b8\u09bf\u09a6\u09cd\u09a7\u09be\u09a8\u09cd\u09a4\u09c7 \u09aa\u09cc\u0981\u099b\u09be\u09a8\u09cb \u09b8\u09ae\u09cd\u09ad\u09ac \u09a8\u09df 
p494
aV\u09b8\u0995\u09b2 \u09aa\u09cd\u09b0\u099c\u09a8\u09a8 \u09b8\u0982\u0996\u09cd\u09af\u09be ( \u0995\u09cb\u09a8\u0993 \u09ae\u09b9\u09bf\u09b2\u09be \u09a6\u09cd\u09ac\u09be\u09b0\u09be \u09a8\u09bf\u099c\u09c7\u09b0 \u09aa\u09cd\u09b0\u099c\u09a8\u09a8 \u0995\u09be\u09b2\u09c7 \u099c\u09a8\u09cd\u09ae \u09b9\u0993\u09df\u09be \u09b6\u09bf\u09b6\u09c1\u09b0 \u09b8\u0982\u0996\u09cd\u09af\u09be ) \u09af\u09be \u09e7\u09ef\u09ed\u09e7 - \u09ed\u09ec \u09b8\u09be\u09b2\u09c7\u09b0 \u0985\u09ac\u09a7\u09bf \u09aa\u09be\u0981\u099a\u09c7\u09b0 \u09ac\u09c7\u09b6\u09bf \u0985\u09a8\u09c1\u09ae\u09be\u09a8 \u0995\u09b0\u09be \u09b9\u09a4 , \u09a4\u09be \u09a8\u09bf\u09b0\u09a8\u09cd\u09a4\u09b0 \u09ac\u09c7\u09a1\u09bc\u09c7 \u099a\u09b2\u09c7\u099b\u09c7 \u098f\u09ac\u0982 \u09e8\u09e6\u09e6\u09e7 \u09b8\u09be\u09b2\u09c7 \u098f\u099f\u09be \u09e9.\u09e9 \u09b9\u09df\u09c7\u099b\u09c7 
p495
aV\u09b0\u09be\u099c\u09be \u09ac\u09a6\u09a8\u09b8\u09bf\u0982\u09b9\u09c7\u09b0 \u09aa\u09b0\u09c7 \u0993\u09a8\u09be\u09b0 \u09aa\u09c1\u09a4\u09cd\u09b0 \u09b8\u09c1\u09b0\u099c\u09ae\u09be\u09b2 \u09a1\u09bf\u0997\u09c7\u09b0 \u09ac\u09bf\u09b8\u09cd\u09a4\u09be\u09b0 \u0995\u09b0\u09c7\u09a8 \u098f\u09ac\u0982 \u098f\u0996\u09be\u09a8\u09c7 \u0985\u09a8\u09c7\u0995 \u09b8\u09c1\u09a8\u09cd\u09a6\u09b0 \u0987\u09ae\u09be\u09b0\u09a4 \u09ac\u09be\u09a8\u09be\u09a8 
p496
aV\u09b0\u09be\u099c\u09b8\u09cd\u09a5\u09be\u09a8\u09c7\u09b0 \u098f\u0987 \u099b\u09cb\u099f\u09cb \u09b6\u09b9\u09b0 \u09ac\u09cd\u09b0\u099c\u09ad\u09c2\u09ae\u09bf\u09b0 \u0985\u0982\u09b6 \u098f\u09ac\u0982 \u0997\u09cb\u09ac\u09b0\u09cd\u09a7\u09a8 \u09aa\u09b0\u09bf\u0995\u09cd\u09b0\u09ae\u09be\u09b0 \u09aa\u09a5\u09c7 \u0985\u09ac\u09b8\u09cd\u09a5\u09bf\u09a4 , \u0995\u09bf\u09a8\u09cd\u09a4\u09c1 \u09aa\u09b0\u09cd\u09af\u099f\u0995\u09a6\u09c7\u09b0 \u0995\u09be\u099b\u09c7 \u098f\u0987 \u09b6\u09b9\u09b0\u09c7\u09b0 \u0986\u0995\u09b0\u09cd\u09b7\u09a3 \u09b9\u09b2 \u098f\u0996\u09be\u09a8\u0995\u09be\u09b0 \u0995\u09c7\u09b2\u09cd\u09b2\u09be , \u09ae\u09b9\u09b2 , \u0989\u09a6\u09cd\u09af\u09be\u09a8 \u098f\u09ac\u0982 \u09ab\u09cb\u09af\u09bc\u09be\u09b0\u09be 
p497
aV\u09af\u09a6\u09bf\u0993 \u09b8\u0982\u09b0\u0995\u09cd\u09b7\u09a3 \u09a8\u09be \u0995\u09b0\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u098f\u0987 \u0995\u09c7\u09b2\u09cd\u09b2\u09be\u099f\u09bf \u098f\u0996\u09a8 \u09ad\u09be\u09b2\u09cb \u0985\u09ac\u09b8\u09cd\u09a5\u09be\u09af\u09bc \u09a8\u09c7\u0987 , \u0995\u09bf\u09a8\u09cd\u09a4\u09c1 \u09a4\u09be\u0993 \u09aa\u09b0\u09cd\u09af\u099f\u0995\u09a6\u09c7\u09b0 \u09aa\u09cd\u09b0\u09ad\u09be\u09ac\u09bf\u09a4 \u0995\u09b0\u09be\u09b0 \u0995\u09cd\u09b7\u09ae\u09a4\u09be \u09a7\u09b0\u09c7 \u09b0\u09c7\u0996\u09c7\u099b\u09c7 
p498
aV\u09a1\u09bf\u0997\u09c7\u09b0 \u09aa\u09cd\u09b0\u09ae\u09c1\u0996 \u0987\u09ae\u09be\u09b0\u09a4 \u098f\u0995\u099f\u09bf \u099a\u09ae\u09a4\u0995\u09be\u09b0 \u0995\u09c7\u09b2\u09cd\u09b2\u09be \u09ac\u09be\u09a8\u09be\u09a8 
p499
aV\u09a6\u09cd\u09ac\u09be\u09b0\u09c7\u09b0 \u099a\u09c1\u09a1\u09bc\u09be\u0997\u09c1\u09b2\u09bf\u09b0 \u0989\u09aa\u09b0 \u0995\u09be\u09ae\u09be\u09a8 \u09b0\u09be\u0996\u09be \u09b9\u09a4 
p500
aV\u098f\u0995 \u0997\u09ad\u09c0\u09b0 \u0996\u09be\u09a6 \u09a6\u09bf\u09af\u09bc\u09c7 \u0998\u09c7\u09b0\u09be \u09a1\u09bf\u0997 \u0995\u09c7\u09b2\u09cd\u09b2\u09be\u09b0 \u099a\u09be\u09b0\u09a6\u09bf\u0995\u09c7 \u0989\u0981\u099a\u09c1 \u09a6\u09c7\u0993\u09af\u09bc\u09be\u09b2 \u09a6\u09bf\u09af\u09bc\u09c7 \u0998\u09c7\u09b0\u09be , \u09af\u09be\u09a4\u09c7 \u09e7\u09e6\u099f\u09be \u09a6\u09b0\u099c\u09be \u0986\u099b\u09c7 \u098f\u09ac\u0982 \u09b6\u09cb\u09ad\u09be \u09ac\u09be\u09a1\u09bc\u09be\u09a8\u09cb\u09b0 \u099c\u09a8\u09cd\u09af \u09e7\u09e8\u099f\u09be \u099a\u09c1\u09a1\u09bc\u09be \u0986\u099b\u09c7 
p501
aV\u0995\u09c7\u09b2\u09cd\u09b2\u09be\u09b0 \u09ad\u09c7\u09a4\u09b0\u09c7 \u0985\u09ac\u09b8\u09cd\u09a5\u09bf\u09a4 \u09b8\u09c1\u09b0\u099c\u09ae\u09b2 \u09b9\u09be\u09ac\u09c7\u09b2\u09bf \u098f\u0996\u09be\u09a8\u0995\u09be\u09b0 \u09a6\u09b0\u09cd\u09b6\u09a8\u09c0\u09af\u09bc \u0987\u09ae\u09be\u09b0\u09a4 
p502
aV\u098f\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 \u09b8\u09ac\u09a5\u09c7\u0995\u09c7 \u09ac\u09a1\u09bc \u099a\u09c1\u09a1\u09bc\u09be\u099f\u09bf\u0995\u09c7 \u09b2\u09be\u0996\u09cd\u0996\u09be \u099a\u09c1\u09a1\u09bc\u09be \u09ac\u09b2\u09be \u09b9\u09af\u09bc 
p503
aV\u09b0\u09be\u099c\u09be \u09b8\u09c1\u09b0\u099c\u09ae\u09b2 \u09aa\u09b0\u09c7 \u09a8\u09bf\u099c\u09c7\u09b0 \u09b0\u09be\u099c\u09a7\u09be\u09a8\u09c0 \u09ad\u09b0\u09a4\u09aa\u09c1\u09b0 \u09b8\u09cd\u09a5\u09be\u09a8\u09be\u09a8\u09cd\u09a4\u09b0 \u0995\u09b0\u09c7 \u0995\u09bf\u09a8\u09cd\u09a4\u09c1 \u09a1\u09bf\u0997 \u09a4\u09be\u0981\u09b0 \u0996\u09c1\u09ac \u09aa\u099b\u09a8\u09cd\u09a6\u09c7\u09b0 \u099b\u09bf\u09b2 
p504
aV\u09a1\u09bf\u0997 \u0995\u09c7\u09b2\u09cd\u09b2\u09be\u09b0 \u098f\u0995\u099f\u09be \u09ad\u09be\u0997\u09c7 \u0986\u099c\u0993 \u0995\u09bf\u099b\u09c1 \u09aa\u09cd\u09b0\u09b6\u09be\u09b8\u09a8\u09bf\u0995 \u0995\u09be\u09b0\u09cd\u09af\u09be\u09b2\u09af\u09bc \u0986\u099b\u09c7 
p505
aV\u0985\u0997\u09cd\u09a8\u09cd\u09af\u09be\u09b6\u09df \u0995\u09cd\u09b7\u09a4\u09bf\u0997\u09cd\u09b0\u09b8\u09cd\u09a5 \u09b9\u0993\u09df\u09be\u09b0 \u09ab\u09b2\u09c7 \u09ae\u09a7\u09c1\u09ae\u09c7\u09b9\u0993 \u09b9\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7 
p506
aV\u09af\u0995\u09c3\u09a4 \u09a8\u09b7\u09cd\u099f \u09b9\u09df\u09c7 \u09af\u09be\u0993\u09df\u09be\u09b0 \u09ab\u09b2\u09c7 \u09b8\u09bf\u09b0\u09cb\u09b8\u09bf\u09b8- \u09b9\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7 , \u09af\u0995\u09c3\u09a4\u09c7\u09b0 \u0995\u09cd\u09af\u09be\u09a8\u09b8\u09be\u09b0\u09c7\u09b0 \u0986\u09b6\u0999\u09cd\u0995\u09be \u09ac\u09c7\u09a1\u09bc\u09c7 \u09af\u09be\u09df 
p507
aV\u09b9\u09cb\u09b2\u09bf\u09a4\u09c7 \u09ad\u09c7\u09b7\u099c \u09b0\u0999 \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0 \u0995\u09b0\u09be\u09b0 \u099a\u09c7\u09b7\u09cd\u099f\u09be \u0995\u09b0\u09c1\u09a8 
p508
aV\u09aa\u09c1\u09b0\u09c1\u09b7\u09a6\u09c7\u09b0 \u09b6\u09c1\u0995\u09cd\u09b0\u09be\u09b6\u09df \u09a8\u09b7\u09cd\u099f \u09b9\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7 
p509
aV\u098f\u09b0 \u09ab\u09b2\u09c7 \u09a8\u09df \u09a4\u09cd\u09ac\u0995\u09c7 \u09b0\u0999 \u09a5\u09be\u0995\u09ac\u09c7 \u09a8\u09be \u0986\u09b0 \u09a8\u09df\u09a4\u09cb \u09a7\u09c1\u09b2\u09c7 \u09a4\u09be \u09b8\u09b9\u099c\u09c7 \u0989\u09a0\u09c7 \u09af\u09be\u09ac\u09c7 
p510
aV\u09af\u0996\u09a8 \u0986\u09aa\u09a8\u09bf \u09b9\u09cb\u09b2\u09bf \u0996\u09c7\u09b2\u09a4\u09c7 \u09ac\u09cb\u09b0\u09cb\u09a8 \u09a4\u0996\u09a8 \u0986\u09aa\u09a8\u09bf \u09b8\u09be\u09b0\u09be \u0997\u09be\u09df\u09c7 \u09ae\u09df\u09c7\u09b8\u09cd\u099a\u09be\u09b0\u09be\u0987\u099c\u09be\u09b0 \u09ac\u09be \u0995\u09cd\u09b2\u09bf\u099e\u09cd\u099c\u09bf\u0982 \u09ae\u09bf\u09b2\u09cd\u0995 \u09b2\u09be\u0997\u09bf\u09df\u09c7 \u09a8\u09bf\u09a8 
p511
aV\u0985\u09ac\u09b6\u09cd\u09af \u098f\u0987 \u09b8\u09ac\u0997\u09c1\u09b2\u09cb\u0987 \u09aa\u09cd\u09b0\u09be\u09a5\u09ae\u09bf\u0995 \u09aa\u09b0\u09cd\u09af\u09be\u09df\u09c7\u09b0 \u099a\u09c1\u09b2\u0995\u09be\u09a8\u09bf \u09b9\u09df 
p512
aV\u09b8\u09ac\u09c1\u099c , \u09b9\u09b2\u09c1\u09a6 \u0993 \u0995\u09ae\u09b2\u09be \u09b0\u0999 \u09a5\u09c7\u0995\u09c7 \u09ac\u09be\u0981\u099a\u09c1\u09a8 , \u0995\u09be\u09b0\u09a3 \u098f\u09a4\u09c7 \u0989\u09aa\u09b8\u09cd\u09a5\u09bf\u09a4 \u09b0\u09be\u09b8\u09be\u09df\u09a8\u09bf\u0995 \u09aa\u09a6\u09be\u09b0\u09cd\u09a5\u09c7\u09b0 \u0995\u09be\u09b0\u09a3\u09c7 \u09a4\u09cd\u09ac\u0995\u09c7\u09b0 \u0985\u09a8\u09c7\u0995 \u09b2\u09cb\u0995\u09b8\u09be\u09a8 \u09b9\u09df 
p513
aV\u098f\u09b0 \u09ab\u09b2\u09c7 \u09a8\u0996\u09c7 \u09b0\u0999 \u09a5\u09be\u0995\u09ac\u09c7 \u09a8\u09be 
p514
aV\u09a6\u09c1\u09b0\u09cd\u09ac\u09b2\u09a4\u09be , \u0995\u09cd\u09b2\u09be\u09a8\u09cd\u09a4\u09bf \u0985\u09a8\u09c1\u09ad\u09c2\u09a4 \u09b9\u09df , \u09a8\u09aa\u09c1\u0982\u09b8\u0995 \u09b9\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7 , \u0993\u099c\u09a8 \u0995\u09ae\u09c7 \u09af\u09be\u09df , \u0997\u09be\u0981\u099f\u09c7 \u09b8\u09cd\u09ab\u09c0\u09a4\u09bf \u09ac\u09be \u09ac\u09cd\u09af\u09be\u09a5\u09be \u09b9\u09df , \u09ac\u09ae\u09bf , \u09ae\u09be\u09a5\u09be \u0998\u09cb\u09b0\u09be \u0986\u09b8\u09c7 
p515
aV\u09a8\u0996\u09c7 \u09a8\u09c7\u09b2\u09aa\u09be\u09b2\u09bf\u09b6 \u09b2\u09be\u0997\u09bf\u09df\u09c7 \u09a8\u09bf\u09a8 
p516
aV\u098f\u0987 \u09b9\u09bf\u09ae\u09be\u099a\u09cd\u099b\u09a8\u09cd\u09a8 \u09b6\u09bf\u0996\u09b0\u0995\u09c7\u0993 \u0993\u0996\u09be\u09a8\u09c7 \u098f\u0995\u099f\u09be \u09aa\u09ac\u09bf\u09a4\u09cd\u09b0 \u09b6\u09bf\u0996\u09b0 \u09ac\u09b2\u09c7 \u09ae\u09a8\u09c7 \u0995\u09b0\u09be \u09b9\u09af\u09bc 
p517
aV\u099c\u09be\u09a8\u09c7\u09a8 \u0995\u09bf , \u0995\u09cb\u099f\u09c7\u09b0 \u0985\u09b0\u09cd\u09a5 \u09b9\u09b2 \u09a6\u09c2\u09b0\u09cd\u0997 \u0993 \u09aa\u09cc\u09b0\u09be\u09a3\u09bf\u0995 \u0986\u0996\u09cd\u09af\u09be\u09a8\u09c7 \u098f\u0987 \u09aa\u09b0\u09cd\u09ac\u09a4 \u09a8\u09a8\u09cd\u09a6\u09be\u09a6\u09c7\u09ac\u09c0 \u0985\u09b0\u09cd\u09a5\u09be\u09a4 \u09aa\u09be\u09b0\u09cd\u09ac\u09a4\u09c0\u09b0 \u0986\u09ac\u09be\u09b8 \u099b\u09bf\u09b2 
p518
aV\u09a8\u09a8\u09cd\u09a6\u09be\u09a6\u09c7\u09ac\u09c0\u0995\u09c7 \u0989\u09a4\u09cd\u09a4\u09b0\u09be\u0996\u09a3\u09cd\u09a1\u09c7\u09b0 \u0995\u09c1\u09ae\u09be\u0989\u09a8 \u098f\u09b2\u09be\u0995\u09be\u09b0 \u09a8\u09bf\u09ac\u09be\u09b8\u09c0 \u09a6\u09c7\u09ac\u09c0 \u09aa\u09be\u09b0\u09cd\u09ac\u09a4\u09c0\u09b0 \u09b8\u09cd\u09ac\u09b0\u09c2\u09aa \u09ac\u09b2\u09c7 \u09ae\u09be\u09a8\u09be \u09b9\u09af\u09bc 
p519
aV\u0995\u09c1\u09ae\u09be\u0989\u09a8\u09c7 \u0986\u09b0\u09c7\u0995\u099f\u09be \u0985\u09a8\u09cd\u09af \u09aa\u09b0\u09cd\u09ac\u09a4 \u09b9\u09b2 \u09a8\u09a8\u09cd\u09a6\u09be\u0995\u09cb\u099f 
p520
aV\u099f\u09cd\u09b0\u09c7\u0995\u09bf\u0999 \u09af\u09be\u09b0\u09be \u09aa\u099b\u09a8\u09cd\u09a6 \u0995\u09b0\u09c7\u09a8 \u09a4\u09be\u09b0\u09be \u098f\u09b0 \u09a6\u09bf\u0995\u09c7 \u0986\u0995\u09b0\u09cd\u09b7\u09bf\u09a4 \u09b9\u09a8 
p521
aV\u09a4\u09cb\u09ae\u09be\u09b0 \u09ae\u09a8\u09c7 \u0986\u099b\u09c7 , \u0986\u09ae\u09bf \u09a4\u09cb\u09ae\u09be\u0995\u09c7 \u09a8\u09a8\u09cd\u09a6\u09be\u09a6\u09c7\u09ac\u09c0 \u09aa\u09b0\u09cd\u09ac\u09a4\u09c7\u09b0 \u09ac\u09bf\u09b7\u09af\u09bc \u09ac\u09b2\u09c7\u099b\u09bf\u09b2\u09be\u09ae ?
p522
aV\u098f\u0987 \u09b6\u09c3\u0999\u09cd\u0997 \u09a8\u09a8\u09cd\u09a6\u09be\u09a6\u09c7\u09ac\u09c0 \u09a5\u09c7\u0995\u09c7 \u09a6\u0995\u09cd\u09b7\u09bf\u09a3 - \u09aa\u09c2\u09b0\u09cd\u09ac \u09a6\u09bf\u0995\u09c7 \u09aa\u09cd\u09b0\u09be\u09af\u09bc \u09e7\u09eb \u0995\u09bf\u09ae\u09bf \u09a6\u09c2\u09b0\u09a4\u09cd\u09ac\u09c7 \u0985\u09ac\u09b8\u09cd\u09a5\u09bf\u09a4 
p523
aV\u09b8\u09ae\u09c1\u09a6\u09cd\u09b0\u09a4\u09b2 \u09a5\u09c7\u0995\u09c7 \u098f\u09b0 \u0989\u099a\u09cd\u099a\u09a4\u09be \u09ec\u09ee\u09ee\u09e7 \u09ae\u09bf\u099f\u09be\u09b0 \u0985\u09b0\u09cd\u09a5\u09be\u09a4 \u09e8\u09e8\u09eb\u09e7\u09e6 \u09ab\u09c1\u099f 
p524
aV\u09ac\u09bf\u09af\u09bc\u09be\u09ac\u09be\u09a8 \u09a6\u09c7\u0996\u09be \u0993 \u0985\u09a8\u09cd\u09ac\u09c7\u09b7\u09a3 \u0995\u09b0\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u09a8\u09cb\u09b0\u09be\u09a7\u09be\u09b0 \u09aa\u09be\u09b9\u09be\u09a1\u09bc\u09c7\u09b0 \u0993\u09aa\u09b0\u09c7 \u09a4\u09c8\u09b0\u09bf \u099c\u0999\u09cd\u0997\u09b2 \u09b9\u09b2 \u0985\u09a4\u09cd\u09af\u09a8\u09cd\u09a4 \u0997\u09c1\u09b0\u09c1\u09a4\u09cd\u09ac\u09aa\u09c2\u09b0\u09cd\u09a3 \u09b8\u09cd\u09a5\u09be\u09a8  
p525
aV\u09a8\u09a8\u09cd\u09a6\u09be\u0995\u09cb\u099f \u09b6\u09c3\u0999\u09cd\u0997 \u0989\u09a4\u09cd\u09a4\u09b0\u09be\u0996\u09a3\u09cd\u09a1\u09c7\u09b0 \u09aa\u09bf\u09a5\u09cb\u09b0\u09be\u0997\u09a2\u09bc \u099c\u09c7\u09b2\u09be\u09af\u09bc \u0985\u09ac\u09b8\u09cd\u09a5\u09bf\u09a4 
p526
aV\u09ac\u09b2\u09be \u09b9\u09af\u09bc \u09af\u09c7 \u0987\u0982\u09b0\u09be\u099c\u09bf\u09b0- \u09a6\u09c1\u099f\u09cb \u09b6\u09ac\u09cd\u09a6 ` \u09a8\u09c7\u099a\u09be\u09b0 ` \u0986\u09b0 ` \u09b9\u09be\u099f\u0995\u09c7 ` \u09aa\u09b0\u09b8\u09cd\u09aa\u09b0\u09c7\u09b0 \u09b8\u09be\u09a5\u09c7 \u09ae\u09bf\u09b2\u09bf\u09af\u09bc\u09c7 \u09a8\u09c7\u09a4\u09be\u09b0\u09b9\u09be\u099f \u09a4\u09c8\u09b0\u09bf \u09b9\u09af\u09bc\u09c7\u099b\u09c7 
p527
aV\u09aa\u09c1\u09b0\u09be\u09a3\u09c7- \u09aa\u09cd\u09b0\u09af\u09bc\u09be\u0997\u0995\u09c7- \u09a4\u09c0\u09b0\u09cd\u09a5\u09b0\u09be\u099c- \u09ac\u09b2\u09be\u09b0 \u0995\u09a5\u09be\u0993 \u09aa\u09be\u0993\u09af\u09bc\u09be \u09af\u09be\u09af\u09bc 
p528
aV\u0995\u09a5\u09be \u09b9\u099a\u09cd\u099b\u09c7 - \u09b8\u09ae\u09c1\u09a6\u09cd\u09b0 \u09ae\u09a8\u09cd\u09a5\u09a8\u09c7\u09b0 \u09b8\u09ae\u09af\u09bc \u09ac\u09c7\u09b0\u09bf\u09af\u09bc\u09c7 \u0986\u09b8\u09be \u09b8\u09ac \u09b0\u09a4\u09cd\u09a8\u09c7\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 \u0985\u09ae\u09c3\u09a4\u0993 \u09ac\u09c7\u09b0 \u09b9\u09af\u09bc \u09af\u09be \u09aa\u09c7\u09a4\u09c7 \u09a6\u09c7\u09ac \u09a6\u09be\u09a8\u09ac\u09c7\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 \u09b2\u09a1\u09bc\u09be\u0987 \u09b6\u09c1\u09b0\u09c1 \u09b9\u09af\u09bc\u09c7 \u09af\u09be\u09af\u09bc 
p529
aV\u09ad\u0997\u09ac\u09be\u09a8 \u09ac\u09be\u09b8\u09c1\u09a6\u09c7\u09ac- \u09b0\u09be\u09af\u09bc\u09c7\u09b0- \u09ae\u09a8\u09cd\u09a6\u09bf\u09b0\u09c7\u09b0 \u09b8\u09cd\u09a5\u09be\u09aa\u09a8\u09be \u09ac\u09bf\u09a7\u09bf\u09ae\u09a4 \u09aa\u09c2\u099c\u09be - \u0985\u09b0\u09cd\u099a\u09a8\u09be \u0995\u09b0\u09c7 \u09a8\u0997\u09b0\u09ac\u0982\u09b6\u09c0- \u09b0\u09be\u099c\u09be \u099a\u09c7\u09a4\u0995\u09b0\u09cd\u09a3- \u09e7\u09ea\u09ed\u09e6 \u09b8\u09be\u09b2\u09c7 \u0995\u09b0\u09c7\u09a8 
p530
aV\u09ad\u0997\u09ac\u09be\u09a8\u09c7\u09b0 \u09ae\u09c2\u09b0\u09cd\u09a4\u09bf \u09b9\u09b2 \u0995\u09be\u09b2\u09cb \u09aa\u09be\u09a5\u09b0\u09c7\u09b0 \u09a4\u09c8\u09b0\u09bf 
p531
aV\u09b8\u09cd\u09a5\u09be\u09a8\u09c0\u09af\u09bc \u0995\u09bf\u0982\u09ac\u09a6\u09a8\u09cd\u09a4\u09c0 \u09b9\u09b2 \u0997\u09b0\u09ae\u09c7\u09b0 \u09a6\u09bf\u09a8\u09c7 \u09ae\u09c2\u09b0\u09cd\u09a4\u09bf \u09a5\u09c7\u0995\u09c7 \u0998\u09be\u09ae \u099d\u09a1\u09bc\u09c7 
p532
aV\u09aa\u09cd\u09b0\u09af\u09bc\u09be\u0997\u0995\u09c7- \u09a4\u09c0\u09b0\u09cd\u09a5\u09c7\u09b0 \u09b0\u09be\u099c\u09be \u09ac\u09b2\u09be \u09b9\u09af\u09bc 
p533
aV\u098f\u0996\u09be\u09a8\u09c7 \u098f\u0995\u099f\u09be \u099c\u09b2\u09be\u09b6\u09af\u09bc \u0986\u099b\u09c7 \u09af\u09be\u0995\u09c7 \u0997\u0999\u09cd\u0997\u09be\u09b8\u09cd\u09b0\u09cb\u09a4- \u09ac\u09b2\u09be \u09b9\u09af\u09bc 
p534
aV\u0997\u09c1\u09ae\u09b2\u09be- \u099c\u09c7\u09b2\u09be\u09b0 \u0998\u09be\u0998\u09b0\u09be- \u09a5\u09be\u09a8\u09be\u09b0 \u0985\u09a8\u09cd\u09a4\u09b0\u09cd\u0997\u09a4 \u09b8\u09c7\u09a8\u09cd\u09b9\u09be- \u09a5\u09c7\u0995\u09c7 \u09ea \u0995\u09bf.\u09ae\u09bf. \u09a6.\u09aa. \u09a6\u09bf\u09b6\u09be\u09af\u09bc \u0995\u09cb\u09b0\u09be\u09ae\u09cd\u09ac\u09c7- \u0997\u09cd\u09b0\u09be\u09ae \u0985\u09ac\u09b8\u09cd\u09a5\u09bf\u09a4 \u09b0\u09af\u09bc\u09c7\u099b\u09c7 
p535
aV\u0995\u09cb\u09b0\u09ae\u09cd\u09ac\u09c7- \u0997\u09cd\u09b0\u09be\u09ae \u09a6\u0995\u09cd\u09b7\u09bf\u09a3- \u0995\u09cb\u09af\u09bc\u09c7\u09b2- \u09a8\u09a6\u09c0\u09b0 \u09a4\u09c0\u09b0\u09c7 \u0985\u09ac\u09b8\u09cd\u09a5\u09bf\u09a4 \u09b0\u09af\u09bc\u09c7\u099b\u09c7 
p536
aV\u09a8\u09a6\u09c0\u09b0 \u09a4\u09c0\u09b0\u09c7\u0987 \u09ad\u0997\u09ac\u09be\u09a8 \u09ac\u09be\u09b8\u09c1\u09a6\u09c7\u09ac- \u09b0\u09be\u09af\u09bc\u09c7\u09b0- \u09ae\u09a8\u09cd\u09a6\u09bf\u09b0 \u09b0\u09af\u09bc\u09c7\u099b\u09c7 
p537
aV\u09ae\u09be\u09b0\u0997\u09cb\u09af\u09bc\u09be\u09af\u09bc- \u0985\u099e\u09cd\u099c\u09c1\u09a8\u09be- \u09ab\u09b2\u09c7\u09b0 \u09ac\u09be\u099c\u09be\u09b0 , \u09ae\u09be\u09aa\u09b8\u09be- \u09ab\u09cd\u09b0\u09be\u0987\u09a1- \u09ae\u09be\u09b0\u09cd\u0995\u09c7\u099f- , \u09a8\u09bf\u0989- \u09ae\u09be\u09b0\u09cd\u0995\u09c7\u099f- \u09b9\u09b2 \u098f\u09ae\u09a8 \u0995\u09bf\u099b\u09c1 \u09b8\u09cd\u09a5\u09be\u09a8 \u09af\u09c7\u0996\u09be\u09a8\u09c7 \u0985\u09cd\u09af\u09be\u09a8\u09cd\u099f\u09bf\u0995 , \u099c\u09be\u0999\u09cd\u0995 \u099c\u09c1\u09af\u09bc\u09c7\u09b2\u09be\u09b0\u09c0 , \u0995\u099a\u09cd\u099b\u09c0 \u0995\u09a1\u09bc\u09be\u0987\u09af\u09bc\u09c7\u09b0 \u09ac\u09b8\u09cd\u09a4\u09c1 \u09b8\u09a0\u09bf\u0995 \u09a6\u09be\u09ae\u09c7 \u09aa\u09be\u0993\u09af\u09bc\u09be \u09af\u09be\u09af\u09bc 
p538
aV\u09ac\u09be\u0997\u09be- \u09ac\u09c0\u099a\u09c7\u09b0 \u09a7\u09be\u09b0\u09c7 \u0997\u09cb\u09af\u09bc\u09be\u09b0- \u09e9\u09e6 \u09b6\u09a4\u09be\u0982\u09b6 \u0985\u09cd\u09af\u09be\u09a8\u09cd\u099f\u09bf\u0995 \u09a6\u09cb\u0995\u09be\u09a8 \u0986\u099b\u09c7 
p539
aV\u09a4\u09be\u09a6\u09c7\u09b0 \u099a\u09cb\u0996 \u09a6\u09bf\u09af\u09bc\u09c7 \u09ad\u0995\u09cd\u09a4\u09bf \u0989\u09aa\u099a\u09c7 \u09aa\u09a1\u09bc\u099b\u09bf\u09b2 
p540
aV\u09b0\u09be\u0981\u099a\u09c0- \u0995\u09b2\u0995\u09be\u09a4\u09be- , \u09a6\u09bf\u09b2\u09cd\u09b2\u09c0- , \u09ae\u09c1\u09ae\u09cd\u09ac\u09be\u0987- \u0986\u09b0 \u09aa\u09be\u099f\u09a8\u09be\u09a4\u09c7- \u09ac\u09bf\u09ae\u09be\u09a8\u09c7\u09b0 \u09b8\u09be\u09b9\u09be\u09af\u09cd\u09af\u09c7 \u09b8\u09b9\u099c\u09c7\u0987 \u09aa\u09cc\u0981\u099b\u09cb\u09a8\u09cb \u09af\u09be\u09af\u09bc 
p541
aV\u09ac\u09bf\u0995\u09c7\u09b2 \u09aa\u09a1\u09bc\u09a4\u09c7 \u09a8\u09be \u09aa\u09a1\u09bc\u09a4\u09c7\u0987 \u09a1\u09cb\u09a8\u09be- \u09aa\u09be\u09b2\u09be\u09b0- \u098f\u0995\u099f\u09be \u09a8\u09be \u098f\u0995\u099f\u09be \u099b\u09cb\u099f \u09b2\u09be\u09b2 \u0987\u0981\u099f\u09c7\u09b0 \u0998\u09b0\u09c7\u09b0 \u09b8\u09be\u09ae\u09a8\u09c7 \u0995\u09cb\u09a8\u0993 \u09af\u09c1\u09ac\u0995 \u0997\u09bf\u099f\u09be\u09b0 \u09ac\u09be\u099c\u09be\u099a\u09cd\u099b\u09c7 \u099a\u09cb\u0996\u09c7 \u09aa\u09a1\u09bc\u09c7 \u09af\u09be\u09ac\u09c7 
p542
aV\u0986\u09b6\u09c7\u09aa\u09be\u09b6\u09c7 \u09a6\u09be\u0981\u09a1\u09bc\u09be\u09a8\u09cb \u0985\u099c\u09be\u09a8\u09be \u0985\u099a\u09c7\u09a8\u09be \u09b2\u09cb\u0995\u09c7\u09a6\u09c7\u09b0\u0993 \u0997\u09be\u09a8\u09c7\u09b0 \u09b8\u09c1\u09b0\u09c7 \u09a8\u09be\u099a\u09a4\u09c7 \u09a6\u09c7\u0996\u09be \u09af\u09be\u09af\u09bc 
p543
aV\u0989\u09a8\u09bf \u09b8\u09be\u09b0\u09be \u09ad\u09be\u09b0\u09a4- \u0998\u09c1\u09b0\u09c7\u099b\u09c7\u09a8 \u0995\u09bf\u09a8\u09cd\u09a4\u09c1 \u09af\u09c7 \u09b8\u09cd\u09ac\u09a4\u09a8\u09cd\u09a4\u09cd\u09b0\u09a4\u09be \u0986\u09b0 \u09ae\u09c1\u0995\u09cd\u09a4\u09ad\u09be\u09ac \u0997\u09cb\u09af\u09bc\u09be\u09af\u09bc- \u09a6\u09c7\u0996\u099b\u09c7\u09a8 \u09a4\u09be\u09b0 \u09a6\u09cd\u09ac\u09be\u09b0\u09be \u0989\u09a8\u09bf \u0985\u09ad\u09bf\u09ad\u09c2\u09a4 
p544
aV\u0997\u09cb\u09af\u09bc\u09be\u09ac\u09be\u09b8\u09c0\u09b0\u09be- \u09b9\u09b2\u09c7\u09a8 \u09b8\u09cd\u09ac\u09af\u09bc\u0982 \u09a8\u09c3\u09a4\u09cd\u09af - \u0997\u09c0\u09a4 \u0986\u09b0 \u0996\u09c7\u09b2\u09be\u09a7\u09c2\u09b2\u09be \u09aa\u09cd\u09b0\u09c7\u09ae\u09c0 
p545
aV\u09ac\u09be\u09b2\u09cd\u09ae\u09bf\u0995\u09c0- \u09a8\u09cd\u09af\u09be\u09b6\u09a8\u09be\u09b2- \u09aa\u09be\u09b0\u09cd\u0995\u09c7- \u09e7\u09ef\u09ed\u09e9 \u09b8\u09be\u09b2\u09c7 \u099f\u09be\u0987\u0997\u09be\u09b0 \u09aa\u09cd\u09b0\u09af\u09c7\u0995\u09cd\u099f \u09b6\u09c1\u09b0\u09c1 \u0995\u09b0\u09be \u09b9\u09af\u09bc \u09af\u09c7\u0996\u09be\u09a8\u09c7 \u0986\u099c\u0993 \u09ac\u09be\u0998 \u0986\u09b0 \u09a4\u09be\u09b0 \u09b8\u09ae\u09c1\u09a6\u09af\u09bc \u09aa\u09b0\u09bf\u09ac\u09c7\u09b6\u0995\u09c7 \u09b8\u09c1\u09b0\u0995\u09cd\u09b7\u09bf\u09a4 \u0995\u09b0\u09be\u09b0 \u0995\u09be\u099c \u099a\u09b2\u099b\u09c7 
p546
aV\u09ac\u09be\u09b2\u09cd\u09ae\u09bf\u0995\u09c0- \u09a8\u09cd\u09af\u09be\u09b6\u09a8\u09be\u09b2- \u0985\u09ad\u09af\u09bc\u09be\u09b0\u09a3\u09cd\u09af\u0995\u09c7- \u09e7\u09ef\u09ee\u09ef \u09b8\u09be\u09b2\u09c7 \u09a8\u09cd\u09af\u09be\u09b6\u09a8\u09be\u09b2 \u09aa\u09be\u09b0\u09cd\u0995\u09c7\u09b0 \u09b8\u09ae\u09cd\u09ae\u09be\u09a8 \u09a6\u09c7\u0993\u09af\u09bc\u09be \u09b9\u09af\u09bc 
p547
aV\u09ac\u09be\u09b2\u09cd\u09ae\u09bf\u0995\u09c0- \u09a8\u09cd\u09af\u09be\u09b6\u09a8\u09be\u09b2- \u09aa\u09be\u09b0\u09cd\u0995 \u09e9\u09e9\u09ee \u09ac\u09b0\u09cd\u0997 \u0995\u09bf\u09b2\u09cb\u09ae\u09bf\u099f\u09be\u09b0 \u099c\u09be\u09af\u09bc\u0997\u09be\u09b0 \u0989\u09aa\u09b0 \u09ac\u09bf\u09b8\u09cd\u09a4\u09c3\u09a4 \u09b0\u09af\u09bc\u09c7\u099b\u09c7 
p548
aV\u09a8\u09c7\u09aa\u09be\u09b2- \u09b8\u09c0\u09ae\u09be\u09b0 \u09b2\u09be\u0997\u09cb\u09af\u09bc\u09be \u09ac\u09bf\u09b9\u09be\u09b0\u09c7\u09b0- \u09aa\u09b6\u09cd\u099a\u09bf\u09ae- \u099a\u09ae\u09cd\u09aa\u09be\u09b0\u09a3- \u099c\u09c7\u09b2\u09be\u09af\u09bc \u0985\u09ac\u09b8\u09cd\u09a5\u09bf\u09a4 \u09ac\u09be\u09b2\u09cd\u09ae\u09bf\u0995\u09c0- \u09a8\u09cd\u09af\u09be\u09b6\u09a8\u09be\u09b2- \u09aa\u09be\u09b0\u09cd\u0995- \u09b9\u09b2 \u09b8\u09c1\u09ac\u09c7\u09b0- \u098f\u0995 \u09ae\u09be\u09a4\u09cd\u09b0 \u099f\u09be\u0987\u0997\u09be\u09b0 \u09aa\u09cd\u09b0\u099c\u09c7\u0995\u09cd\u099f 
p549
aV\u09ac\u09be\u09b2\u09cd\u09ae\u09bf\u0995\u09c0- \u09a8\u09cd\u09af\u09be\u09b6\u09a8\u09be\u09b2- \u09aa\u09be\u09b0\u09cd\u0995\u09c7\u09b0- \u09a8\u09bf\u0995\u099f\u09ac\u09b0\u09cd\u09a4\u09c0 \u09b0\u09c7\u09b2\u0993\u09af\u09bc\u09c7 \u09b8\u09cd\u099f\u09c7\u09b6\u09a8 \u09b9\u09b2 \u09b0\u0995\u09cd\u09b8\u09cc\u09b2- 
p550
aV\u09ac\u09c7\u09b8\u09b0\u0995\u09be\u09b0\u09bf \u0993 \u09b8\u09b0\u0995\u09be\u09b0\u09bf \u09ac\u09be\u09b8\u09c7\u09b0 \u09b8\u09be\u09a5\u09c7 \u09aa\u09cd\u09b0\u09be\u0987\u09ad\u09c7\u099f \u099f\u09cd\u09af\u09be\u0995\u09cd\u09b8\u09bf\u0993 \u09b8\u09b0\u09cd\u09ac\u09a6\u09be \u09aa\u09be\u0993\u09af\u09bc\u09be \u09af\u09be\u09af\u09bc 
p551
aV\u09ac\u09be\u09b2\u09cd\u09ae\u09bf\u0995\u09c0- \u09a8\u09cd\u09af\u09be\u09b6\u09a8\u09be\u09b2- \u09aa\u09be\u09b0\u09cd\u0995- \u09aa\u09be\u099f\u09a8\u09be- \u09a5\u09c7\u0995\u09c7 \u09e8\u09ec\u09e6 \u0995\u09bf\u09b2\u09cb\u09ae\u09bf\u099f\u09be\u09b0 \u09a6\u09c2\u09b0\u09c7 \u0985\u09ac\u09b8\u09cd\u09a5\u09bf\u09a4 
p552
aV\u09aa\u09be\u09b0\u09cd\u0995\u09c7 \u09ac\u09be\u0998 , \u099c\u09be\u0997\u09c1\u09af\u09bc\u09be\u09b0 , \u0997\u09a3\u09cd\u09a1\u09be\u09b0 , \u099a\u09bf\u09a4\u09b2 , \u0995\u09c3\u09b7\u09cd\u09a3\u09b8\u09be\u09b0 \u09b9\u09b0\u09bf\u09a3 , \u09a8\u09c0\u09b2 \u0997\u09be\u0987 , \u09b9\u09b0\u09bf\u09a3 \u0987\u09a4\u09cd\u09af\u09be\u09a6\u09bf \u099c\u0982\u09b2\u09c0 \u099c\u09be\u09a8\u09cb\u09af\u09bc\u09be\u09b0\u09a6\u09c7\u09b0 \u0996\u09cb\u09b2\u09be \u0985\u09ac\u09b8\u09cd\u09a5\u09be\u09af\u09bc \u0998\u09c1\u09b0\u09c7 \u09ac\u09c7\u09a1\u09bc\u09be\u09a4\u09c7 , \u0986\u09a8\u09a8\u09cd\u09a6 \u0995\u09b0\u09a4\u09c7 , \u09b6\u09bf\u0995\u09be\u09b0 \u0995\u09b0\u09a4\u09c7 \u09b8\u09b9\u099c\u09c7 \u09a6\u09c7\u0996\u09be \u09af\u09be\u09af\u09bc 
p553
aV\u09b9\u09be\u099c\u09be\u09b0 \u09b9\u09be\u09a1\u09be\u09b0 \u09aa\u09b0\u09cd\u09af\u099f\u0995\u09b0\u09be \u098f\u0987 \u099c\u09b2\u09a6\u09c1\u09b0\u09cd\u0997\u0995\u09c7 \u09a6\u09c7\u0996\u09be\u09b0 \u099c\u09a8\u09cd\u09af\u09c7 \u09ad\u09bf\u09a1\u09bc \u0995\u09b0\u09c7\u09a8 \u0986\u09b0 \u09b8\u09be\u0997\u09b0\u09c7 \u09a4\u09c8\u09b0\u09bf \u098f\u0987 \u09a6\u09c1\u09b0\u09cd\u0997\u09c7\u09b0 \u09a8\u09bf\u09b0\u09cd\u09ae\u09be\u09a3\u09c7\u09b0 \u099c\u09a8\u09cd\u09af\u09c7 \u09b6\u09bf\u09ac\u09be\u099c\u09c0 \u09ae\u09b9\u09be\u09b0\u09be\u099c\u09c7\u09b0 \u0995\u09b2\u09cd\u09aa\u09a8\u09be\u09b6\u0995\u09cd\u09a4\u09bf\u09b0 \u09aa\u09cd\u09b0\u09b6\u0982\u09b8\u09be \u09a8\u09be \u0995\u09b0\u09c7 \u09a5\u09be\u0995\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7\u09a8 \u09a8\u09be 
p554
aV\u098f\u0987 \u09aa\u09a6\u09be\u09b0\u09cd\u09a5 \u09b2\u09be\u09b2 \u09b0\u0999\u09c7\u09b0 \u098f\u09ac\u0982 \u098f\u099f\u09be\u0987 \u0995\u09ae\u09b2\u09be \u09b2\u09c7\u09ac\u09c1\u0995\u09c7 \u0997\u09be\u09a2\u09bc \u09b0\u0999 \u09aa\u09cd\u09b0\u09a6\u09be\u09a8 \u0995\u09b0\u09c7 
p555
aV\u0985\u09cd\u09af\u09be\u09a8\u09cd\u09a5\u09cb\u09b8\u09be\u0987\u09a8\u09bf\u09a8 \u098f\u0995 \u09aa\u09cd\u09b0\u0995\u09be\u09b0\u09c7\u09b0 \u0985\u09cd\u09af\u09be\u09a8\u09cd\u099f\u09bf\u0985\u0995\u09cd\u09b8\u09bf\u09a1\u09c7\u09a8\u09cd\u099f 
p556
aV\u098f\u099f\u09be \u098f\u0995 \u09aa\u09cd\u09b0\u09be\u0995\u09c3\u09a4\u09bf\u0995 \u09b0\u09b8\u09be\u09df\u09a8 \u09af\u09be \u09b0\u09cb\u0997 \u09a6\u09c2\u09b0 \u0995\u09b0\u09a4\u09c7 \u09b8\u09be\u09b9\u09be\u09af\u09cd\u09af \u0995\u09b0\u09c7 
p557
aV\u09a6\u09c1\u09a8\u09bf\u09df\u09be\u09df \u09b8\u09ac\u09a5\u09c7\u0995\u09c7 \u09ac\u09c7\u09b6\u09bf \u09ad\u09be\u09b0\u09a4\u09c0\u09df\u09b0\u09be \u0995\u09b0\u09cb\u09a8\u09be\u09b0\u09bf \u0986\u09b0\u09cd\u099f\u09be\u09b0\u09bf\u09b0 \u0985\u09b8\u09c1\u0996 ( \u09b8\u09bf.\u098f.\u09a1\u09bf ) \u0985\u09b0\u09cd\u09a5\u09be\u09a4 \u09b9\u09c3\u09a6\u09df\u09c7\u09b0 \u09a7\u09ae\u09a8\u09c0\u09b0 \u0985\u09b8\u09c1\u0996\u09c7 \u09aa\u09c0\u09a1\u09bc\u09bf\u09a4 \u09ac\u09be \u098f\u09b0 \u0995\u09be\u09b0\u09a3\u09c7\u0987 \u09a4\u09be\u09a6\u09c7\u09b0 \u09ae\u09c3\u09a4\u09cd\u09af\u09c1 \u09b9\u09df 
p558
aV\u09ad\u09be\u09b0\u09a4\u09c7\u09b0 \u09ac\u09a1\u09bc \u09ac\u09a1\u09bc \u09b6\u09b9\u09b0\u09c7 \u09a5\u09be\u0995\u09be \u09e7\u09e6 \u09b6\u09a4\u09be\u0982\u09b6 \u09b2\u09cb\u0995 \u09b8\u09bf.\u098f.\u09a1\u09bf-\u09b0 \u09b6\u09bf\u0995\u09be\u09b0 
p559
aV\u09aa\u09be\u09b6\u09cd\u099a\u09be\u09a4\u09cd\u09af \u098f\u09ac\u0982 \u0985\u09a8\u09cd\u09af\u09be\u09a8\u09cd\u09af \u098f\u09b6\u09bf\u09df\u09a6\u09c7\u09b0 \u09a4\u09c1\u09b2\u09a8\u09be\u09df \u09ad\u09be\u09b0\u09a4\u09c0\u09df\u09a6\u09c7\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 \u09b8\u09bf.\u098f.\u09a1\u09bf\u09b0 \u09aa\u09cd\u09b0\u09ad\u09be\u09ac \u09a6\u09c1\u0987 \u09a5\u09c7\u0995\u09c7 \u099a\u09be\u09b0 \u0997\u09c1\u09a3 \u09ac\u09c7\u09b6\u09bf \u098f\u09ac\u0982 \u099a\u09b2\u09cd\u09b2\u09bf\u09b6\u09c7\u09b0 \u0995\u09ae \u09ac\u09df\u09b8\u09c0 \u09ac\u09cd\u09af\u0995\u09cd\u09a4\u09bf\u09a6\u09c7\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 \u098f\u099f\u09bf\u09b0 \u09aa\u09cd\u09b0\u09ad\u09be\u09ac \u09aa\u09be\u0981\u099a \u09a5\u09c7\u0995\u09c7 \u09a6\u09b6 \u0997\u09c1\u09a3 \u09ac\u09c7\u09b6\u09bf 
p560
aV\u0985\u09a8\u09c1\u09ae\u09be\u09a8 \u0995\u09b0\u09be \u09b9\u09df\u09c7\u099b\u09c7 \u09af\u09c7 \u0989\u09a4\u09cd\u09a4\u09b0 \u09ad\u09be\u09b0\u09a4\u09c7\u09b0 \u09ac\u09a1\u09bc \u09b6\u09b9\u09b0\u09c7\u09b0 \u09ac\u09be\u09b8\u09bf\u09a8\u09cd\u09a6\u09be\u09a6\u09c7\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 \u09ed - \u09e7\u09e6 \u09b6\u09a4\u09be\u0982\u09b6 \u098f\u09ac\u0982 \u09a6\u0995\u09cd\u09b7\u09bf\u09a3 \u09ad\u09be\u09b0\u09a4\u09c7\u09b0 \u09ac\u09a1\u09bc \u09b6\u09b9\u09b0\u09c7\u09b0 \u09ac\u09be\u09b8\u09bf\u09a8\u09cd\u09a6\u09be\u09a6\u09c7\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 \u09e7\u09ea \u09b6\u09a4\u09be\u0982\u09b6 \u09b2\u09cb\u0995 \u09b8\u09bf.\u098f.\u09a1\u09bf-\u09b0 \u09a6\u09cd\u09ac\u09be\u09b0\u09be \u09aa\u09c0\u09a1\u09bc\u09bf\u09a4 
p561
aV\u09b9\u09c3\u09a6\u09b0\u09cb\u0997\u0997\u09c1\u09b2\u09bf\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 \u09b8\u09bf.\u098f.\u09a1\u09bf \u09b8\u09ac\u09a5\u09c7\u0995\u09c7 \u09ac\u09c7\u09b6\u09bf \u09aa\u09be\u0993\u09df\u09be \u09af\u09be\u09df \u09af\u09be \u09ac\u09c1\u0995\u09c7 \u09ac\u09cd\u09af\u09be\u09a5\u09be , \u09b9\u09c3\u09a6\u09df\u09be\u0998\u09be\u09a4 \u09ac\u09be \u09b9\u09a0\u09be\u09a4 \u09ae\u09c3\u09a4\u09cd\u09af\u09c1\u09b0 \u09b0\u09c2\u09aa\u09c7 \u09ac\u09cd\u09af\u0995\u09cd\u09a4 \u09b9\u09df 
p562
aV\u09b9\u09c3\u09a6\u09df \u09a7\u09ae\u09a8\u09c0 \u09ac\u09cd\u09b2\u09cb\u09df\u09be\u09b0\u09c7\u09b0 \u09ae\u09a4\u09cb \u09b0\u0995\u09cd\u09a4 \u09aa\u09be\u09ae\u09cd\u09aa \u0995\u09b0\u09c7 
p563
aV\u098f\u09b0 \u09ab\u09b2\u09c7 \u09b0\u0995\u09cd\u09a4\u09c7 \u09ae\u09bf\u09b6\u09c7 \u09a5\u09be\u0995\u09be \u09aa\u09c1\u09b7\u09cd\u099f\u09bf\u0995\u09b0 \u09a4\u09a4\u09cd\u09ac \u0993 \u0985\u0995\u09cd\u09b8\u09bf\u099c\u09c7\u09a8 \u09b6\u09b0\u09c0\u09b0\u09c7\u09b0 \u09b8\u09ac \u0985\u0982\u09b6\u09c7 \u09aa\u09cc\u0981\u099b\u09be\u09df 
p564
aV\u09ac\u09b2\u09be \u09b9\u09af\u09bc \u09aa\u09c0\u09b0- \u09ac\u09be\u09ac\u09be\u09b0- \u09a6\u09b0\u0997\u09be- \u09a8\u09bf\u099c\u09c7\u09b0 \u09b2\u09cb\u0995\u09a6\u09c7\u09b0 \u0985\u09b6\u09c1\u09ad \u09b6\u0995\u09cd\u09a4\u09bf \u0986\u09b0 \u09a6\u09c1\u09b0\u09cd\u0998\u099f\u09a8\u09be\u09b0 \u0995\u09ac\u09b2 \u09a5\u09c7\u0995\u09c7 \u09b0\u0995\u09cd\u09b7\u09be \u0995\u09b0\u09c7\u09a8 
p565
aV\u0997\u09c1\u09b0\u09c1- \u0997\u09cb\u09ac\u09bf\u09a8\u09cd\u09a6\u09c7\u09b0- \u09ac\u09a8\u09cd\u09a7\u09c1 \u09aa\u09c0\u09b0- \u09ac\u09be\u09ac\u09be- \u09b8\u09be\u09b0\u09be \u099c\u09c0\u09ac\u09a8 \u09b6\u09c1\u09a7\u09c1 \u09a6\u09c1\u09a7\u0987 \u0996\u09c7\u09af\u09bc\u09c7\u099b\u09c7\u09a8 \u0986\u09b0 \u09eb\u09e6\u09e6 \u09ac\u099b\u09b0 \u09ac\u09af\u09bc\u09b8\u09c7 \u09a6\u09c7\u09b9\u09a4\u09cd\u09af\u09be\u0997 \u0995\u09b0\u09c7\u09a8 
p566
aV\u0989\u09a6\u09be\u09b9\u09b0\u09a3\u09b8\u09cd\u09ac\u09b0\u09c2\u09aa \u0986\u09ab\u09cd\u09b0\u09bf\u0995\u09be\u09a8- \u099c\u09c7\u09ac\u09cd\u09b0\u09be , \u099c\u09bf\u09b0\u09be\u09ab , \u09b6\u09c1\u09a4\u09c1\u09b0\u09cd\u09ae\u09c1\u09b0\u09cd\u0997 , \u0987\u09ae\u09cd\u09aa\u09be\u09b2\u09be , \u099a\u09bf\u09a4\u09be , \u09b8\u09bf\u0982\u09b9 , \u09b9\u09be\u09a4\u09bf , \u0997\u09a3\u09cd\u09a1\u09be\u09b0 \u0987\u09a4\u09cd\u09af\u09be\u09a6\u09bf\u09b0 \u09a8\u09be\u09ae \u0995\u09b0\u09be \u09af\u09c7\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7 
p567
aV\u098f\u09a6\u09c7\u09b0 \u09a6\u09c7\u0996\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u0987\u0989\u09b0\u09cb\u09aa- , \u0986\u09ae\u09c7\u09b0\u09bf\u0995\u09be- , \u0995\u09be\u09a8\u09be\u09a1\u09be- , \u0985\u09b8\u09cd\u099f\u09cd\u09b0\u09c7\u09b2\u09bf\u09af\u09bc\u09be\u09b0- \u09aa\u09b0\u09cd\u09af\u099f\u0995\u09b0\u09be \u09ad\u09bf\u09a1\u09bc \u0995\u09b0\u09c7 
p568
aV\u09b9\u09bf\u09a8\u09cd\u09a6\u09c0- \u098f\u09ac\u0982 \u0997\u09c1\u099c\u09b0\u09be\u09a4\u09bf- \u09a4\u09cb \u09a8\u09be\u0987\u09b0\u09cb\u09ac\u09c0\u09b0- \u0985\u09b2\u09bf\u09a4\u09c7 \u0997\u09b2\u09bf\u09a4\u09c7 \u09b6\u09cb\u09a8\u09be \u09af\u09be\u09af\u09bc \u0986\u09b0 \u09ad\u09be\u09b0\u09a4\u09c0\u09af\u09bc- \u09b6\u09be\u09a1\u09bc\u09bf , \u09b2\u09c7\u09b9\u09c7\u0999\u09cd\u0997\u09be \u0987\u09a4\u09cd\u09af\u09be\u09a6\u09bf \u098f\u0996\u09be\u09a8\u09c7 \u0985\u09a4\u09cd\u09af\u09a8\u09cd\u09a4 \u09aa\u09b0\u09bf\u099a\u09bf\u09a4 
p569
aV\u09a8\u09be\u0987\u09b0\u09cb\u09ac\u09c0\u09b0- \u09ac\u09cd\u09af\u09be\u09aa\u09be\u09b0\u09c7 \u098f\u0995\u099f\u09c1 \u09b8\u09be\u09ac\u09a7\u09be\u09a8 \u09b9\u09a4\u09c7\u0993 \u09ac\u09b2\u09be \u09b9\u09af\u09bc 
p570
aV\u09a8\u09be\u0987\u09b0\u09cb\u09ac\u09c0\u09a4\u09c7- \u099a\u09c1\u09b0\u09bf , \u099b\u09bf\u09a8\u09a4\u09be\u0987 , \u09a0\u0995\u09be\u09a8\u09cb \u0993 \u09b2\u09c1\u099f\u09aa\u09be\u099f \u0987\u09a4\u09cd\u09af\u09be\u09a6\u09bf\u09b0 \u09ac\u09b9\u09c1 \u0998\u099f\u09a8\u09be \u0998\u099f\u09c7 \u09a5\u09be\u0995\u09c7 
p571
aV\u0985\u09a4\u0983\u09aa\u09b0 \u09aa\u09b0\u09cd\u09af\u099f\u0995\u09a6\u09c7\u09b0 \u098f\u0995\u09b2\u09be \u0995\u09cb\u09a8\u09cb \u0985\u099a\u09c7\u09a8\u09be \u09b8\u09cd\u09a5\u09be\u09a8\u09c7 \u09af\u09be\u0993\u09af\u09bc\u09be \u09a5\u09c7\u0995\u09c7 \u09ac\u09bf\u09b0\u09a4 \u09a5\u09be\u0995\u09be \u0989\u099a\u09bf\u09a4 
p572
aV\u09a8\u09be\u0987\u09b0\u09cb\u09ac\u09c0\u09b0- \u09b8\u09ac\u09a5\u09c7\u0995\u09c7 \u09ac\u09a1\u09bc \u09ac\u09c8\u09b6\u09bf\u09b7\u09cd\u099f\u09cd\u09af \u09b9\u09b2 \u098f\u0996\u09be\u09a8\u0995\u09be\u09b0 \u09b0\u09be\u09b7\u09cd\u099f\u09cd\u09b0\u09c0\u09af\u09bc \u09aa\u09be\u09b0\u09cd\u0995 \u09af\u09c7\u099f\u09bf \u09ae\u09cb\u099f\u09be\u09ae\u09c1\u099f\u09bf \u09a6\u0995\u09cd\u09b7\u09bf\u09a3\u09c7- \u09b6\u09c1\u09a7\u09c1 \u09ed \u0995\u09bf\u09b2\u09cb\u09ae\u09bf\u099f\u09be\u09b0 \u09a6\u09c2\u09b0\u09a4\u09cd\u09ac\u09c7 \u09b8\u09cd\u09a5\u09bf\u09a4 \u09b0\u09af\u09bc\u09c7\u099b\u09c7 
p573
aV\u09ac\u09be\u09b8\u09cd\u09a4\u09ac\u09c7 , \u09af\u0996\u09a8 \u09a8\u09be\u0987\u09b0\u09cb\u09ac\u09c0- \u098f\u09af\u09bc\u09be\u09b0\u09aa\u09cb\u09b0\u09cd\u099f\u09c7 \u09ac\u09bf\u09ae\u09be\u09a8 \u09a8\u09be\u09ae\u09ac\u09c7\u09a8 \u09a4\u0996\u09a8 \u09af\u09a6\u09bf \u09af\u09be\u09a4\u09cd\u09b0\u09c0 \u099c\u09be\u09a8\u09be\u09b2\u09be \u09a6\u09bf\u09af\u09bc\u09c7 \u09ac\u09be\u0987\u09b0\u09c7 \u09a4\u09be\u0995\u09be\u09a8 , \u0995\u09cb\u09a8\u09cb \u099c\u09bf\u09b0\u09be\u09ab\u0995\u09c7 \u09a4\u09bf\u09a8\u09bf \u09a6\u09cc\u09a1\u09bc\u09c7 \u099a\u09b2\u09c7 \u09af\u09c7\u09a4\u09c7 \u09a6\u09c7\u0996\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7\u09a8 
p574
aV\u09a8\u09be\u0987\u09b0\u09cb\u09ac\u09c0- \u09b9\u09b2 \u098f\u0995\u099f\u09bf \u09aa\u09cd\u09b0\u09a7\u09be\u09a8 \u09b0\u09be\u09b7\u09cd\u099f\u09cd\u09b0\u09c0\u09af\u09bc \u09aa\u09be\u09b0\u09cd\u0995 
p575
aV\u098f\u0987 \u09b0\u09be\u09b7\u09cd\u099f\u09cd\u09b0\u09c0\u09af\u09bc \u09aa\u09be\u09b0\u09cd\u0995 \u09b9\u09b2 \u0995\u09c7\u09a8\u09bf\u09af\u09bc\u09be\u09b0- \u09aa\u09cd\u09b0\u09a7\u09be\u09a8 \u09aa\u09b0\u09cd\u09af\u099f\u09a8 \u09b8\u09cd\u09a5\u09b2 \u09af\u09be \u098f\u0987 \u09b8\u09ac \u0985\u09b8\u09be\u09a7\u09be\u09b0\u09a3 \u09aa\u09b6\u09c1\u09a6\u09c7\u09b0 \u0989\u09aa\u09b0 \u09a8\u09bf\u09b0\u09cd\u09ad\u09b0 \u0995\u09b0\u09c7 \u0986\u09b0 \u098f\u0987 \u09b8\u09ac \u09aa\u09b6\u09c1 \u09b9\u09b2 \u098f\u09ae\u09a8 \u09af\u09be\u09b0\u09be \u0995\u09c7\u09ac\u09b2 \u0986\u09ab\u09cd\u09b0\u09bf\u0995\u09be\u09a4\u09c7\u0987- \u09aa\u09be\u0993\u09af\u09bc\u09be \u09af\u09be\u09af\u09bc 
p576
aV\u09a6\u09c1\u099f\u09bf \u09aa\u09be\u0996\u09a8\u09be \u099b\u09a1\u09bc\u09be\u09a8\u09cb \u0997\u09bf\u09a6\u09cd\u09a7-\u09aa\u09be\u0996\u09bf\u09b0- \u09ae\u09a4\u09cb \u09a6\u09c7\u0996\u09a4\u09c7 \u0997\u09c3\u09a6\u09cd\u09a7-\u0995\u09c2\u099f- \u09aa\u09be\u09b9\u09be\u09a1\u09bc\u09c7 \u09ac\u09c1\u09a6\u09cd\u09a7- \u09b8\u09ae\u09cd\u09b0\u09be\u099f \u09ac\u09bf\u09ae\u09cd\u09ac\u09bf\u09b8\u09be\u09b0\u0995\u09c7- \u09ac\u09cc\u09a6\u09cd\u09a7- \u09a7\u09b0\u09cd\u09ae\u09c7 \u09a6\u09c0\u0995\u09cd\u09b7\u09be \u09aa\u09cd\u09b0\u09a6\u09be\u09a8 \u0995\u09b0\u09c7\u09a8 
p577
aV\u0997\u09c3\u09a6\u09cd\u09a7-\u0995\u09c2\u099f\u09c7\u09b0- \u09b8\u09be\u09a5\u09c7 \u09af\u09c1\u0995\u09cd\u09a4 \u09b0\u09a4\u09cd\u09a8\u0997\u09bf\u09b0\u09bf- \u09a8\u09be\u09ae\u0995 \u09aa\u09be\u09b9\u09be\u09a1\u09bc\u09c7\u09b0 \u09b6\u09bf\u0996\u09b0\u09c7 \u099c\u09be\u09aa\u09be\u09a8- \u09a6\u09cd\u09ac\u09be\u09b0\u09be \u098f\u0995 \u09b8\u09c1\u09a8\u09cd\u09a6\u09b0 \u09ac\u09bf\u09b6\u09cd\u09ac \u09b6\u09be\u09a8\u09cd\u09a4\u09bf \u09b8\u09cd\u09a4\u09c2\u09aa \u09a8\u09bf\u09b0\u09cd\u09ae\u09bf\u09a4 \u0986\u099b\u09c7 
p578
aV\u0997\u09c3\u09a6\u09cd\u09a7-\u0995\u09c2\u099f- \u09aa\u09be\u09b9\u09be\u09a1\u09bc\u09c7\u09b0 \u09a8\u09c0\u099a \u09a6\u09bf\u09af\u09bc\u09c7 \u09b6\u09bf\u0996\u09b0\u09c7 \u09aa\u09cc\u0981\u099b\u09be\u09a8\u09cb\u09b0 \u099c\u09a8\u09cd\u09af \u09ac\u09bf\u09b9\u09be\u09b0- \u09aa\u09b0\u09cd\u09af\u099f\u09a8 \u09a6\u09cd\u09ac\u09be\u09b0\u09be \u098f\u0995 \u0986\u0995\u09b0\u09cd\u09b7\u09a3\u09c0\u09af\u09bc \u09b0\u099c\u09cd\u099c\u09c1\u09aa\u09a5- \u09b8\u09c7\u09ac\u09be\u0993 \u09b8\u099e\u09cd\u099a\u09be\u09b2\u09bf\u09a4 \u09b9\u09af\u09bc 
p579
aV\u09ac\u09bf\u09ae\u09cd\u09ac\u09bf\u09b8\u09be\u09b0- \u0995\u09be\u09b0\u09be\u0997\u09be\u09b0- - \u09a8\u09bf\u099c\u09c7\u09b0 \u09ac\u09be\u09ac\u09be \u09b8\u09ae\u09cd\u09b0\u09be\u099f \u09ac\u09bf\u09ae\u09cd\u09ac\u09bf\u09b8\u09be\u09b0\u0995\u09c7- \u09b8\u09bf\u0982\u09b9\u09be\u09b8\u09a8 - \u099a\u09cd\u09af\u09c1\u09a4 \u0995\u09b0\u09c7 \u0985\u099c\u09be\u09a4\u09b6\u09a4\u09cd\u09b0\u09c1- \u098f\u0987 \u09b8\u09cd\u09a5\u09be\u09a8\u09c7 \u09ac\u09a8\u09cd\u09a6\u09c0 \u0995\u09b0\u09c7 \u09b0\u09c7\u0996\u09c7\u099b\u09bf\u09b2\u09c7\u09a8 
p580
aV\u09ae\u09a3\u09bf\u09af\u09bc\u09be\u09b0\u09ae\u09a0- - \u09aa\u09c1\u09b0\u09be\u09a4\u09a4\u09cd\u09ac \u09a6\u09cd\u09ac\u09be\u09b0\u09be \u0995\u09c2\u09aa\u09c7\u09b0 \u0986\u0995\u09be\u09b0\u09c7 \u0997\u09a0\u09bf\u09a4 \u098f\u0987 \u09ae\u09a0\u099f\u09bf\u0995\u09c7 \u09aa\u09cd\u09b0\u09be\u099a\u09c0\u09a8 \u099c\u09c8\u09a8- \u09aa\u09c2\u099c\u09be\u09b0 \u09b8\u09cd\u09a5\u09b2 \u09ae\u09a8\u09c7 \u0995\u09b0\u09be \u09b9\u09af\u09bc 
p581
aV\u09b8\u09cb\u09a8- \u09ad\u09be\u09a3\u09cd\u09a1\u09be\u09b0- - \u09aa\u09be\u09b9\u09be\u09a1\u09bc \u0995\u09c7\u099f\u09c7 \u09a4\u09c8\u09b0\u09bf \u0995\u09b0\u09be \u098f\u0987 \u0997\u09c1\u09b9\u09be\u09af\u09bc , \u09af\u09be\u0995\u09c7 \u09b8\u09cd\u09ac\u09b0\u09cd\u09a3- - \u09ad\u09be\u09a3\u09cd\u09a1\u09be\u09b0\u0993- \u09ac\u09b2\u09be \u09b9\u09af\u09bc , \u099c\u09c8\u09a8- \u09a4\u09c0\u09b0\u09cd\u09a5\u0999\u09cd\u0995\u09b0\u09c7\u09b0 \u09aa\u09cd\u09b0\u09be\u099a\u09c0\u09a8 \u09b8\u09ac \u09ae\u09c2\u09b0\u09cd\u09a4\u09bf \u09b8\u09cd\u09a5\u09be\u09aa\u09bf\u09a4 \u0986\u099b\u09c7 
p582
aV\u09b8\u09cd\u09ac\u09b0\u09cd\u09a3- - \u09ad\u09be\u09a3\u09cd\u09a1\u09be\u09b0- \u09a5\u09c7\u0995\u09c7 \u09aa\u09cd\u09b0\u09be\u09af\u09bc \u09e7.\u09ec \u0995\u09bf.\u09ae\u09bf. \u09a6\u0995\u09cd\u09b7\u09bf\u09a3 - \u09aa\u09b6\u09cd\u099a\u09bf\u09ae \u09a6\u09bf\u0995\u09c7 \u098f\u0995\u099f\u09bf \u09b8\u09cd\u09a5\u09be\u09a8 \u09b9\u09b2 \u201c \u0986\u0996\u09a1\u09bc\u09be- \u201d \u09ac\u09be \u09b0\u09a3- - \u09ad\u09c2\u09ae\u09bf- \u09a8\u09be\u09ae\u09c7 \u09aa\u09b0\u09bf\u099a\u09bf\u09a4 
p583
aV\u09ac\u09b2\u09be \u09b9\u09af\u09bc \u09af\u09c7 , \u09aa\u09cc\u09b0\u09be\u09a3\u09bf\u0995 \u09ae\u09b9\u09be\u09ad\u09be\u09b0\u09a4- \u0995\u09be\u09b2\u09c7\u09b0 \u09a6\u09c1\u0987 \u09aa\u09be\u09a4\u09cd\u09b0 \u09ad\u09c0\u09ae- \u0986\u09b0 \u099c\u09b0\u09be\u09b8\u09a8\u09cd\u09a7\u09c7\u09b0- \u09ae\u09be\u099d\u09c7 \u098f\u0987 \u0986\u0996\u09a1\u09bc\u09be\u09a4\u09c7\u0987 \u09a6\u09cd\u09ac\u09a8\u09cd\u09a6\u09cd\u09ac \u09af\u09c1\u09a6\u09cd\u09a7 \u09b9\u09af\u09bc 
p584
aV\u09ac\u09c8\u09ad\u09be\u09b0- - \u0997\u09bf\u09b0\u09bf- \u09a8\u09be\u09ae\u09c7 \u09aa\u09b0\u09bf\u099a\u09bf\u09a4 \u09aa\u09be\u09b9\u09be\u09a1\u09bc\u09c7\u09b0 \u09aa\u09be\u09a6\u09a6\u09c7\u09b6\u09c7 \u0985\u09ac\u09b8\u09cd\u09a5\u09bf\u09a4 \u0997\u09b0\u09ae \u099c\u09b2\u09c7\u09b0 \u099d\u09b0\u09cd\u09a8\u09be \u09b9\u09af\u09bc\u09c7 \u0989\u09aa\u09b0\u09c7\u09b0 \u09a6\u09bf\u0995\u09c7 \u0993\u09a0\u09be\u09b0 \u09b8\u09ae\u09af\u09bc \u09aa\u09be\u09a5\u09b0\u09c7\u09b0 \u099f\u09c1\u0995\u09b0\u09cb\u09b0 \u09a6\u09cd\u09ac\u09be\u09b0\u09be \u09a8\u09bf\u09b0\u09cd\u09ae\u09bf\u09a4 \u098f\u0995 \u09aa\u09cd\u09b0\u09be\u099a\u09c0\u09a8 \u09ac\u09c3\u09b9\u09a4\u09cd \u09b8\u09cd\u09a5\u09b2 \u09a6\u09c7\u0996\u09be \u09af\u09be\u09af\u09bc 
p585
aV\u09ac\u09b2\u09be \u09b9\u09af\u09bc \u09af\u09c7 \u098f\u0987 \u09b8\u09cd\u09a5\u09be\u09a8 \u09aa\u09cc\u09b0\u09be\u09a3\u09bf\u0995 \u09ae\u09b9\u09be\u09ad\u09be\u09b0\u09a4\u09c7\u09b0- \u09aa\u09be\u09a4\u09cd\u09b0 \u099c\u09b0\u09be\u09b8\u09a8\u09cd\u09a7\u09c7\u09b0- \u09ac\u09c8\u09a0\u0995\u0996\u09be\u09a8\u09be \u099b\u09bf\u09b2 
p586
aV\u09ae\u09be\u09a8\u09b8- \u09b0\u09be\u09b7\u09cd\u099f\u09cd\u09b0\u09c0\u09af\u09bc- \u0989\u09a6\u09cd\u09af\u09be\u09a8\u09c7\u09b0- \u2018 \u09ac\u09a8\u09b6\u09be\u09b0\u09c0- \u2019 \u0986\u09b0 \u2018 \u09ae\u09cb\u09a4\u09be\u0997\u09c1\u09a1\u09bc\u09c0\u09a4\u09c7- \u2019 \u09ac\u09bf\u09b6\u09cd\u09b0\u09be\u09ae \u0997\u09c3\u09b9 \u0986\u099b\u09c7 
p587
aV\u09ae\u09be\u09a8\u09b8- \u09b0\u09be\u09b7\u09cd\u099f\u09cd\u09b0\u09c0\u09af\u09bc- \u0989\u09a6\u09cd\u09af\u09be\u09a8\u09c7\u09b0- \u09a6\u09c3\u09b6\u09cd\u09af \u0985\u09ac\u09b2\u09cb\u0995\u09a8 \u0995\u09b0\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u098f\u0996\u09be\u09a8\u09c7 \u0995\u09cd\u09af\u09be\u09ae\u09cd\u09aa\u0993 \u09ac\u09be\u09a8\u09be\u09a8\u09cb \u09b9\u09af\u09bc\u09c7\u099b\u09c7 
p588
aV\u09aa\u09cd\u09b0\u09be\u09df \u098f\u0987 \u09ae\u09b6\u09be \u09b0\u09be\u09a4\u09c7\u09b0 \u09a6\u09bf\u0995\u09c7 \u0995\u09be\u09ae\u09a1\u09bc\u09be\u09df 
p589
aV\u09ae\u09cd\u09af\u09be\u09b2\u09c7\u09b0\u09bf\u09df\u09be \u09af\u0995\u09c3\u09a4 \u0993 \u0995\u09c7\u09a8\u09cd\u09a6\u09cd\u09b0\u09c0\u09df \u09b8\u0982\u09b8\u09cd\u09a5\u09be\u09a8\u0995\u09c7 \u09aa\u09cd\u09b0\u09ad\u09be\u09ac\u09bf\u09a4 \u0995\u09b0\u09c7 \u09ae\u09c2\u09b0\u09cd\u099b\u09be \u0993 \u0995\u0996\u09a8\u09cb \u0995\u0996\u09a8\u09cb \u09ae\u09c3\u09a4\u09cd\u09af\u09c1\u09b0 \u0995\u09be\u09b0\u09a3\u0993 \u09b9\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7 
p590
aV\u09ae\u09cd\u09af\u09be\u09b2\u09c7\u09b0\u09bf\u09df\u09be\u09b0 \u09b2\u0995\u09cd\u09b7\u09a3 \u09b9\u09b2 \u09aa\u09cd\u09b0\u09ac\u09b2 \u099c\u09cd\u09ac\u09b0 \u09b9\u0993\u09df\u09be , \u09ac\u09ae\u09bf \u09ac\u09ae\u09bf \u09aa\u09be\u0993\u09df\u09be , \u09ae\u09be\u09a5\u09be \u09ac\u09cd\u09af\u09a5\u09be , \u0996\u09c1\u09ac \u0998\u09be\u09ae \u09b9\u0993\u09df\u09be 
p591
aV\u09ae\u09cd\u09af\u09be\u09b2\u09c7\u09b0\u09bf\u09df\u09be \u09b0\u09cb\u0997 \u098f\u0995 \u09ac\u09cd\u09af\u0995\u09cd\u09a4\u09bf \u09a5\u09c7\u0995\u09c7 \u0986\u09b0\u09c7\u0995 \u09ac\u09cd\u09af\u0995\u09cd\u09a4\u09bf\u09a4\u09c7 \u098f\u09a8\u09cb\u09ab\u09bf\u09b2\u09bf\u099c\u09c7\u09b0 \u099a\u09be\u09b0 \u09aa\u09cd\u09b0\u099c\u09be\u09a4\u09bf\u09b0 \u09ae\u09b6\u09be , \u09af\u09c7\u09ae\u09a8 \u09aa\u09cd\u09b2\u09be\u09b8\u09ae\u09cb\u09a1\u09bf\u09df\u09be\u09ae \u09ab\u09be\u09b2\u09b8\u09bf\u09aa\u09c7\u09b0\u09be\u09ae , \u09aa\u09cd\u09b2\u09be\u09b8\u09ae\u09cb\u09a1\u09bf\u09df\u09be\u09ae \u09ad\u09be\u0987\u09ad\u09cd\u09af\u09be\u0995\u09cd\u09b8 , \u09aa\u09cd\u09b2\u09be\u09b8\u09ae\u09cb\u09a1\u09bf\u09df\u09be\u09ae \u0993\u09ac\u09c7\u09b2 \u0993 \u09aa\u09cd\u09b2\u09be\u09b8\u09ae\u09cb\u09a1\u09bf\u09df\u09be\u09ae \u09ae\u09cd\u09af\u09be\u09b2\u09cb\u09b0\u09c0\u09b0 \u09a6\u09cd\u09ac\u09be\u09b0\u09be \u099b\u09a1\u09bc\u09be\u09df 
p592
aV\u098f\u0987 \u09b0\u09cb\u0997 \u09b8\u09ae\u09c2\u09b9\u09c7\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 \u09ac\u09bf\u09ad\u09bf\u09a8\u09cd\u09a8 \u09aa\u09cd\u09b0\u0995\u09be\u09b0\u09c7\u09b0 \u09ae\u09cd\u09af\u09be\u09b2\u09c7\u09b0\u09bf\u09df\u09be , \u09ab\u09be\u0987\u09b2\u09c7\u09b0\u09bf\u09df\u09be , \u09a1\u09c7\u0999\u09cd\u0997\u09c1 \u098f\u09ac\u0982 \u09aa\u09c0\u09a4-\u099c\u09cd\u09ac\u09b0 \u0987\u09a4\u09cd\u09af\u09be\u09a6\u09bf \u09b8\u09ae\u09cd\u09ae\u09c0\u09b2\u09bf\u09a4 \u09b9\u09df\u09c7 \u0986\u099b\u09c7 
p593
aV\u099c\u09b2\u09c7\u09b0 \u099c\u09c0\u09ac\u09be\u09a8\u09c1\u0997\u09c1\u09b2\u09bf\u09b0 \u09a6\u09cd\u09ac\u09be\u09b0\u09be \u099b\u09a1\u09bc\u09be\u09a8\u09cb \u09b0\u09cb\u0997\u0997\u09c1\u09b2\u09bf \u09b9\u09b2 -
p594
aV\u09a8\u09be\u09b0\u09c1 \u09b0\u09cb\u0997\u09c7\u09b0 \u09b6\u09c7\u09b7 \u09b6\u09bf\u09b0\u09be\u09df \u0995\u09bf\u099b\u09c1 \u09b0\u09b8\u09be\u09df\u09a3\u09af\u09c1\u0995\u09cd\u09a4 \u099f\u09bf\u0995\u09be \u09b2\u09be\u0997\u09bf\u09df\u09c7\u0993 \u0995\u09b0\u09be \u09af\u09be\u09df 
p595
aV\u09a8\u09be\u09b0\u09c1 \u09b0\u09cb\u0997\u09c7\u09b0 \u09a8\u09bf\u09ac\u09be\u09b0\u09a3\u09c7\u09b0 \u099c\u09a8\u09cd\u09af \u09aa\u09c1\u0995\u09c1\u09b0 , \u099d\u09bf\u09b2 , \u098f\u09ac\u0982 \u0985\u09a8\u09cd\u09af \u099c\u09b2\u09be\u09b6\u09df \u0987\u09a4\u09cd\u09af\u09be\u09a6\u09bf\u09b0 \u099c\u09b2 \u099b\u09c7\u0981\u0995\u09c7\u0987 \u0995\u09be\u099c\u09c7 \u09b2\u09be\u0997\u09be\u09a8\u09cb \u0989\u099a\u09bf\u09a4 
p596
aV\u098f\u0987 \u0995\u09be\u09b0\u09a3\u09c7 \u09b0\u09cb\u0997\u09c0\u09b0 \u099c\u09cd\u09ac\u09b0 , \u09ae\u09be\u09a5\u09be \u09ac\u09cd\u09af\u09a5\u09be \u0993 \u09ac\u09ae\u09bf\u09b0 \u09b2\u0995\u09cd\u09b7\u09a3\u0993 \u09b9\u09df\u09c7 \u09af\u09be\u09df 
p597
aV\u09b6\u09b0\u09c0\u09b0\u09c7\u09b0 \u09af\u09c7 \u0985\u0982\u09b6 \u09a5\u09c7\u0995\u09c7 \u0995\u09c0\u099f \u09ac\u09be\u09b0 \u09b9\u09df , \u0993\u0996\u09be\u09a8\u09c7 \u09aa\u09cd\u09b0\u09a5\u09ae\u09c7 \u0996\u09c1\u09ac \u099c\u09cb\u09b0 \u099a\u09c1\u09b2\u0995\u09be\u09a8\u09bf \u09b9\u09df , \u09ab\u09c1\u09b8\u09cd\u0995\u09c1\u09a1\u09bc\u09bf \u09b9\u09df \u0986\u09b0 \u099b\u09bf\u09a6\u09cd\u09b0 \u09b9\u09df\u09c7 \u09af\u09be\u09df 
p598
aV\u09a8\u09c7\u09a4\u09be\u09b0\u09b9\u09be\u099f- \u09ae\u09be\u09b2\u09ad\u09c2\u09ae\u09bf\u09b0 \u0995\u09be\u099b\u09c7\u09b0 \u09aa\u09be\u09b9\u09be\u09a1\u09bc\u0995\u09c7 \u09aa\u09be\u09a4- ( \u09aa\u09be\u099f- ) \u09ac\u09b2\u09be \u09b9\u09af\u09bc - \u09a8\u09c7\u09a4\u09be\u09b0\u09b9\u09be\u099f- \u09aa\u09be\u099f- , \u09aa\u09b8\u09c7\u09b0\u09c0- \u09aa\u09be\u099f- , \u09a1\u09c1\u09ae\u09b0- \u09aa\u09be\u099f- , \u099c\u09cb\u09ad\u09c0- \u09aa\u09be\u099f- , \u099c\u09ae\u09c7\u09a1\u09c1\u09b0\u09be- \u09aa\u09be\u099f- , \u0995\u09cb\u09af\u09bc\u09c7\u09b2- \u0986\u09b0 \u09b6\u0999\u09cd\u0996 \u09a8\u09a6\u09c0 \u09b8\u09a8\u09cd\u09a7\u09bf\u09b8\u09cd\u09a5\u09b2 \u09a5\u09c7\u0995\u09c7 \u09a4\u09c8\u09b0\u09bf \u09b9\u09af\u09bc\u09c7\u099b\u09c7 
p599
aV\u09ae\u09cd\u09af\u09be\u099f \u0986\u09b0 \u0995\u09df\u09c7\u09b2\u0993 \u09ac\u09be\u099a\u09cd\u099a\u09be\u09a6\u09c7\u09b0 \u09aa\u0995\u09cd\u09b7\u09c7 \u0995\u09cd\u09b7\u09a4\u09bf\u0995\u09be\u09b0\u0995 \u09b9\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7 
p600
aV\u09ac\u09be\u099a\u09cd\u099a\u09be\u09a6\u09c7\u09b0 \u09ae\u09b6\u09be\u09b0\u09bf\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 \u09b6\u09cb\u09df\u09be\u09a8\u09cb\u0987 \u09ad\u09be\u09b2\u09cb 
p601
aV\u09aa\u09cd\u09b0\u09b8\u09ac\u09c7\u09b0 \u09aa\u09b0 \u098f\u0995 \u09ae\u09be\u09b8\u09c7\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 \u09af\u09a6\u09bf \u09ae\u09be-\u098f\u09b0 \u099c\u09cd\u09ac\u09b0 \u09b9\u09df \u09ac\u09be \u09ac\u09be\u09b0\u09ac\u09be\u09b0 \u099c\u09cd\u09ac\u09b0 \u099b\u09c7\u09dc\u09c7 \u09af\u09be\u09df , \u09ac\u09c7\u09dc\u09c7 \u09af\u09be\u09df \u09ac\u09be \u09a4\u09be\u09b0 \u09b0\u0995\u09cd\u09a4\u0995\u09cd\u09b7\u09b0\u09a8 \u09af\u09a6\u09bf \u0986\u09ac\u09be\u09b0 \u09ac\u09a8\u09cd\u09a7 \u09a8\u09be \u09b9\u09df \u09a4\u09be\u09b9\u09b2\u09c7 \u09b8\u09ae\u09df \u09a8\u09b7\u09cd\u099f \u09a8\u09be \u0995\u09b0\u09c7 \u09a4\u09be\u0995\u09c7 \u09b6\u09c0\u0998\u09cd\u09b0\u0987 \u09ac\u09dc \u09b9\u09be\u09b8\u09aa\u09be\u09a4\u09be\u09b2\u09c7 \u09a8\u09bf\u09df\u09c7 \u09af\u09be\u0993\u09df\u09be \u0989\u099a\u09bf\u09a4 
p602
aV\u09a6\u09c1\u0983\u09b8\u09be\u09b9\u09b8\u09bf\u0995 \u09af\u09be\u09a4\u09cd\u09b0\u09be \u09b9\u09b2 \u09aa\u09b0\u09bf\u09ac\u09c7\u09b6 \u09aa\u09b0\u09cd\u09af\u099f\u09a8\u09c7\u09b0 \u098f\u0995\u099f\u09be \u0985\u09ad\u09bf\u09a8\u09cd\u09a8 \u0985\u0999\u09cd\u0997 \u098f\u09ac\u0982 \u0997\u09a4 \u09a6\u09c1\u0987 \u09a6\u09b6\u0995 \u09a7\u09b0\u09c7\u0987 \u098f\u099f\u09be \u09a6\u09c1\u0983\u09b8\u09be\u09b9\u09b8\u09bf\u0995 \u09af\u09be\u09a4\u09cd\u09b0\u09be\u09b0 \u0985\u09ad\u09bf\u09af\u09be\u09a8\u09c7 \u0995\u09c1\u09a1\u09bc\u09bf \u0997\u09c1\u09a3 \u09ac\u09c3\u09a6\u09cd\u09a7\u09bf \u09b9\u09af\u09bc\u09c7\u099b\u09c7 
p603
aV\u098f\u0987 \u09a6\u09c1\u0983\u09b8\u09be\u09b9\u09b8\u09bf\u0995 \u09af\u09be\u09a4\u09cd\u09b0\u09be\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 \u09b9\u09bf\u09ae\u09be\u09b2\u09af\u09bc , \u09b8\u09ae\u09a4\u09b2 \u09ac\u09bf\u09af\u09bc\u09be\u09ac\u09be\u09a8 , \u09a8\u09a6\u09c0\u09b0 \u09a4\u099f , \u09ae\u09b0\u09c2\u09ad\u09c2\u09ae\u09bf \u0985\u099e\u09cd\u099a\u09b2 , \u09b8\u09ae\u09c1\u09a6\u09cd\u09b0 \u098f\u09ac\u0982 \u09b8\u09ae\u09c1\u09a6\u09cd\u09b0 \u09a4\u099f \u09aa\u09a1\u09bc\u09c7 
p604
aV\u098f\u0987 \u09b8\u09ae\u09b8\u09cd\u09a4 \u0995\u09bf\u099b\u09c1 \u09aa\u09b0\u09cd\u09af\u099f\u0995 \u09ad\u09cd\u09b0\u09ae\u09a3\u09c7\u09b0 \u099c\u09a8\u09cd\u09af \u09ac\u09cd\u09af\u09aa\u0995\u09ad\u09be\u09ac\u09c7 \u0995\u09be\u099c\u09c7 \u09b2\u09be\u0997\u09be\u09a8\u09cb \u09b9\u09af\u09bc 
p605
aV\u09a4\u09be\u09a6\u09c7\u09b0 \u09aa\u09cd\u09b0\u09a4\u09bf\u099f\u09bf \u0990\u09a4\u09bf\u09b9\u09be\u09b8\u09bf\u0995 \u0987\u09ae\u09be\u09b0\u09a4 \u09b8\u09ae\u09cd\u09ac\u09a8\u09cd\u09a7\u09c7 \u0997\u09c1\u09b0\u09c1\u09a4\u09cd\u09ac\u09aa\u09c2\u09b0\u09cd\u09a3 \u09a4\u09a5\u09cd\u09af \u09a6\u09c7\u0993\u09af\u09bc\u09be \u0989\u099a\u09bf\u09a4 
p606
aV\u098f\u09a6\u09c7\u09b0 \u09aa\u09b0\u09cd\u09af\u09be\u09aa\u09cd\u09a4\u09ad\u09be\u09ac\u09c7 \u09b8\u0982\u09b0\u0995\u09cd\u09b7\u09bf\u09a4 \u0995\u09b0\u09c7 \u098f\u09a6\u09c7\u09b0 \u09a6\u09c7\u0996\u09be\u09b6\u09cb\u09a8\u09be\u09b0 \u09aa\u09cd\u09b0\u09a4\u09bf \u09ac\u09bf\u09b6\u09c7\u09b7 \u09ae\u09a8\u09c7\u09af\u09cb\u0997 \u09a6\u09c7\u0993\u09af\u09bc\u09be \u0989\u099a\u09bf\u09a4 
p607
aV\u09aa\u09cd\u09b0\u09be\u099a\u09c0\u09a8 \u0987\u09ae\u09be\u09b0\u09a4 , \u09aa\u09c1\u09b0\u09be\u09a8\u09cb \u09ae\u09a8\u09cd\u09a6\u09bf\u09b0 , \u09b9\u09be\u09ad\u09c7\u09b2\u09bf , \u09b0\u09be\u099c\u09ae\u09b9\u09b2 , \u0995\u09c7\u09b2\u09cd\u09b2\u09be \u098f\u09ac\u0982 \u0990\u09a4\u09bf\u09b9\u09be\u09b8\u09bf\u0995 \u09af\u09c1\u09a6\u09cd\u09a7\u09c7\u09b0 \u09ae\u09be\u09a0 \u09a6\u09c7\u0996\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u09aa\u09b0\u09cd\u09af\u099f\u0995\u09b0\u09be \u09a8\u09bf\u099c\u09c7\u09a6\u09c7\u09b0 \u09ae\u09a8\u09c7 \u09b8\u099a\u09c7\u09a4\u09a8\u09be \u0993 \u0989\u09a4\u09cd\u09b8\u09be\u09b9 \u09a8\u09bf\u09af\u09bc\u09c7 \u0986\u09b8\u09c7 
p608
aV\u09a6\u09c7\u09b6 - \u09ac\u09bf\u09a6\u09c7\u09b6 \u09a5\u09c7\u0995\u09c7 \u09aa\u09b0\u09cd\u09af\u099f\u0995\u09a6\u09c7\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 \u09aa\u09cd\u09b0\u09be\u099a\u09c0\u09a8 \u0997\u09c1\u09b0\u09c1\u09a4\u09cd\u09ac\u09b8\u09ae\u09cd\u09aa\u09a8\u09cd\u09a8 \u0990\u09a4\u09bf\u09b9\u09be\u09b8\u09bf\u0995 \u0987\u09ae\u09be\u09b0\u09a4 , \u09aa\u09cd\u09b0\u09be\u099a\u09c0\u09a8 \u09ae\u09a8\u09cd\u09a6\u09bf\u09b0 , \u09b8\u09cd\u09a4\u09c1\u09aa \u0993 \u0997\u09ae\u09cd\u09ac\u09c1\u099c \u09ad\u09cd\u09b0\u09ae\u09a3\u09c7\u09b0 \u09b6\u0996 \u0995\u09cd\u09b0\u09ae\u09be\u0997\u09a4 \u09ac\u09be\u09a1\u09bc\u099b\u09c7 
p609
aV\u0986\u09ae\u09be\u09a6\u09c7\u09b0 \u09a6\u09c7\u09b6 \u09b9\u09b2 \u09aa\u09cd\u09b0\u09be\u099a\u09c0\u09a8 \u09ad\u09be\u09b0\u09a4\u09c0\u09af\u09bc \u09b8\u09ad\u09cd\u09af\u09a4\u09be\u09b0 \u0985\u09ac\u09b6\u09bf\u09b7\u09cd\u099f\u09be\u0982\u09b6 , \u09ae\u09c2\u09b0\u09cd\u09a4\u09bf \u0993 \u0990\u09a4\u09bf\u09b9\u09be\u09b8\u09bf\u0995 \u0987\u09ae\u09be\u09b0\u09a4\u09c7\u09b0 \u098f\u0995 \u099c\u09c0\u09ac\u09a8\u09cd\u09a4 \u09ac\u09bf\u09b6\u09be\u09b2 \u09b8\u0982\u0997\u09cd\u09b0\u09b9\u09be\u09b2\u09af\u09bc 
p610
aV\u0986\u099c\u0993 \u09a6\u09c7\u09b6 \u09ac\u09bf\u09a6\u09c7\u09b6\u09c7 \u09a8\u09be\u0997\u09b0\u09bf\u0995\u09a6\u09c7\u09b0 \u098f\u0987 \u09ae\u09c7\u09b2\u09be\u09b0 \u09aa\u09cd\u09b0\u09a4\u09bf \u098f\u0995\u099f\u09be \u09ac\u09bf\u09b6\u09c7\u09b7 \u0986\u0995\u09b0\u09cd\u09b7\u09a3 \u0993 \u0997\u09ad\u09c0\u09b0 \u099f\u09be\u09a8 \u0986\u099b\u09c7 
p611
aV\u09a4\u09be\u0987 \u098f\u09a6\u09c7\u09b0 \u099c\u09a8\u09aa\u09cd\u09b0\u09bf\u09af\u09bc\u09a4\u09be \u09a6\u09c7\u0996\u09c7 \u098f\u09a6\u09c7\u09b0 \u09ac\u09be\u09a1\u09bc\u09bf\u09af\u09bc\u09c7 \u09a4\u09cb\u09b2\u09be \u09af\u09c7\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7 
p612
aV\u098f\u099f\u09be \u0996\u09be\u09a6\u09cd\u09af \u09b9\u099c\u09ae\u09c7\u09b0 \u099c\u09a8\u09cd\u09af \u0985\u09cd\u09af\u09be\u09b8\u09bf\u09a1 \u09a4\u09c8\u09b0\u09bf\u09a4\u09c7 \u09b8\u09be\u09b9\u09be\u09af\u09cd\u09af \u0995\u09b0\u09c7 
p613
aV\u099c\u09c7\u09a8\u09c7 \u09ae\u09be\u09a8\u09bf \u09af\u09c7 \u098f\u09b0 \u09a6\u09cd\u09ac\u09be\u09b0\u09be \u09b8\u09cd\u09ac\u09be\u09a6 \u0997\u09cd\u09b0\u09a8\u09cd\u09a5\u09bf \u0989\u09a6\u09cd\u09a6\u09c0\u09aa\u09a4 \u09b9\u09df 
p614
aV\u0997\u09cb\u09b2\u09ae\u09b0\u09bf\u099a \u09a5\u09c7\u0995\u09c7 \u09aa\u09be\u099a\u09a8\u09a4\u09a8\u09cd\u09a4\u09cd\u09b0 \u0989\u09aa\u0995\u09be\u09b0 \u09b2\u09be\u09ad \u0995\u09b0\u09c7 
p615
aV\u0996\u09be\u09a6\u09cd\u09af\u09c7 \u098f\u099f\u09be \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0 \u0995\u09b0\u09c7 \u098f\u09ae\u09a8 \u09ac\u09b9\u09c1 \u09b2\u09cb\u0995 \u098f\u09b0 \u09b8\u09cd\u09ac\u09be\u09b8\u09cd\u09a5\u09cd\u09af\u0995\u09b0 \u09a6\u09bf\u0995\u099f\u09be\u09b0 \u0995\u09a5\u09be \u099c\u09be\u09a8\u09c7\u09a8 \u09a8\u09be 
p616
aV\u0986\u09df\u09c1\u09b0\u09cd\u09ac\u09c7\u09a6\u09c7 \u0997\u09cb\u09b2\u09ae\u09b0\u09bf\u099a\u0995\u09c7 \u0997\u09c1\u09b0\u09c1\u09a4\u09cd\u09ac\u09aa\u09c2\u09b0\u09cd\u09a3 \u0985\u09cd\u09af\u09be\u09a8\u09cd\u099f\u09bf\u0993\u0995\u09cd\u09b8\u09bf\u09a1\u09c7\u09a8\u09cd\u099f \u0986\u09b0 \u0985\u09cd\u09af\u09be\u09a8\u09cd\u099f\u09bf\u09ac\u09cd\u09af\u0995\u099f\u09c7\u09b0\u09bf\u09df\u09be\u09b2 \u09ae\u09be\u09a8\u09be \u09b9\u09df 
p617
aV\u09ae\u09a7\u09cd\u09af\u0995\u09be\u09b2\u09c7 \u09ae\u09be\u09a8\u09be \u09b9\u09a4 \u09af\u09c7 \u09af\u09be\u09b0 \u0995\u09be\u099b\u09c7 \u0997\u09cb\u09b2\u09ae\u09b0\u09bf\u099a \u09af\u09a4 \u09ac\u09c7\u09b6\u09bf \u09a5\u09be\u0995\u09ac\u09c7 \u09b8\u09c7 \u09a4\u09a4 \u09ac\u09c7\u09b6\u09bf \u09a7\u09a8\u09ac\u09be\u09a8 \u09b9\u09ac\u09c7 
p618
aV\u0986\u0997\u09c7\u0995\u09be\u09b0 \u09a6\u09bf\u09a8\u09c7 \u0997\u09cd\u09b0\u09c0\u09b8\u09c7- \u0997\u09cb\u09b2\u09ae\u09b0\u09bf\u099a\u0995\u09c7 \u09ad\u0997\u09ac\u09be\u09a8\u09c7\u09b0 \u09a8\u09c8\u09ac\u09c7\u09a6\u09cd\u09af\u09c7 \u09a6\u09c7\u0993\u09df\u09be\u09b0 \u09b8\u09be\u09a5\u09c7 \u09b8\u09be\u09a5\u09c7 \u099f\u09cd\u09af\u09be\u0995\u09cd\u09b8 \u0986\u09b0 \u09aa\u09a3 \u09aa\u09b0\u09cd\u09af\u09a8\u09cd\u09a4 \u09a6\u09c7\u0993\u09df\u09be \u09b9\u09a4 
p619
aV\u0997\u09cb\u09b2\u09ae\u09b0\u09bf\u099a\u09c7\u09b0 \u09a8\u09be\u09ae \u0987\u09a4\u09bf\u09b9\u09be\u09b8\u09c7\u0993 \u09aa\u09be\u0993\u09df\u09be \u09af\u09be\u09df 
p620
aV\u099b\u09cb\u099f \u0997\u09cb\u09b2\u09ae\u09b0\u09bf\u099a\u09c7\u09b0 \u0996\u09c1\u09ac \u0989\u09aa\u0995\u09be\u09b0\u09bf\u09a4\u09be \u0986\u099b\u09c7 
p621
aV\u098f\u0995 \u09a6\u09bf\u0995 \u09a6\u09bf\u09df\u09c7 \u098f\u099f\u09be \u09aa\u09be\u099a\u09a8\u09a4\u09a8\u09cd\u09a4\u09cd\u09b0\u0995\u09c7 \u09b6\u0995\u09cd\u09a4\u09bf\u09b6\u09be\u09b2\u09c0 \u0995\u09b0\u09c7 
p622
aV\u098f\u0996\u09a8\u0993 \u09aa\u09b0\u09cd\u09af\u09a8\u09cd\u09a4 \u09b0\u09c7\u09b2 \u09aa\u09be\u09b8 \u09a8\u09c7\u0993\u09af\u09bc\u09be\u0987 \u09b8\u09ac\u09a5\u09c7\u0995\u09c7 \u09ad\u09be\u09b2\u09cb , \u098f\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 \u09ac\u09be\u09b8 \u0986\u09b0 \u09a8\u09cc\u0995\u09be\u09b0 \u09af\u09be\u09a4\u09cd\u09b0\u09be\u0993 \u09af\u09c1\u0995\u09cd\u09a4 \u09a5\u09be\u0995\u09c7 
p623
aV\u09b6\u09c1\u09a7\u09c1 \u099d\u09bf\u09b2 \u0986\u099b\u09c7 \u09af\u09be \u09e9 \u0995\u09bf\u09b2\u09cb\u09ae\u09bf\u099f\u09be\u09b0\u09c7\u09b0 \u0998\u09c7\u09b0\u09be\u099f\u09cb\u09aa\u09c7 \u09b6\u09be\u09a8\u09cd\u09a4 \u0986\u09b0 \u09b8\u09cd\u09a4\u09ac\u09cd\u09a7 \u09a6\u09c7\u0996\u09be\u09af\u09bc 
p624
aV\u09aa\u09c1\u09b0\u09cb \u099a\u09be\u0981\u09a6\u09c7\u09b0 \u09a8\u09bf\u099a\u09c7 \u09af\u0996\u09a8 \u099d\u09bf\u09b2 \u09b9\u09be\u09b2\u09cd\u0995\u09be \u09a2\u09c7\u0989 \u09a4\u09cb\u09b2\u09c7 \u09a4\u0996\u09a8 \u09ae\u09a8\u09c7 \u09b9\u09af\u09bc \u099a\u09be\u0981\u09a6\u09c7\u09b0 \u0986\u09b2\u09cb \u09a8\u09b0\u09cd\u09a4\u0995\u09c0 \u09b0\u09c2\u09aa\u09c7 \u0986\u09b8\u09cd\u09a4\u09c7 \u0986\u09b8\u09cd\u09a4\u09c7 \u09a8\u09be\u099a\u099b\u09c7 
p625
aV\u09b2\u09c7\u0996\u0995 \u09b8\u09cd\u09ac\u09af\u09bc\u0982 \u0995\u09c7\u09b2\u09be\u0999\u09cd\u0997\u09c7- \u09a8\u09bf\u099c\u09c7\u09b0 \u09a4\u09be\u0981\u09ac\u09c1 \u09a4\u09c8\u09b0\u09bf \u0995\u09b0\u099b\u09c7 
p626
aV\u098f\u0987 \u09b8\u09cd\u09a5\u09be\u09a8 \u09aa\u09a6\u09cd\u09ae\u09b8\u09ae\u09cd\u09ad\u09ac\u09c7\u09b0- \u0995\u09a5\u09be \u09ae\u09a8\u09c7 \u0995\u09b0\u09be\u09af\u09bc \u09af\u09bf\u09a8\u09bf \u098f\u0996\u09be\u09a8\u09c7 \u098f\u09a8\u09be\u09b0 \u0986\u09a7\u09be\u09b0\u09b6\u09bf\u09b2\u09be \u09b0\u09c7\u0996\u09c7 \u09af\u09be\u09a8 
p627
aV\u0995\u09cb\u09b0\u09a6\u0999\u09cd\u0997- \u0997\u09c1\u09b9\u09be\u09b0 \u09aa\u09c1\u09b8\u09cd\u09a4\u0995\u09be\u09b2\u09af\u09bc \u0986\u09b0 \u099a\u09bf\u09a4\u09cd\u09b0 \u09b8\u0982\u0997\u09cd\u09b0\u09b9\u09be\u09b2\u09af\u09bc \u0985\u09ac\u09be\u0995 \u0995\u09b0\u09c7 
p628
aV\u0995\u09c7\u09b2\u09be\u0999\u09cd\u0997- \u09b2\u09be\u09b9\u09c1\u09b2- \u09b8\u09cd\u09aa\u09bf\u09a4\u09bf\u09b0- \u09b8\u09ac\u09a5\u09c7\u0995\u09c7 \u0985\u09b8\u09be\u09a7\u09be\u09b0\u09a3 \u0986\u09b0 \u09b0\u09b9\u09b8\u09cd\u09af\u09ae\u09af\u09bc \u099c\u09be\u09af\u09bc\u0997\u09be \u09af\u09c7\u0996\u09be\u09a8\u09c7 \u09a8\u09be \u0986\u099b\u09c7 \u0995\u09cb\u09a8\u09cb \u099c\u09a8\u09ac\u09b8\u09a4\u09bf \u09a8\u09be \u0995\u09cb\u09a8\u09cb \u09a6\u09cb\u0995\u09be\u09a8\u09aa\u09be\u099f 
p629
aV\u09aa\u09b0\u09cd\u09af\u099f\u0995\u09a6\u09c7\u09b0 \u09b8\u09ac\u09a5\u09c7\u0995\u09c7 \u09aa\u099b\u09a8\u09cd\u09a6\u09c7\u09b0 \u0995\u09be\u099c \u09b9\u09b2 \u0993\u0996\u09be\u09a8\u09c7 \u09a6\u09be\u0981\u09a1\u09bc\u09bf\u09af\u09bc\u09c7 \u09a5\u09be\u0995\u09be \u09af\u09be\u09a4\u09c7 \u09b9\u09be\u0993\u09af\u09bc\u09be\u09b0 \u09b8\u09be\u09a5\u09c7 \u09aa\u09a1\u09bc\u09be \u099c\u09b2\u09c7\u09b0 \u0995\u09bf\u099b\u09c1 \u09ab\u09cb\u0981\u099f\u09be \u09a4\u09be\u09a6\u09c7\u09b0 \u09ae\u09c1\u0996 \u09b8\u09cd\u09aa\u09b0\u09cd\u09b6 \u0995\u09b0\u09c7 \u09af\u09c7\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7 
p630
aV\u099c\u09c1\u09a8- \u09a5\u09c7\u0995\u09c7 \u0985\u0995\u09cd\u099f\u09cb\u09ac\u09b0- \u09aa\u09b0\u09cd\u09af\u09a8\u09cd\u09a4 \u09ae\u09be\u09a8\u09be\u09b2\u09bf- \u09a5\u09c7\u0995\u09c7 \u09aa\u09cd\u09b0\u099a\u09c1\u09b0 \u09b2\u09cb\u0995 \u098f\u0996\u09be\u09a8\u09c7 \u09aa\u09cc\u0981\u099b\u09cb\u09af\u09bc 
p631
aV\u09aa\u09cd\u09b0\u09a7\u09be\u09a8 \u099c\u09be\u09af\u09bc\u0997\u09be \u09ac\u09be \u09b0\u09be\u09b8\u09cd\u09a4\u09be \u09a5\u09c7\u0995\u09c7 \u09e7\u09e7 \u0995\u09bf\u09b2\u09cb\u09ae\u09bf\u099f\u09be\u09b0 \u09af\u09be\u09a8\u09ac\u09be\u09b9\u09a8\u09c7\u09b0 \u09b8\u09be\u09b9\u09be\u09af\u09cd\u09af\u09c7 \u0986\u09b0\u0993 \u09e9 \u0995\u09bf\u09b2\u09cb\u09ae\u09bf\u099f\u09be\u09b0 \u09b9\u09c7\u0981\u099f\u09c7 \u099a\u09a8\u09cd\u09a6\u09cd\u09b0\u09a4\u09be\u09b2- \u09aa\u09cc\u0981\u099b\u09be\u09a8\u09cb \u09af\u09be\u09af\u09bc 
p632
aV\u098f\u099f\u09be \u0995\u09cb\u09a8\u0993 \u0996\u09be\u09b0\u09be\u09aa \u09ac\u09bf\u0995\u09b2\u09cd\u09aa \u09a8\u09af\u09bc , \u0995\u09be\u09b0\u09a3 \u098f\u09b0 \u09aa\u09b0 \u0986\u09aa\u09a8\u09bf \u09b8\u09c1\u0987\u099c\u09be\u09b0\u09b2\u09cd\u09af\u09be\u09a8\u09cd\u09a1\u09c7- \u09af\u09c7\u0995\u09cb\u09a8\u0993 \u099c\u09be\u09af\u09bc\u0997\u09be\u09af\u09bc \u09ad\u09cd\u09b0\u09ae\u09a3 \u0995\u09b0\u09a4\u09c7 \u09aa\u09be\u09b0\u09ac\u09c7\u09a8 
p633
aV\u098f\u0995\u09a6\u09bf\u09a8\u09c7\u09b0 \u09aa\u09be\u09b8\u09c7\u09b0 \u09ae\u09c2\u09b2\u09cd\u09af \u09aa\u09cd\u09b0\u09be\u09af\u09bc \u09e9\u09eb - \u09ea\u09e6 \u09a1\u09b2\u09be\u09b0 \u09b9\u09af\u09bc 
p634
aV\u09b6\u09bf\u09ac\u0997\u0999\u09cd\u0997\u09be- \u09b6\u09bf\u0996\u09b0\u09c7 ` \u09b9\u09cb\u09a8\u09c0- \u09a6\u09c7\u09ac\u09c0- ` \u098f\u09ac\u0982 ` \u0997\u0999\u09cd\u0997\u09be\u09a7\u09b0\u09c7\u09b6\u09cd\u09ac\u09b0- \u0997\u09c1\u09b9\u09be- ` \u09ae\u09a8\u09cd\u09a6\u09bf\u09b0\u0993 \u09b9\u09b2 \u09a6\u09c7\u0996\u09be\u09b0 \u09ae\u09a4\u09cb 
p635
aV\u09ac\u09cd\u09af\u09be\u0999\u09cd\u0997\u09be\u09b2\u09cb\u09b0- \u09a5\u09c7\u0995\u09c7 \u09eb\u09ec \u0995\u09bf\u09b2\u09cb\u09ae\u09bf\u099f\u09be\u09b0 \u09a6\u09c2\u09b0\u09c7 ` \u09a4\u09c1\u09ae\u0995\u09c1\u09b0- \u09b0\u09cb\u09a1\u09c7 ` \u09e7\u09e9\u09ec\u09ed \u09ae\u09bf\u099f\u09be\u09b0 \u0989\u0981\u099a\u09c1 \u09b6\u09bf\u09ac\u0997\u0999\u09cd\u0997\u09be- \u09aa\u09be\u09b9\u09be\u09a1\u09bc \u09a5\u09c7\u0995\u09c7 \u09a8\u09bf\u099a\u09c7\u09b0 \u09a6\u09c3\u09b6\u09cd\u09af \u09b8\u09c1\u09a8\u09cd\u09a6\u09b0 \u09a6\u09c7\u0996\u09a4\u09c7 \u09aa\u09be\u0993\u09af\u09bc\u09be \u09af\u09be\u09af\u09bc 
p636
aV\u09ac\u09bf\u09b6\u09cd\u09ac\u09b6\u09cd\u09ac\u09b0\u09af\u09bc\u09cd\u09af\u09be- \u09ae\u09bf\u0989\u099c\u09bf\u09af\u09bc\u09be\u09ae\u09c7- \u09a6\u09c7\u09b6\u09c7\u09b0 \u09b6\u09bf\u09b2\u09cd\u09aa\u09b8\u0982\u0995\u09cd\u09b0\u09be\u09a8\u09cd\u09a4 \u098f\u09ac\u0982 \u09aa\u09cd\u09b0\u09be\u09af\u09bc\u09cb\u0997\u09bf\u0995 \u09aa\u09a6\u09cd\u09a7\u09a4\u09bf\u09b0 \u099d\u09b2\u0995 \u09a6\u09c7\u0996\u09be\u09a8\u09cb \u09b9\u09af\u09bc\u09c7\u099b\u09c7 
p637
aV\u09ac\u09bf\u09b6\u09cd\u09ac\u09b6\u09cd\u09ac\u09b0\u09af\u09bc\u09cd\u09af\u09be- \u09ae\u09bf\u0989\u099c\u09bf\u09af\u09bc\u09be\u09ae- \u09ae\u09b9\u09be\u09a8 \u09aa\u09cd\u09b0\u0995\u09cc\u09b6\u09b2\u09c0 \u098f\u09ac\u0982 \u0986\u09a7\u09c1\u09a3\u09bf\u0995 \u0995\u09b0\u09cd\u09a3\u09be\u099f\u0995\u09c7\u09b0 \u09a8\u09bf\u09b0\u09cd\u09ae\u09be\u09a4\u09be \u09ac\u09bf\u09b6\u09cd\u09ac\u09b6\u09cd\u09ac\u09b0\u09af\u09bc\u09cd\u09af\u09be\u09b0- \u09b8\u09cd\u09ae\u09c3\u09a4\u09bf\u09a4\u09c7 \u09a8\u09bf\u09b0\u09cd\u09ae\u09bf\u09a4 \u0995\u09b0\u09be \u09b9\u09af\u09bc\u09c7\u099b\u09c7 
p638
aV\u09af\u09cb\u0997\u09a8\u09b0\u09b8\u09bf\u09ae\u09cd\u09b9\u09be- \u0993 \u09ad\u09cb\u0997\u09a8\u09b0\u09b8\u09bf\u09ae\u09cd\u09b9\u09be- \u09ae\u09a8\u09cd\u09a6\u09bf\u09b0 \u09b9\u09b2 \u09b6\u09bf\u09b2\u09cd\u09aa\u0995\u09b2\u09be\u09b0 \u0985\u09a6\u09cd\u09ad\u09c2\u09a4 \u09a8\u09bf\u09a6\u09b0\u09cd\u09b6\u09a8 
p639
aV\u09ac\u09cd\u09af\u09be\u0999\u09cd\u0997\u09be\u09b2\u09cb\u09b0- \u09a5\u09c7\u0995\u09c7 \u09ed\u09e6 \u0995\u09bf\u09b2\u09cb\u09ae\u09bf\u099f\u09be\u09b0 \u09a6\u09c2\u09b0\u09c7 ` \u09a4\u09c1\u09ae\u0995\u09c1\u09b0- \u09b0\u09cb\u09a1\u09c7 ` \u09e7\u09e7\u09ee\u09ee \u09ae\u09bf\u099f\u09be\u09b0 \u0989\u0981\u099a\u09c1 \u09a6\u09c7\u09ac\u09a8\u09be\u09b0\u09be\u09af\u09bc\u09a3- \u09a6\u09c1\u09b0\u09cd\u0997\u09be- \u09aa\u09be\u09b9\u09be\u09a1\u09bc\u09c7 \u09af\u09cb\u0997\u09a8\u09b0\u09b8\u09bf\u09ae\u09cd\u09b9\u09be- \u0993 \u09ad\u09cb\u0997\u09a8\u09b0\u09b8\u09bf\u09ae\u09cd\u09b9\u09be\u09b0- \u09aa\u09cd\u09b0\u09a4\u09bf \u09b8\u09ae\u09b0\u09cd\u09aa\u09bf\u09a4 \u09a6\u09c1\u099f\u09cb \u09ae\u09a8\u09cd\u09a6\u09bf\u09b0 \u0986\u099b\u09c7 
p640
aV\u0995\u09b0\u09c0\u09ac\u09c7\u099f\u09be- \u0986\u09b0 \u09ac\u09bf\u09b2\u09bf\u09ac\u09c7\u099f\u09be- \u09b6\u09bf\u0996\u09b0\u09c7 \u09e8 \u099c\u09a8 \u09b9\u09bf\u09a8\u09cd\u09a6\u09c1- \u09a6\u09c7\u09ac\u09a4\u09be \u09a8\u09b0\u09b8\u09bf\u09ae\u09cd\u09b9\u09be- \u0993 \u09ac\u09c0\u09b0\u09ae\u09a8\u09cd\u09a6\u09cd\u09b0\u09c7\u09b0- \u09aa\u09cd\u09b0\u09a4\u09bf \u09b8\u09ae\u09b0\u09cd\u09aa\u09bf\u09a4 \u09a6\u09c1\u099f\u09bf \u09ae\u09a8\u09cd\u09a6\u09bf\u09b0 \u0986\u099b\u09c7 
p641
aV\u09ac\u09cd\u09af\u09be\u0999\u09cd\u0997\u09be\u09b2\u09cb\u09b0- \u09a5\u09c7\u0995\u09c7 \u09ec\u09e7 \u0995\u09bf\u09b2\u09cb\u09ae\u09bf\u099f\u09be\u09b0 \u09a6\u09c2\u09b0\u09c7 \u09e7\u09e8\u09e6\u09ed \u09ae\u09bf\u099f\u09be\u09b0 \u0989\u0981\u099a\u09c1 \u09aa\u09be\u09b9\u09be\u09a1\u09bc\u09c7\u09b0 \u0995\u09b0\u09c0\u09ac\u09c7\u099f\u09be- \u0986\u09b0 \u09ac\u09bf\u09b2\u09bf\u09ac\u09c7\u099f\u09be- \u09b6\u09bf\u0996\u09b0 \u09aa\u09b0\u09cd\u09af\u09a8\u09cd\u09a4 \u09af\u09be\u0993\u09af\u09bc\u09be\u09b0 \u09b8\u09ae\u09af\u09bc \u0996\u09c1\u09ac\u0987 \u09b8\u09c1\u0996\u09ae\u09af\u09bc \u0985\u09a8\u09c1\u09ad\u09c2\u09a4\u09bf \u09b9\u09af\u09bc 
p642
aV` \u09a1\u09cb\u09a1\u09cd\u09a1\u09be\u0987\u09b2\u09be\u09a6\u09be- \u09ae\u09b0\u09be- ` \u0985\u099e\u09cd\u099a\u09b2\u09c7\u09b0 \u09ea\u09e6\u09e6 \u09ac\u099b\u09b0\u09c7\u09b0 \u09aa\u09c1\u09b0\u09be\u09a8\u09cb \u09ac\u099f \u0997\u09be\u099b \u09b9\u09b2 \u09ac\u09bf\u09b6\u09c7\u09b7 \u0986\u0995\u09b0\u09cd\u09b7\u09a3\u09c0\u09af\u09bc 
p643
aV\u09ac\u09cd\u09af\u09be\u0999\u09cd\u0997\u09be\u09b2\u09cb\u09b0- \u09a5\u09c7\u0995\u09c7 \u09e8\u09ee \u0995\u09bf\u09b2\u09cb\u09ae\u09bf\u099f\u09be\u09b0 \u09a6\u09c2\u09b0\u09c7 ` \u09a1\u09cb\u09a1\u09cd\u09a1\u09be\u0987\u09b2\u09be\u09a6\u09be- \u09ae\u09b0\u09be- ` \u09b8\u09cd\u09a5\u09be\u09a8\u09c7 \u09e9 \u098f\u0995\u09b0 \u09ac\u09bf\u09b8\u09cd\u09a4\u09c3\u09a4 \u098f\u0995\u099f\u09be \u09aa\u09bf\u0995\u09a8\u09bf\u0995 \u09b8\u09cd\u09aa\u099f \u0986\u099b\u09c7 
p644
aV\u09ac\u099e\u09cd\u099c\u09b0- \u09b9\u09b2 \u0995\u09c1\u09b2\u09cd\u09b2\u09c1- \u09a5\u09c7\u0995\u09c7 \u09eb\u09ee \u0995\u09bf.\u09ae\u09bf. \u09a6\u09c2\u09b0\u09c7 \u0985\u09ac\u09b8\u09cd\u09a5\u09bf\u09a4 
p645
aV\u09ac\u099e\u09cd\u099c\u09b0- \u09b9\u09b2 \u09a4\u09bf\u09b0\u09a5\u09be\u09a8- \u09a8\u09a6\u09c0\u09a4\u09c7 \u099f\u09cd\u09b0\u09be\u0989\u099f- \u09ab\u09bf\u09b6\u09bf\u0982\u09af\u09bc\u09c7\u09b0 \u099c\u09a8\u09cd\u09af \u098f\u0995 \u0989\u09a4\u09cd\u0995\u09c3\u09b7\u09cd\u099f \u09b8\u09cd\u09a5\u09b2 ( \u09e7\u09eb\u09e9\u09ea \u09ae\u09bf\u099f\u09be\u09b0 ) 
p646
aV\u098f\u0996\u09be\u09a8\u09c7 \u09ac\u09c7\u09b6\u09bf\u09b0\u09ad\u09be\u0997 \u09ae\u09a4\u09cd\u09b8\u099c\u09c0\u09ac\u09c0 \u09ac\u09b8\u09ac\u09be\u09b8 \u0995\u09b0\u09c7 
p647
aV\u09aa\u09c1\u09b2\u0997\u09be- \u098f\u09ac\u0982 \u0996\u09c0\u09b0\u0997\u0999\u09cd\u0997\u09be- \u09b9\u09b2 \u09e7\u09ec \u0995\u09bf.\u09ae\u09bf. \u09a6\u09c2\u09b0\u09c7 \u0985\u09ac\u09b8\u09cd\u09a5\u09bf\u09a4 
p648
aV\u0995\u09a0\u09bf\u09a8 \u09aa\u09a6\u09af\u09be\u09a4\u09cd\u09b0\u09be\u09b0 \u09aa\u09b0 \u0986\u09aa\u09a8\u09bf \u09b6\u09be\u09a8\u09cd\u09a4 \u09aa\u09cd\u09b0\u09be\u0995\u09c3\u09a4\u09bf\u0995 \u09ac\u09be\u09a4\u09be\u09ac\u09b0\u09a3\u09c7 \u09ab\u09b0\u09c7\u09b8\u09cd\u099f \u09b0\u09c7\u09b8\u09cd\u099f \u09b9\u09be\u0989\u09b8 \u09aa\u09cc\u0981\u099b\u09be\u09ac\u09c7\u09a8 \u09af\u09c7\u0996\u09be\u09a8\u09c7 \u09ae\u09a8 \u098f\u09ac\u0982 \u09b6\u09b0\u09c0\u09b0 \u09a6\u09c1\u0987\u0987 \u098f\u0995 \u0985\u09a6\u09cd\u09ad\u09c1\u09a4 \u09b6\u09be\u09a8\u09cd\u09a4\u09bf \u0985\u09a8\u09c1\u09ad\u09ac \u0995\u09b0\u09c7 
p649
aV\u09ae\u09c1\u0996\u09cd\u09af \u09aa\u09be\u09b0\u09cd\u09ac\u09a4\u09cd\u09af \u0989\u09aa\u09a4\u09cd\u09af\u09be\u0995\u09be \u09a5\u09c7\u0995\u09c7 \u0986\u09b2\u09be\u09a6\u09be \u09a4\u09cb\u09b8- \u09a8\u09b2\u09cd\u09b2\u09be\u09b9- \u09b9\u09b2 \u09b8\u09c7\u0987 \u09b8\u09ac \u09aa\u09b0\u09cd\u09ac\u09a4\u09be\u09b0\u09cb\u09b9\u09c0\u09b0 \u099c\u09a8\u09cd\u09af \u0986\u09b0\u09cd\u09a6\u09b6 \u099c\u09be\u09af\u09bc\u0997\u09be \u09af\u09be\u09b0\u09be \u09a8\u09c8\u09b8\u09b0\u09cd\u0997\u09bf\u0995 \u09aa\u09cd\u09b0\u0995\u09c3\u09a4\u09bf\u09b0 \u09a6\u09c3\u09b6\u09cd\u09af\u09c7\u09b0 \u098f\u09ac\u0982 \u0995\u09cd\u09af\u09be\u09ae\u09cd\u09aa\u09c7 \u09a5\u09be\u0995\u09be\u09b0 \u0986\u09a8\u09a8\u09cd\u09a6 \u09a8\u09bf\u09a4\u09c7 \u099a\u09be\u09a8 
p650
aV\u098f\u0995 \u0995\u09bf\u0982\u09ac\u09a6\u09a8\u09cd\u09a4\u09c0 \u0985\u09a8\u09c1\u09b8\u09be\u09b0\u09c7 \u09ae\u09a3\u09bf\u0995\u09b0\u09a3\u09c7\u09b0- \u09b8\u09be\u09a5\u09c7 \u09ad\u0997\u09ac\u09be\u09a8 \u09b6\u09bf\u09ac- \u098f\u09ac\u0982 \u0993\u09a8\u09be\u09b0 \u09b8\u09cd\u09a4\u09cd\u09b0\u09c0 \u09aa\u09be\u09b0\u09cd\u09ac\u09a4\u09c0\u09b0- \u09b8\u09b0\u09cd\u09ae\u09cd\u09aa\u0995 \u0986\u099b\u09c7 \u09af\u09c7\u0996\u09be\u09a8\u09c7 \u0993\u09a8\u09be\u09b0 \u09b9\u09be\u09b0\u09bf\u09af\u09bc\u09c7 \u09af\u09be\u0993\u09af\u09bc\u09be \u0995\u09be\u09a8\u09c7\u09b0 \u09a6\u09c1\u09b2 \u09aa\u09be\u0993\u09af\u09bc\u09be \u0997\u09bf\u09af\u09bc\u09c7\u099b\u09bf\u09b2 
p651
aV\u0995\u09c1\u09b2\u09cd\u09b2\u09c1- \u09a5\u09c7\u0995\u09c7 \u09b2\u09be\u09b0\u099c\u09c0- \u09b9\u09b2 \u09e9\u09ea \u0995\u09bf.\u09ae\u09bf. \u09a6\u09c2\u09b0\u09c7 \u0985\u09ac\u09b8\u09cd\u09a5\u09bf\u09a4 
p652
aV\u0995\u09c1\u09b2\u09cd\u09b2\u09c1\u09b0- \u09a6\u0995\u09cd\u09b7\u09bf\u09a3- \u09a6\u09bf\u0995\u09c7 \u09ef\u09eb\u09ed \u09ae\u09bf\u099f\u09be\u09b0 \u0989\u0981\u099a\u09c1\u09a4\u09c7 \u098f\u0995\u099f\u09be \u099b\u09cb\u099f\u09cb \u0997\u09cd\u09b0\u09be\u09ae \u0986\u099b\u09c7 \u09af\u09be \u09b9\u09b2 \u099f\u09cd\u09b0\u09be\u0989\u099f \u09ab\u09bf\u09b6\u09bf\u0982\u09af\u09bc\u09c7\u09b0 \u099c\u09a8\u09cd\u09af \u09ad\u09be\u09b2\u09cb \u09b8\u09cd\u09a5\u09be\u09a8 
p653
aV\u098f\u0996\u09be\u09a8\u09c7 \u098f\u0995 \u09b0\u09c7\u09b8\u09cd\u099f \u09b9\u09be\u0989\u09b8 \u09b9\u09b2 \u098f\u09ae\u09a8 \u0986\u0995\u09b0\u09cd\u09b7\u0995 \u099c\u09be\u09af\u09bc\u0997\u09be\u09af\u09bc \u0985\u09ac\u09b8\u09cd\u09a5\u09bf\u09a4 \u09af\u09c7 \u098f\u09b0 \u09a6\u09c1\u0987 \u09a6\u09bf\u0995 \u09a6\u09bf\u09af\u09bc\u09c7 \u09a4\u09c0\u09ac\u09cd\u09b0 \u0997\u09ad\u09c0\u09b0 \u09b8\u09c7\u099c \u098f\u09ac\u0982 \u09a4\u09bf\u09b0\u09a5\u09be\u09a8- \u09a8\u09a6\u09c0 \u09aa\u09cd\u09b0\u09be\u09af\u09bc \u09e7\u09e6\u09e6 \u09ae\u09bf\u099f\u09be\u09b0 \u09a8\u09bf\u099a\u09c7 \u0997\u09bf\u09af\u09bc\u09c7 \u09ac\u09cd\u09af\u09be\u09b8- \u09a8\u09a6\u09c0\u09b0 \u09b8\u09be\u09a5\u09c7 \u09ae\u09bf\u09b2\u09c7 \u09af\u09be\u09af\u09bc 
p654
aV\u09ae\u09a7\u09c1\u09ae\u09c7\u09b9\u09c7\u09b0 \u09ab\u09b2\u09c7 \u09aa\u09be\u09df\u09c7\u09b0 \u09b8\u09ae\u0995\u09cd\u09b0\u09ae\u09a3 \u09aa\u09be\u09df\u09c7\u09b0 \u09a4\u09b2\u09be \u09a5\u09c7\u0995\u09c7 \u09b9\u09df 
p655
aV\u09b6\u09c1\u09b0\u09c1\u09a4\u09c7 \u099a\u09be\u09ae\u09a1\u09bc\u09be \u09b2\u09be\u09b2 \u09b9\u09df\u09c7 \u09af\u09be\u09df \u09af\u09be \u099a\u09c7\u09aa\u09c7 \u09a6\u09bf\u09b2\u09c7\u0993 \u098f\u0995\u0987 \u09a5\u09be\u0995\u09c7 
p656
aV\u09ae\u09a7\u09c1\u09ae\u09c7\u09b9\u09c7\u09b0 \u09ab\u09b2\u09c7 \u09b9\u0993\u09df\u09be \u0985\u09b8\u09cd\u09a5\u09bf\u09b0\u09cb\u0997 \u09b9\u09b2 \u098f\u0987 \u09aa\u09cd\u09b0\u0995\u09be\u09b0 -
p657
aV\u09b9\u09be\u09a1\u09bc \u0995\u09ae\u099c\u09cb\u09b0 \u09b9\u09df\u09c7 \u09af\u09be\u0993\u09df\u09be , \u0997\u09a4\u09bf\u09ac\u09bf\u09a7\u09bf\u09a4\u09c7 \u09aa\u09b0\u09bf\u09ac\u09b0\u09cd\u09a4\u09a8 \u0986\u09b8\u09be ( \u09a8\u09be\u09a1\u09bc\u09bf \u0985\u09ac\u09b0\u09cb\u09a7\u09c7\u09b0 \u09ab\u09b2\u09c7 \u09b6\u09b0\u09c0\u09b0\u09c7\u09b0 \u09b8\u09be\u09ae\u099e\u09cd\u099c\u09b8\u09cd\u09af \u09ac\u099c\u09be\u09df \u09a8\u09be \u09a5\u09be\u0995\u09be ) , \u09b0\u09cb\u0997 \u09aa\u09cd\u09b0\u09a4\u09bf\u09b0\u09cb\u09a7\u0995 \u0995\u09cd\u09b7\u09ae\u09a4\u09be \u0995\u09ae\u09c7 \u0986\u09b8\u09be , \u09a4\u09be\u09a1\u09bc\u09be\u09a4\u09be\u09a1\u09bc\u09bf \u0995\u09cd\u09b7\u09a4 \u09a8\u09be \u09ad\u09b0\u09be , \u099a\u09cb\u099f \u09a0\u09bf\u0995 \u09a8\u09be \u09b9\u0993\u09df\u09be\u09b0 \u09ab\u09b2\u09c7 \u09ac\u09be\u09b0\u09ac\u09be\u09b0 \u09b8\u0982\u0995\u09cd\u09b0\u09ae\u09a3 \u09b9\u0993\u09df\u09be \u0986\u09b0 \u098f\u0987 \u09b0\u09cb\u0997\u09c7\u09b0 \u09aa\u09b0\u09bf\u09a3\u09be\u09ae\u09b8\u09cd\u09ac\u09b0\u09c2\u09aa \u09b0\u09cb\u0997\u09c0\u09b0 \u09ac\u09bf\u0995\u09be\u09b2\u0999\u09cd\u0997\u09a4\u09be \u09a5\u09c7\u0995\u09c7 \u09ae\u09c3\u09a4\u09cd\u09af\u09c1 \u09aa\u09b0\u09cd\u09af\u09a8\u09cd\u09a4\u0993 \u09b9\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7 
p658
aV\u0995\u09bf\u099b\u09c1 \u09b6\u09cd\u09ac\u09c7\u09a4\u0995\u09a3\u09bf\u0995\u09be \u09af\u09be \u09aa\u09cc\u0981\u099b\u09c7 \u09af\u09be\u09df \u09a4\u09be\u09b0\u09be \u09ae\u09a7\u09c1\u09ae\u09c7\u09b9\u09c7\u09b0 \u09ab\u09b2\u09c7 \u09a8\u09b7\u09cd\u099f \u09b9\u09df\u09c7 \u09af\u09be\u09df \u0986\u09b0 \u09af\u09c7\u09b9\u09c7\u09a4\u09c1 \u098f\u0987 \u0995\u09a3\u09bf\u0995\u09be \u09b6\u09b0\u09c0\u09b0\u0995\u09c7 \u09b8\u0982\u0995\u09cd\u09b0\u09ae\u09a3\u09c7\u09b0 \u09b9\u09be\u09a4 \u09a5\u09c7\u0995\u09c7 \u09ac\u09be\u0981\u099a\u09be\u09df 
p659
aV\u09a4\u09be\u0987 \u09b0\u09cb\u0997 \u09aa\u09cd\u09b0\u09a4\u09bf\u09b0\u09cb\u09a7\u0995 \u0995\u09cd\u09b7\u09ae\u09a4\u09be \u0995\u09ae\u09c7 \u09af\u09be\u09df 
p660
aV\u098f\u0987 \u09ad\u09be\u09ac\u09c7 \u09a4\u09cd\u09ac\u0995\u09c7\u09b0 \u09a4\u09be\u09aa\u09ae\u09be\u09a4\u09cd\u09b0\u09be \u09a8\u09bf\u09df\u09a3\u09cd\u09a4\u09cd\u09b0\u09bf\u09a4 \u0995\u09b0\u09be\u09b0 \u09b6\u09bf\u09b0\u09be \u0995\u09cd\u09b7\u09a4\u09bf\u0997\u09cd\u09b0\u09b8\u09cd\u09a5 \u09b9\u0993\u09df\u09be\u09b0 \u09ab\u09b2\u09c7 \u099a\u09be\u09ae\u09a1\u09bc\u09be \u0986\u09b0\u0993 \u0996\u09be\u09b0\u09be\u09aa \u09b9\u09df\u09c7 \u09af\u09be\u09df 
p661
aV\u09aa\u09be\u09df\u09c7\u09b0 \u09b0\u0995\u09cd\u09a4 \u09b8\u099e\u09cd\u099a\u09be\u09b2\u09a8\u09c7 \u09ac\u09be\u09a7\u09be \u09aa\u09a1\u09bc\u09be\u09b0 \u09ab\u09b2\u09c7 \u09aa\u09be\u09df\u09c7 \u09aa\u09cd\u09b0\u09df\u09cb\u099c\u09a8\u09c0\u09df \u09aa\u09c1\u09b7\u09cd\u099f\u09bf \u09aa\u09cc\u0981\u099b\u09be\u09df \u09a8\u09be \u0986\u09b0 \u09b8\u09c7\u0996\u09be\u09a8\u09c7 \u09b6\u09cd\u09ac\u09c7\u09a4 \u09b0\u0995\u09cd\u09a4 \u0995\u09a3\u09bf\u0995\u09be \u0995\u09ae \u09aa\u09cc\u0981\u099b\u09be\u09df 
p662
aV\u099a\u09be\u09ae\u09a1\u09bc\u09be \u09b6\u0995\u09cd\u09a4 \u09b9\u09df\u09c7 \u09af\u09be\u09df 
p663
aV\u09aa\u09c7\u099f\u09c7 \u099a\u09cb\u099f \u09b2\u09be\u0997\u09b2\u09c7 \u0995\u09cd\u09b7\u09a4 \u09b9\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7 \u09af\u09be \u09ae\u09be\u0982\u09b8\u09aa\u09c7\u09b6\u09c0 \u0993 \u09b9\u09be\u09a1\u09bc \u09aa\u09b0\u09cd\u09af\u09a8\u09cd\u09a4 \u09aa\u09cc\u0981\u099b\u09c7 \u09af\u09be\u09df \u0986\u09b0 \u09aa\u09be\u09df\u09c7 \u09aa\u09c1\u0981\u099c \u09b9\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7 , \u0986\u0999\u09c1\u09b2 \u0997\u09b2\u09c7 \u09af\u09c7\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7 \u09ac\u09be \u0995\u09be\u09b2\u09cb \u09b9\u09df\u09c7 \u09af\u09c7\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7 
p664
aV\u09b8\u09cd\u09af\u09be\u099a\u09c1\u09b0\u09c7\u099f\u09c7\u09a1 \u09ab\u09cd\u09af\u09be\u099f - \u098f\u09b0 \u0986\u09a7\u09bf\u0995\u09cd\u09af \u09b0\u0995\u09cd\u09a4\u099a\u09be\u09aa \u09ac\u09be\u09a1\u09bc\u09be\u09a4\u09c7 \u09b8\u09b9\u09be\u09df\u0995 \u09ad\u09c2\u09ae\u09bf\u0995\u09be \u09aa\u09be\u09b2\u09a8 \u0995\u09b0\u09c7 
p665
aV\u09ae\u09a6 \u2013 \u098f\u099f\u09be \u09aa\u09be\u09a8 \u0995\u09b0\u09c7 \u09b0\u0995\u09cd\u09a4\u099a\u09be\u09aa \u09ac\u09be\u09a1\u09bc\u09c7 
p666
aV\u09b2\u09ac\u09a3 \u09b8\u09c7\u09ac\u09a8 \u2013 \u0985\u09a7\u09bf\u0995 \u09ae\u09be\u09a4\u09cd\u09b0\u09be\u09df \u09b2\u09ac\u09a3 \u0996\u09be\u0993\u09df\u09be\u09b0 \u09ab\u09b2\u09c7 \u0989\u099a\u09cd\u099a \u09b0\u0995\u09cd\u09a4\u099a\u09be\u09aa \u09ac\u09c3\u09a6\u09cd\u09a7\u09bf \u09aa\u09be\u09df 
p667
aV\u09b9\u09be\u09a4\u09be\u09b6\u09be \u09a5\u09c7\u0995\u09c7 \u09ac\u09be\u0981\u099a\u09a4\u09c7 \u09a8\u09bf\u099c\u09c7\u09b0 \u09ac\u09a8\u09cd\u09a7\u09c1\u09b0 \u09b8\u0982\u0996\u09cd\u09af\u09be \u09ac\u09be\u09a1\u09bc\u09be\u09a8 
