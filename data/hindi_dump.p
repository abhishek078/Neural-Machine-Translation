p818
aV\u0905\u0917\u0930 \u091a\u092e\u091a\u092e\u093e\u0924\u0947 \u0926\u093e\u0901\u0924 \u0928 \u0939\u094b\u0902 ,  \u0924\u094b \u091a\u0947\u0939\u0930\u0947 \u0915\u093e \u0906\u0915\u0930\u094d\u0937\u0923 \u0905\u0927\u0942\u0930\u093e \u0930\u0939 \u091c\u093e\u0924\u093e \u0939\u0948  
p819
aV\u0930\u093e\u092e\u0926\u093e \u0928\u0947 \u0905\u092a\u0928\u0947 \u092c\u0941\u091c\u0941\u0930\u094d\u0917\u094b\u0902 \u0938\u0947 \u0938\u0941\u0928\u093e \u0925\u093e \u0915\u093f \u0935\u0930\u094d\u0937\u094b\u0902 \u092a\u0939\u0932\u0947 \u0905\u0902\u0917\u094d\u0930\u0947\u091c \u0932\u094b\u0917 \u2019\u0925\u093e\u0932\u093e\u0917\u094d\u0935\u093e\u0930\u2019 \u092e\u0947\u0902 \u092c\u0930\u094d\u092b \u0915\u0947 \u090a\u092a\u0930 \u0932\u0915\u0921\u093c\u0940 \u0915\u0940 \u0928\u093e\u0935 \u0938\u0940 \u0924\u0948\u0930\u093e\u0924\u0947 \u0925\u0947 ( \u0938\u094d\u0915\u0940\u0908\u0902\u0917 \u0915\u0930\u0924\u0947 \u0939\u094b\u0902\u0917\u0947 ) 
p820
aV\u0928\u093e\u0939\u0928 \u092e\u0947\u0902 \u0920\u0939\u0930\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u0938\u0930\u094d\u0915\u093f\u091f \u0939\u093e\u0909\u0938 , \u092a\u0940\u0921\u092c\u094d\u0932\u094d\u092f\u0942\u0921\u0940 \u0935 \u092e\u094d\u092f\u0942\u0928\u093f\u0938\u093f\u092a\u0932 \u0930\u0947\u0938\u094d\u091f \u0939\u093e\u0909\u0938 , \u090f\u0938\u090f\u092b\u0921\u0940\u090f \u0935 \u0905\u0928\u0947\u0915 \u092a\u094d\u0930\u093e\u0907\u0935\u0947\u091f \u0930\u093f\u0939\u093e\u092f\u0936\u0917\u093e\u0939\u0947\u0902 \u0939\u0948\u0902 
p821
aV\u0930\u093e\u091c\u0938\u094d\u0925\u093e\u0928 \u0915\u0947 \u0926\u0930\u094d\u0936\u0928\u0940\u092f \u0938\u094d\u0925\u0932\u094b\u0902 \u092e\u0947\u0902 \u0905\u0932\u0935\u0930 \u0915\u0947 \u092a\u093e\u0938 \u2019 \u0938\u093f\u0932\u0940\u0938\u0947\u095c \u2019 \u0938\u094d\u0925\u093e\u0928 \u092e\u0939\u0932 , \u091d\u0940\u0932 , \u091c\u0932\u092a\u0915\u094d\u0937\u0940 \u0915\u0947 \u0932\u093f\u090f \u0932\u094b\u0915\u092a\u094d\u0930\u093f\u092f \u0939\u0948 
p822
aV\u0930\u093e\u091c\u0938\u094d\u0925\u093e\u0928 \u0915\u093e \u2019 \u092c\u092f\u093e\u0928\u093e \u2019 \u0938\u094d\u0925\u093e\u0928 \u0910\u0924\u093f\u0939\u093e\u0938\u093f\u0915 \u0915\u093f\u0932\u0947 \u0915\u0947 \u0932\u093f\u090f \u0932\u094b\u0915\u092a\u094d\u0930\u093f\u092f \u0939\u0948 
p823
aV`` \u091c\u093f\u0902\u0926\u0917\u0940 \u0915\u0940 \u0938\u092c \u091a\u0939\u0915 \u092f\u0939\u093e\u0901 \u0939\u094b\u0924\u0940 \u0939\u0948 
p824
aV\u0930\u093e\u091c\u0938\u094d\u0925\u093e\u0928 \u0915\u0947 \u092a\u094d\u0930\u0924\u094d\u092f\u0947\u0915 \u091c\u0917\u0939 \u0915\u0940 \u0905\u092a\u0928\u0940 \u0905\u0928\u0942\u0920\u0940 \u0935\u093e\u0938\u094d\u0924\u0941\u0936\u093f\u0932\u094d\u092a \u0938\u0902\u0930\u091a\u0928\u093e \u0939\u0948 \u0914\u0930 \u0905\u092a\u0928\u0940 \u090f\u0915 \u0905\u0932\u0917 \u0917\u093e\u0925\u093e 
p825
aV\u0930\u093e\u091c\u0938\u094d\u0925\u093e\u0928 \u0915\u0940 \u092a\u0939\u093e\u095c\u093f\u092f\u094b\u0902 \u0915\u0947 \u092c\u0940\u091a \u092c\u0928\u0947 \u092c\u0917\u0940\u091a\u0947 \u0914\u0930 \u091d\u0940\u0932\u0947\u0902 \u092a\u0930\u094d\u092f\u091f\u0915\u094b\u0902 \u0915\u093e \u092e\u0928 \u092e\u094b\u0939\u0928\u0947 \u092e\u0947\u0902 \u0915\u094b\u0908 \u0915\u0938\u0930 \u0928\u0939\u0940\u0902 \u091b\u094b\u095c\u0924\u0947 
p826
aV\u0930\u093e\u091c\u0938\u094d\u0925\u093e\u0928 \u0907\u0924\u093f\u0939\u093e\u0938 \u092e\u0947\u0902 \u0928 \u0915\u0947\u0935\u0932 \u0905\u092a\u0928\u0947 \u0936\u094d\u0930\u0943\u0902\u0917\u093e\u0930 \u0935 \u0936\u094c\u0930\u094d\u092f \u0915\u0947 \u0915\u093e\u0930\u0923 \u092e\u0939\u0924\u094d\u0924\u094d\u0935\u092a\u0942\u0930\u094d\u0923 \u0938\u094d\u0925\u093e\u0928 \u0930\u0916\u0924\u093e \u0939\u0948 \u092c\u0932\u094d\u0915\u093f \u0938\u0902\u0938\u094d\u0915\u0943\u0924\u093f , \u0907\u0924\u093f\u0939\u093e\u0938 \u0914\u0930 \u092a\u094d\u0930\u093e\u0915\u0943\u0924\u093f\u0915 \u0938\u0941\u0902\u0926\u0930\u0924\u093e \u092e\u0947\u0902 \u092d\u0940 \u0905\u092a\u0928\u0940 \u0938\u092e\u0943\u0926\u094d\u0927\u0924\u093e \u0926\u0930\u094d\u0936\u093e\u0924\u093e \u0939\u0948 
p827
aV\u0930\u093e\u091c\u0938\u094d\u0925\u093e\u0928 \u092e\u0947\u0902 \u0938\u0948\u0915\u095c\u094b\u0902 \u0915\u093f\u0932\u094b\u092e\u0940\u091f\u0930 \u0924\u0915 \u092b\u0948\u0932\u0940 \u092c\u093e\u0932\u0942 \u0930\u0947\u0924 \u0915\u0947 \u091f\u0940\u0932\u0947 , \u0926\u093e\u0901\u0924\u094b\u0902 \u0924\u0932\u0947 \u0905\u0901\u0917\u0941\u0932\u0940 \u0926\u092c\u093e\u0928\u0947 \u0915\u094b \u0935\u093f\u0935\u0936 \u0915\u0930 \u0926\u0947\u0928\u0947 \u0935\u093e\u0932\u0947 \u0905\u0926\u094d\u200d\u092d\u0941\u0924 \u0915\u093f\u0932\u0947 , \u0915\u0932\u093e\u0924\u094d\u092e\u0915 \u0930\u093e\u091c\u092a\u094d\u0930\u093e\u0938\u093e\u0926 \u0914\u0930 \u0939\u0935\u0947\u0932\u093f\u092f\u093e\u0901 \u0939\u0948\u0902 
p828
aV\u0930\u093e\u091c\u0938\u094d\u0925\u093e\u0928 \u0915\u0940 \u0936\u093e\u092e\u0947\u0902 \u0909\u0938 \u0938\u092e\u092f \u0905\u092d\u093f\u092d\u0942\u0924 \u0915\u0930\u0928\u0947 \u0935\u093e\u0932\u0940 \u0939\u094b\u0924\u0940 \u0939\u0948\u0902 , \u091c\u092c \u091d\u093f\u0932\u092e\u093f\u0932\u093e\u0924\u0940 \u0930\u0947\u0924 \u0915\u0947 \u090f\u0915\u093e\u0902\u0924 \u0938\u0947 \u0917\u0942\u0901\u091c\u0924\u0940 \u0938\u0941\u0930\u0940\u0932\u0940 \u0927\u0941\u0928\u0947\u0902 \u0939\u0935\u093e \u092e\u0947\u0902 \u0924\u0948\u0930\u0924\u0940 \u0939\u0948\u0902 
p829
aV\u091c\u0948\u0938\u0932\u092e\u0947\u0930 \u090f\u0935\u0902 \u092c\u093e\u095c\u092e\u0947\u0930 \u0915\u094d\u0937\u0947\u0924\u094d\u0930 \u0915\u0947 \u2019 \u0932\u0902\u0917\u093e \u2019 \u090f\u0935\u0902 \u2019 \u092e\u093e\u0902\u0917\u0923\u093f\u092f\u093e\u0930 \u2019 \u091c\u093e\u0924\u093f \u0915\u0947 \u0932\u094b\u0915 \u0915\u0932\u093e\u0915\u093e\u0930 \u0906\u091c \u092a\u0942\u0930\u0947 \u0935\u093f\u0936\u094d\u200d\u0935 \u092e\u0947\u0902 \u092a\u094d\u0930\u0938\u093f\u0926\u094d\u0927\u093f \u092a\u093e \u0930\u0939\u0947 \u0939\u0948\u0902 
p830
aV\u0930\u093e\u091c\u0938\u094d\u0925\u093e\u0928 \u0915\u0947 \u0932\u094b\u0915\u0917\u0940\u0924\u094b\u0902 \u092e\u0947\u0902 \u091c\u0939\u093e\u0901 \u0926\u0947\u0935\u0940 - \u0926\u0947\u0935\u0924\u093e\u0913\u0902 \u0915\u093e \u0938\u094d\u092e\u0930\u0923 \u0915\u093f\u092f\u093e \u091c\u093e\u0924\u093e \u0939\u0948 \u0935\u0939\u0940\u0902 \u092a\u094d\u0930\u0947\u092e \u0914\u0930 \u0936\u094d\u0930\u0943\u0902\u0917\u093e\u0930 \u0915\u094b \u092d\u0940 \u092a\u094d\u0930\u0927\u093e\u0928\u0924\u093e \u0926\u0940 \u091c\u093e\u0924\u0940 \u0939\u0948 
p831
aV\u0905\u092a\u0928\u0947 \u0909\u0932\u094d\u0932\u093e\u0938\u092a\u0942\u0930\u094d\u0923 \u0932\u094b\u0915 \u0928\u0943\u0924\u094d\u092f\u094b\u0902 \u0935 \u0938\u0902\u0917\u0940\u0924 , \u0917\u093e\u0925\u093e\u0913\u0902 \u0935 \u092d\u0935\u094d\u092f \u0909\u0924\u094d\u0938\u0935\u094b\u0902 \u0915\u0947 \u0915\u093e\u0930\u0923 \u0930\u093e\u091c\u0938\u094d\u0925\u093e\u0928 \u0915\u0940 \u092f\u0939 \u092d\u0942\u092e\u093f \u091c\u0940\u0935\u0902\u0924 \u0939\u094b \u0909\u0920\u0924\u0940 \u0939\u0948 
p832
aV\u0936\u0930\u0940\u0930 \u0915\u094b \u091c\u093f\u0924\u0928\u0947 \u092d\u094b\u091c\u0928 \u0915\u0940 \u0906\u0935\u0936\u094d\u092f\u0915\u0924\u093e \u0939\u0948 \u0909\u0924\u0928\u093e \u0939\u0940 \u092d\u094b\u091c\u0928 \u0915\u0930\u0947\u0902 
p833
aV\u092a\u094d\u0930\u0924\u094d\u092f\u0947\u0915 \u092d\u094b\u091c\u0928 \u0915\u0947 \u092c\u093e\u0926 \u092e\u0941\u0932\u093e\u092f\u092e \u092c\u094d\u0930\u0936 \u092f\u093e \u0928\u0940\u092e \u0915\u0947 \u0926\u093e\u0924\u0942\u0928 \u0938\u0947 \u0926\u093e\u0901\u0924\u094b\u0902 \u0915\u0940 \u0938\u092b\u093e\u0908 \u0915\u0930\u0928\u093e \u0928 \u092d\u0942\u0932\u0947\u0902 
p834
aV\u0917\u0941\u0921\u093c \u0936\u0930\u092c\u0924 \u092c\u0928\u093e\u0915\u0930 \u0932\u0947\u0902 \u092f\u093e \u0917\u0941\u0921\u093c \u0915\u0940 \u0936\u0939\u0926 \u092c\u0928\u093e\u0915\u0930 \u0938\u0947\u0935\u0928 \u0915\u0930\u0947\u0902 
p835
aV\u0905\u091a\u094d\u091b\u0940 \u0924\u094d\u0935\u091a\u093e \u0915\u0947 \u0932\u093f\u090f \u091c\u0930\u0942\u0930\u0940 \u0939\u0948 \u0915\u093f \u0906\u092a \u0926\u093f\u0928 \u092e\u0947\u0902 \u0915\u092e \u0938\u0947 \u0915\u092e 3 - 5 \u0932\u0940\u091f\u0930 \u092a\u093e\u0928\u0940 \u090f\u0915 \u0938\u0941\u0928\u093f\u0936\u094d\u200d\u091a\u093f\u0924 \u0905\u0902\u0924\u0930\u093e\u0932 \u092a\u0930 \u0905\u0935\u0936\u094d\u092f \u092a\u0940\u090f\u0901 
p836
aV\u092a\u093e\u092f\u0930\u093f\u092f\u093e \u0939\u094b\u0928\u0947 \u092a\u0930 \u092a\u094d\u0930\u093e\u092f: \u0926\u0902\u0924 \u0935\u093f\u0936\u0947\u0937\u091c\u094d\u091e \u0926\u093e\u0901\u0924\u094b\u0902 \u0915\u094b \u0928\u093f\u0915\u0932\u0935\u093e \u0926\u0947\u0928\u0947 \u0915\u0940 \u0938\u0932\u093e\u0939 \u0926\u0947\u0924\u0947 \u0939\u0948\u0902 
p837
aV\u092b\u0932\u0938\u094d\u0935\u0930\u0942\u092a \u092a\u0940\u0935 , \u092e\u0935\u093e\u0926 , \u0926\u0941\u0930\u094d\u0917\u0928\u094d\u0927 \u0909\u0924\u094d\u092a\u0928\u094d\u0928 \u0939\u094b \u091c\u093e\u0924\u0940 \u0939\u0948 \u091c\u094b \u0905\u0928\u094d\u0924 \u092e\u0947\u0902 \u092a\u093e\u092f\u0930\u093f\u092f\u093e \u0915\u093e \u0930\u0942\u092a \u0932\u0947 \u0932\u0947\u0924\u093e \u0939\u0948 
p838
aV\u0926\u093e\u0901\u0924\u094b \u0915\u094b \u092a\u093f\u0928 \u092f\u093e \u0938\u0932\u093e\u0908 \u0938\u0947 \u0928 \u0915\u0941\u0930\u0947\u0926\u0947\u0902 
p839
aV\u0905\u0927\u093f\u0915 \u0917\u0930\u092e \u091a\u093e\u092f , \u0915\u0949\u092b\u0940 , \u0938\u0942\u092a \u0924\u0925\u093e \u0905\u0927\u093f\u0915 \u0920\u0902\u0921\u0947 \u092a\u0947\u092f \u092a\u0926\u093e\u0930\u094d\u0925 \u092f\u093e \u0920\u094b\u0938 \u092a\u0926\u093e\u0930\u094d\u0925 \u091c\u0948\u0938\u0947 \u0906\u0907\u0938\u0915\u094d\u0930\u0940\u092e \u0906\u0926\u093f \u0938\u0947 \u092c\u091a\u0947\u0902 
p840
aV\u0905\u092e\u0947\u0930\u093f\u0915\u0928 \u0915\u0948\u0902\u0938\u0930 \u0930\u093f\u0938\u0930\u094d\u091a \u090f\u0938\u094b\u0938\u093f\u090f\u0936\u0928 \u0926\u094d\u0935\u093e\u0930\u093e \u0915\u093f\u090f \u0917\u090f \u0936\u094b\u0927 \u0915\u0947 \u0905\u0928\u0941\u0938\u093e\u0930 \u091f\u092e\u093e\u091f\u0930 \u092a\u094d\u0930\u094b\u0938\u094d\u091f\u0947\u091f \u0915\u0948\u0902\u0938\u0930 \u0915\u094b \u0930\u094b\u0915\u0928\u0947 \u092e\u0947\u0902 \u092b\u093e\u092f\u0926\u0947\u092e\u0902\u0926 \u0939\u0948 
p841
aV\u0907\u0938\u092e\u0947\u0902 \u0938\u092b\u0947\u0926 \u0926\u093e\u0917 \u091c\u0948\u0938\u093e \u0924\u094d\u0935\u091a\u093e \u0930\u094b\u0917 \u092d\u0940 \u0939\u094b \u0938\u0915\u0924\u093e \u0939\u0948 , \u0915\u094d\u092f\u094b\u0902\u0915\u093f \u0938\u093f\u0902\u0926\u0942\u0930 \u092e\u0947\u0902 \u0932\u0947\u0921 \u0911\u0915\u094d\u0938\u093e\u0907\u0921 \u0914\u0930 \u092a\u0948\u0930\u093e\u092b\u093f\u0928\u093e\u0907\u0932 \u0921\u093e\u0907\u090f\u092e\u0940\u0928 \u091c\u0948\u0938\u0947 \u0918\u093e\u0924\u0915 \u0930\u0938\u093e\u092f\u0928\u094b\u0902 \u0915\u0940 \u092e\u093f\u0932\u093e\u0935\u091f \u0915\u0940 \u091c\u093e\u0924\u0940 \u0939\u0948 
