p851
aV\u0915\u0947\u0926\u093e\u0930\u0928\u093e\u0925 \u0915\u094b \u091b\u094b\u0921\u093c\u0915\u0930 \u092c\u0926\u094d\u0930\u0940\u0928\u093e\u0925 \u091c\u093e\u0928\u0947 \u0935\u093e\u0932\u0947 \u092f\u093e\u0924\u094d\u0930\u0940 \u0930\u0941\u0926\u094d\u0930\u092a\u094d\u0930\u092f\u093e\u0917 \u0938\u0947 \u0905\u0932\u0915\u0928\u0902\u0926\u093e \u0915\u0947 \u0915\u093f\u0928\u093e\u0930\u0947-\u0915\u093f\u0928\u093e\u0930\u0947 \u091a\u092e\u094b\u0932\u0940 \u091c\u093e\u0924\u0947 \u0939\u0948\u0902 
p852
aV\u0917\u094b\u092a\u0947\u0936\u094d\u200d\u0935\u0930 \u0938\u0947 \u0906\u0917\u0947 1 / 2 \u092e\u0940\u0932 \u092a\u0930 \u092c\u093e\u0901\u092f\u0947 \u0913\u0930 \u090f\u0915 \u092c\u0939\u0941\u0924 \u091b\u094b\u091f\u093e \u092e\u0902\u0926\u093f\u0930 \u0914\u0930 \u0926\u093e\u0939\u093f\u0928\u0940 \u0913\u0930 \u092c\u093e\u0932\u093e\u0938\u094b\u0924\u0940 \u0928\u0926\u0940 \u0915\u0947 \u0915\u093f\u0928\u093e\u0930\u0947 \u092a\u0930 \u090f\u0915 \u092c\u0938\u094d\u0924\u0940 \u0939\u0948\u0902 
p853
aV\u090f\u0915 \u092e\u0940\u0932 \u092a\u0930 \u0905\u0932\u0915\u0928\u0902\u0926\u093e \u0914\u0930 \u0915\u0941\u091b \u0926\u0942\u0930 \u0926\u093e\u0939\u093f\u0928\u0940 \u0913\u0930 \u0905\u0932\u0915\u0928\u0902\u0926\u093e \u0914\u0930 \u092c\u093e\u0932\u093e\u0938\u094b\u0924\u0940 \u0915\u093e \u0938\u0902\u0917\u092e , 11 / 2 \u092e\u0940\u0932 \u092a\u0930 \u0905\u0932\u0915\u0928\u0902\u0926\u093e \u0915\u0947 \u0926\u093e\u0939\u093f\u0928\u0947 \u0915\u093f\u0928\u093e\u0930\u0947 \u092a\u0930 \u0915\u094b\u091f\u093e\u0932 \u0917\u093e\u0901\u0935 \u0928\u093e\u092e\u0915 \u091b\u094b\u091f\u0940 \u092c\u0938\u094d\u0924\u0940 \u0914\u0930 \u0926\u094b \u092e\u0940\u0932 \u092a\u0930 \u091a\u092e\u094b\u0932\u0940 \u0939\u0948 , \u091c\u093f\u0938\u0915\u094b \u0932\u094b\u0917 \u0932\u093e\u0932 \u0938\u093e\u0902\u0917\u093e \u092d\u0940 \u0915\u0939\u0924\u0947 \u0939\u0948\u0902 
p854
aV\u092a\u0902\u091a \u0915\u0947\u0926\u093e\u0930\u094b\u0902 \u092e\u0947\u0902 \u0938\u0947 \u0930\u0941\u0926\u094d\u0930\u0928\u093e\u0925 \u0917\u094b\u092a\u0947\u0936\u094d\u200d\u0935\u0930 \u0938\u0947 \u0915\u0947\u0935\u0932 12 \u092e\u0940\u0932 \u0926\u0942\u0930 \u0939\u0948 ; \u0915\u093f\u0902\u0924\u0941 \u0935\u0939 \u092a\u0917\u0921\u0902\u0921\u0940 \u0915\u093e \u0915\u0920\u093f\u0928 \u092e\u093e\u0930\u094d\u0917 \u0939\u0948 
p855
aV\u0907\u0938 \u0915\u093e\u0930\u0923 \u0938\u0947 \u0915\u0947\u0935\u0932 \u092a\u0939\u093e\u0921\u093c\u0940 \u0932\u094b\u0917 \u0909\u0938 \u092e\u093e\u0930\u094d\u0917 \u0938\u0947 \u0930\u0941\u0926\u094d\u0930\u0928\u093e\u0925 \u091c\u093e\u0924\u0947 \u0939\u0948\u0902 
