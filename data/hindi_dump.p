aV\u091f\u093e\u0907\u092b\u093e\u0908\u0921 \u091c\u094d\u0935\u0930 \u092e\u0947\u0902 \u090f\u0902\u091f\u0940\u092c\u093e\u092f\u094b\u091f\u093f\u0915 \u0930\u094b\u0927\u0915\u0924\u093e \u092f\u0941\u0915\u094d\u0924 \u0938\u093e\u0932\u092e\u094b\u0928\u0947\u0932\u093e \u091f\u093f\u092b\u0940 \u0928\u0938\u094d\u0932 \u092a\u093e\u0908 \u091c\u093e\u0924\u0940 \u0939\u0948 
p48
aV\u0910\u0938\u0947 \u0907\u0932\u093e\u0915\u094b\u0902 \u0915\u0947 \u0938\u094d\u0915\u0942\u0932\u0940 \u092c\u091a\u094d\u091a\u094b\u0902 \u0914\u0930 \u092f\u0941\u0935\u093e \u0935 \u0935\u092f\u0938\u094d\u0915\u094b\u0902 \u0915\u094b \u092d\u0940 \u092f\u0939 \u091f\u0940\u0915\u093e \u0932\u0917\u0935\u093e\u0928\u0947 \u0915\u0940 \u0938\u093f\u092b\u093e\u0930\u093f\u0936 \u0915\u0940 \u091c\u093e\u0924\u0940 \u0939\u0948 , \u091c\u093f\u0928 \u0915\u094d\u0937\u0947\u0924\u094d\u0930\u094b\u0902 \u092e\u0947 \u0907\u0938 \u0906\u092f\u0941 \u0938\u092e\u0942\u0939 \u0915\u0947 \u0935\u094d\u092f\u0915\u094d\u0924\u093f\u092f\u094b\u0902 \u092e\u0947\u0902 \u091f\u093e\u0907\u092b\u093e\u0908\u0921 \u091c\u094d\u0935\u0930 \u090f\u0915 \u092a\u094d\u0930\u092e\u0941\u0916 \u0938\u093e\u0930\u094d\u0935\u091c\u0928\u093f\u0915 \u0938\u094d\u0935\u093e\u0938\u094d\u0925\u094d\u092f \u0938\u092e\u0938\u094d\u092f\u093e \u0915\u0947 \u0930\u0942\u092a \u092e\u0947 \u092e\u094c\u091c\u0942\u0926 \u0939\u0948 
p49
aV\u092a\u0940\u0924\u091c\u094d\u0935\u0930 \u0930\u094b\u0917 \u0915\u0947 \u0917\u0902\u092d\u0940\u0930 \u092e\u093e\u092e\u0932\u094b\u0902 \u092e\u0947 \u0924\u0940\u0935\u094d\u0930 \u091c\u094d\u0935\u0930 , \u0938\u0930\u094d\u0926\u0940 , \u0924\u094d\u0935\u091a\u093e \u0938\u0947 \u0930\u0915\u094d\u0924 \u0928\u093f\u0915\u0932\u0928\u093e , \u0924\u0940\u0935\u094d\u0930 \u0926\u093f\u0932 \u0915\u0940 \u0927\u0921\u093c\u0915\u0928 , \u0938\u093f\u0930\u0926\u0930\u094d\u0926 , \u0915\u092e\u0930 \u0926\u0930\u094d\u0926 \u0924\u0925\u093e \u092c\u0939\u0941\u092e\u0942\u0924\u094d\u0930 \u0939\u094b \u091c\u093e\u0924\u093e \u0939\u0948 
