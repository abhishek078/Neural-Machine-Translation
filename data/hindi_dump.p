aV\u0938\u093f\u0915\u094d\u0915\u093f\u092e \u092e\u0947\u0902 \u0905\u092c \u0924\u0915 \u0924\u094b \u0938\u0948\u0930-\u0938\u092a\u093e\u091f\u0947 \u0938\u0947 \u0939\u0940 \u092e\u093e\u0928\u0938\u093f\u0915 \u0938\u0941\u0915\u0942\u0928 \u092e\u093f\u0932\u0924\u093e \u0925\u093e \u0932\u0947\u0915\u093f\u0928 \u0905\u092c \u0935\u093f\u0936\u094d\u0935\u0938\u094d\u0924\u0930\u0940\u092f \u092a\u094d\u0930\u093e\u0915\u0943\u0924\u093f\u0915 \u091a\u093f\u0915\u093f\u0924\u094d\u0938\u093e \u0914\u0930 \u092e\u0938\u093e\u091c \u092a\u093e\u0930\u094d\u0932\u0930 \u0916\u094b\u0932\u0928\u0947 \u0915\u0940 \u092d\u0940 \u092f\u094b\u091c\u0928\u093e \u0924\u0948\u092f\u093e\u0930 \u0915\u0940 \u091c\u093e \u0930\u0939\u0940 \u0939\u0948 
p422
aV\u0907\u0938\u0947 \u092d\u0940 \u092a\u0930\u094d\u092f\u091f\u0928 \u0938\u0947 \u091c\u094b\u095c\u0928\u0947 \u0915\u0940 \u0924\u0948\u092f\u093e\u0930\u0940 \u091a\u0932 \u0930\u0939\u0940 \u0939\u0948 
p423
aV\u0924\u0940\u0938\u0930\u0940 \u2019 \u092c\u093e\u0923-\u0917\u0902\u0917\u093e \u2019 \u0915\u0941\u0930\u0941\u0915\u094d\u0937\u0947\u0924\u094d\u0930 \u0930\u0947\u0932\u0935\u0947 \u0938\u094d\u091f\u0947\u0936\u0928 \u0938\u0947 9 \u0915\u093f\u0932\u094b\u092e\u0940\u091f\u0930 \u0915\u0940 \u0926\u0942\u0930\u0940 \u092a\u0930 \u2019 \u091c\u092e\u0940\u0928 \u2019 \u0917\u093e\u0901\u0935 \u0915\u0947 \u0938\u092e\u0940\u092a \u0939\u0948 
p424
aV\u091a\u0942\u0901\u0915\u093f \u0938\u093f\u0915\u094d\u0915\u093f\u092e \u092e\u0947\u0902 \u0914\u0937\u0927\u0940\u092f \u0935\u0928\u0938\u094d\u092a\u0924\u093f\u092f\u094b\u0902 \u0915\u093e \u0905\u0902\u092c\u093e\u0930 \u0939\u0948 , \u0907\u0938\u0932\u093f\u090f \u0935\u093f\u092d\u093f\u0928\u094d\u0928 \u0935\u094d\u092f\u0935\u0938\u093e\u092f\u093f\u0915 \u0915\u094d\u0937\u0947\u0924\u094d\u0930\u094b\u0902 \u092e\u0947\u0902 \u0907\u0938\u0915\u0947 \u0909\u092a\u092f\u094b\u0917 \u0915\u094b \u0927\u094d\u092f\u093e\u0928 \u092e\u0947\u0902 \u0930\u0916\u0924\u0947 \u0939\u0941\u090f \u0935\u093f\u0936\u094d\u0935\u0938\u094d\u0924\u0930\u0940\u092f \u0935\u093f\u0936\u0947\u0937\u091c\u094d\u091e\u094b\u0902 \u0938\u0947 \u0907\u0938 \u0938\u0902\u092c\u0902\u0927 \u092e\u0947\u0902 \u092a\u0930\u093e\u092e\u0930\u094d\u0936 \u0932\u093f\u092f\u093e \u091c\u093e \u0930\u0939\u093e \u0939\u0948 
p425
aV\u0907\u0938\u0915\u0947 \u0924\u0939\u0924 \u0915\u0947\u0930\u0932 \u0915\u0940 \u0924\u0930\u094d\u091c \u092a\u0930 \u092a\u094d\u0930\u093e\u0915\u0943\u0924\u093f\u0915 \u0914\u0937\u0927\u0940\u092f \u0924\u0947\u0932\u094b\u0902 \u0938\u0947 \u092e\u093e\u0932\u093f\u0936 , \u091c\u0948\u0935\u093f\u0915 \u092e\u0932\u094b\u0902 \u0938\u0947 \u0909\u0924\u094d\u092a\u093e\u0926\u093f\u0924 \u0939\u0930\u0940 \u0938\u092c\u094d\u091c\u093f\u092f\u093e\u0901 \u0935 \u092a\u0915\u0935\u093e\u0928 \u0906\u0926\u093f \u092a\u0930\u094d\u092f\u091f\u0915\u094b\u0902 \u0915\u094b \u0909\u092a\u0932\u092c\u094d\u0927 \u0915\u0930\u093e\u092f\u0947 \u091c\u093e\u0928\u0947 \u0915\u0940 \u092f\u094b\u091c\u0928\u093e \u0924\u0948\u092f\u093e\u0930 \u0915\u0940 \u0917\u0908 \u0939\u0948 
p426
aV\u092e\u091b\u0932\u0940 \u0915\u0947 \u0924\u0947\u0932 \u0915\u094b \u0905\u092a\u0928\u0940 \u0921\u093e\u0907\u091f \u092e\u0947\u0902 \u0936\u093e\u092e\u093f\u0932 \u0915\u0930\u0947\u0902 
p427
aV\u0907\u0928\u092e\u0947\u0902 \u092b\u0948\u091f\u0940 \u090f\u0938\u093f\u0921\u094d\u200d\u0938 \u0939\u094b\u0924\u093e \u0939\u0948 , \u091c\u094b \u0939\u0949\u0930\u094d\u091f \u0905\u091f\u0948\u0915 \u0914\u0930 \u0915\u0948\u0902\u0938\u0930 \u0938\u0947 \u0932\u095c\u0928\u0947 \u092e\u0947\u0902 \u0938\u0939\u093e\u092f\u0924\u093e \u0915\u0930\u0924\u093e \u0939\u0948 
p428
aV\u092b\u0932 \u0914\u0930 \u0938\u092c\u094d\u091c\u093f\u092f\u094b\u0902 \u092e\u0947\u0902 \u0939\u094b\u0928\u0947 \u0935\u093e\u0932\u0940 \u0935\u0938\u093e \u0936\u0930\u0940\u0930 \u0915\u0947 \u0932\u093f\u090f \u0909\u092a\u092f\u094b\u0917\u0940 \u0939\u094b\u0924\u0940 \u0939\u0948 
p429
aV\u092f\u0939\u093e\u0901 \u0924\u0915 \u0915\u093f \u0906\u092a \u0938\u0939\u0940 \u0924\u0930\u0940\u0915\u0947 \u0938\u0947 \u0938\u093e\u0901\u0938 \u0932\u0947\u0928\u0947 \u0915\u0940 \u0906\u0926\u0924 \u0921\u093e\u0932\u0915\u0930 \u092a\u0947\u091f \u0915\u0940 \u091a\u0930\u094d\u092c\u0940 \u092d\u0940 \u0915\u092e \u0915\u0930 \u0938\u0915\u0924\u0940 \u0939\u0948\u0902 
p430
aV\u0907\u0938\u0938\u0947 \u092b\u0947\u092b\u095c\u093e \u0938\u093e\u092b \u0939\u094b\u0917\u093e , \u0915\u093f\u0938\u0940 \u092d\u0940 \u091a\u0940\u091c \u092a\u0930 \u0927\u094d\u092f\u093e\u0928 \u0915\u0947\u0902\u0926\u094d\u0930\u093f\u0924 \u0915\u0930\u0928\u0947 \u092e\u0947\u0902 \u092e\u0926\u0926 \u092e\u093f\u0932\u0947\u0917\u0940 
p431
aV\u0917\u0939\u0930\u0940 \u0938\u093e\u0901\u0938 \u0932\u0947\u0902 
p432
aV\u0916\u093e\u0928\u0947 \u0915\u093e \u0938\u094d\u0935\u093e\u0926 \u0928\u093f\u0916\u093e\u0930\u0928\u0947 \u0915\u0947 \u0938\u093e\u0925 \u0924\u0947\u091c\u092a\u0924\u094d\u0924\u093e \u0915\u0908 \u0924\u0930\u0939 \u0915\u0940 \u0938\u094d\u0935\u093e\u0938\u094d\u0925\u094d\u092f \u0938\u0902\u092c\u0902\u0927\u0940 \u092a\u0930\u0947\u0936\u093e\u0928\u0940 \u0938\u0947 \u092d\u0940 \u0928\u093f\u091c\u093e\u0924 \u0926\u093f\u0932\u093e\u0928\u0947 \u092e\u0947\u0902 \u092e\u0926\u0926 \u0915\u0930\u0924\u093e \u0939\u0948 
p433
aV\u0915\u093f\u0938\u0940 \u092d\u0940 \u0924\u0930\u0939 \u0915\u0947 \u092e\u094b\u091a \u0939\u094b\u0928\u0947 \u092a\u0930 \u0924\u0947\u091c\u092a\u0924\u094d\u0924\u093e \u0915\u094b \u092a\u0940\u0938\u0915\u0930 , \u0909\u0938 \u091c\u0917\u0939 \u092a\u0930 \u0932\u0917\u093e\u0915\u0930 \u0914\u0930 \u0909\u0938\u0915\u0947 \u090a\u092a\u0930 \u092a\u091f\u094d\u200d\u091f\u0940 \u092c\u093e\u0901\u0927 \u0926\u0947\u0928\u0947 \u0938\u0947 \u0915\u0941\u091b \u0939\u0940 \u0926\u093f\u0928\u094b\u0902 \u092e\u0947\u0902 \u0906\u0930\u093e\u092e \u092e\u093f\u0932 \u091c\u093e\u0924\u093e \u0939\u0948 
p434
aV\u092c\u093e\u0939\u0930 \u0928\u093f\u0915\u093e\u0932\u0915\u0930 \u092e\u0942\u0932\u092c\u0902\u0927 , \u0909\u0921\u094d\u0921\u0940\u092f\u093e\u0928 \u092c\u0902\u0927 \u090f\u0935\u0902 \u091c\u093e\u0932\u0902\u0927\u0930 \u092c\u0902\u0927 \u0932\u0917\u093e\u0915\u0930 \u0936\u094d\u200d\u0935\u093e\u0938 \u0915\u094b \u092f\u0925\u093e\u0936\u0915\u094d\u200d\u0924\u093f \u092c\u093e\u0939\u0930 \u0939\u0940 \u0930\u094b\u0915\u0915\u0930 \u0930\u0916\u0947\u0902 
p435
aV\u092c\u093e\u0939\u094d\u092f\u093e\u0935\u0943\u0924\u094d\u0924\u093f \u092a\u094d\u0930\u093e\u0923\u093e\u092f\u093e\u092e \u0935\u093f\u0927\u093f \u092e\u0947\u0902 \u0938\u093f\u0926\u094d\u0927\u093e\u0938\u0928 \u0935\u093e \u092a\u0926\u094d\u200d\u092e\u093e\u0938\u0928 \u092e\u0947\u0902 \u0935\u093f\u0927\u093f\u092a\u0942\u0930\u094d\u0935\u0915 \u092c\u0948\u0920\u0915\u0930 \u0936\u094d\u200d\u0935\u093e\u0938 \u0915\u094b \u090f\u0915 \u0939\u0940 \u092c\u093e\u0930 \u092e\u0947\u0902 \u092f\u0925\u093e\u0936\u0915\u094d\u200d\u0924\u093f \u092c\u093e\u0939\u0930 \u0928\u093f\u0915\u093e\u0932 \u0926\u0947\u0902 
p436
aV\u0936\u094d\u200d\u0935\u093e\u0938 \u092d\u0940\u0924\u0930 \u0932\u0947\u0915\u0930 \u0909\u0938\u0947 \u092c\u093f\u0928\u093e \u0930\u094b\u0915\u0947 \u0939\u0940 \u092a\u0941\u0928: \u092a\u0942\u0930\u094d\u0935\u0935\u0924\u094d \u0936\u094d\u200d\u0935\u0938\u0928-\u0915\u094d\u0930\u093f\u092f\u093e \u0915\u0930\u0947\u0902 
p437
aV\u091c\u092c \u0936\u094d\u200d\u0935\u093e\u0938 \u0932\u0947\u0928\u0947 \u0915\u0940 \u0907\u091a\u094d\u091b\u093e \u0939\u094b \u0924\u094b \u092c\u0902\u0927\u094b\u0902 \u0915\u094b \u0939\u091f\u093e\u0924\u0947 \u0939\u0941\u090f \u0927\u0940\u0930\u0947-\u0927\u0940\u0930\u0947 \u0936\u094d\u200d\u0935\u093e\u0938 \u0932\u0947\u0902 
p438
aV\u092a\u094d\u0930\u0923\u0935 \u090f\u0935\u0902 \u0917\u093e\u092f\u0924\u094d\u0930\u0940 \u0906\u0926\u093f \u092e\u0902\u0924\u094d\u0930\u094b\u0902 \u0915\u093e \u0926\u0940\u0930\u094d\u0918\u0915\u093e\u0932 \u0924\u0915 \u0936\u094d\u0930\u0926\u094d\u0927\u093e\u092a\u0942\u0930\u094d\u0935\u0915 \u0915\u093f\u092f\u093e \u0939\u0941\u0906 \u091c\u092a \u092a\u0930\u092e\u0936\u0915\u094d\u200d\u0924\u093f , \u0936\u093e\u0902\u0924\u093f \u090f\u0935\u0902 \u0906\u0928\u0902\u0926\u0926\u093e\u092f\u0915 \u0939\u094b\u0924\u093e \u0939\u0948 
p439
aV\u0936\u094c\u091a , \u0938\u094d\u0928\u093e\u0928 , \u0906\u0938\u0928\u093e\u0926\u093f \u0928\u093f\u0924\u094d\u092f\u0915\u0930\u094d\u092e\u094b\u0902 \u0938\u0947 \u0928\u093f\u0935\u0943\u0924\u094d\u0924 \u0939\u094b\u0915\u0930 \u0938\u0941\u0916 , \u0936\u093e\u0902\u0924\u093f \u090f\u0935\u0902 \u0906\u0928\u0902\u0926 \u0915\u0940 \u0915\u093e\u092e\u0928\u093e \u0930\u0916\u0928\u0947 \u0935\u093e\u0932\u0947 \u0935\u094d\u092f\u0915\u094d\u200d\u0924\u093f \u092a\u094d\u0930\u0924\u093f\u0926\u093f\u0928 \u0915\u092e \u0938\u0947 \u0915\u092e 15 \u092e\u093f\u0928\u091f \u0938\u0947 \u0932\u0947\u0915\u0930 \u090f\u0915 \u0918\u0902\u091f\u0947 \u0924\u0915 \u092d\u0917\u0935\u093e\u0928 \u0915\u093e \u0927\u094d\u092f\u093e\u0928 , \u0909\u092a\u093e\u0938\u0928\u093e \u0905\u0935\u0936\u094d\u092f \u0915\u0930\u0947\u0902 
p440
aV\u0907\u0938\u0915\u0947 \u0928\u093f\u092f\u092e\u093f\u0924 \u0938\u0947\u0935\u0928 \u0938\u0947 \u092b\u093f\u091f \u0930\u0939\u0947\u0902\u0917\u0940 \u0914\u0930 \u092c\u095d\u0924\u0940 \u0909\u092e\u094d\u0930 \u0915\u0940 \u0928\u093f\u0936\u093e\u0928\u093f\u092f\u093e\u0901 \u092d\u0940 \u0906\u092a\u0938\u0947 \u0926\u0942\u0930 \u0930\u0939\u0947\u0902\u0917\u0940 
p441
aV\u0915\u0948\u0932\u094d\u0936\u093f\u092f\u092e \u092e\u0939\u093f\u0932\u093e\u0913\u0902 \u0915\u0947 \u0932\u093f\u090f \u091c\u0930\u0942\u0930\u0940 \u0939\u0948 , \u092a\u0930 \u0938\u093f\u0930\u094d\u092b \u0939\u0921\u094d\u200d\u0921\u093f\u092f\u094b\u0902 \u0915\u094b \u092e\u091c\u092c\u0942\u0924 \u092c\u0928\u093e\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u0928\u0939\u0940\u0902 , \u092c\u0932\u094d\u0915\u093f \u0938\u0902\u092a\u0942\u0930\u094d\u0923 \u0938\u094d\u0935\u093e\u0938\u094d\u0925\u094d\u092f \u0915\u0947 \u0932\u093f\u090f \u092d\u0940 
p442
aV\u092b\u093f\u091c\u093f\u092f\u094b\u0925\u0947\u0930\u0948\u092a\u0940 \u0938\u094d\u091f\u094d\u0930\u0947\u0902\u0925 \u0915\u0947 \u0938\u093e\u0925 \u0938\u094d\u091f\u0947\u092e\u093f\u0928\u093e \u092d\u0940 \u0926\u0947\u0924\u0940 \u0939\u0948 
p443
aV\u0906\u091c\u0915\u0932 \u092c\u091a\u094d\u091a\u0947 \u091a\u094c\u092c\u0940\u0938\u094b\u0902 \u0918\u0902\u091f\u0947 \u091f\u0940\u0935\u0940 \u0915\u0947 \u0906\u0917\u0947 \u092c\u0948\u0920\u0947 \u0930\u0939\u0924\u0947 \u0939\u0948\u0902 , \u091c\u093f\u0938\u0938\u0947 \u0909\u0928\u094d\u0939\u0947\u0902 \u0915\u0908 \u0924\u0930\u0939 \u0915\u0940 \u0938\u092e\u0938\u094d\u092f\u093e\u090f\u0901 \u0939\u094b \u091c\u093e\u0924\u0940 \u0939\u0948\u0902 
p444
aV\u0939\u092e\u093e\u0930\u093e \u092e\u093e\u0928\u0928\u093e \u0939\u0948 \u0915\u093f \u0939\u0930 \u0915\u093f\u0938\u0940 \u0915\u0940 \u0905\u092a\u0928\u0940 \u0915\u0926\u0915\u093e\u0920\u0940 \u0939\u094b\u0924\u0940 \u0939\u0948 \u0907\u0938\u0932\u093f\u090f \u0939\u092e \u092a\u0939\u0932\u0947 \u092e\u0930\u0940\u091c \u0915\u093e \u092b\u093f\u091c\u093f\u0915\u0932\u0940 \u090f\u0915\u094d\u091c\u093e\u092e\u093f\u0928 \u0915\u0930\u0924\u0947 \u0939\u0948\u0902 \u092b\u093f\u0930 \u0909\u0938\u0915\u0947 \u0939\u093f\u0938\u093e\u092c \u0938\u0947 \u090f\u0915\u094d\u0938\u0930\u0938\u093e\u0907\u091c \u0935 \u091f\u094d\u0930\u0940\u091f\u092e\u0947\u0902\u091f \u0926\u0947\u0924\u0947 \u0939\u0948\u0902 
p445
aV\u092f\u094b\u0917\u093e \u0915\u0947 \u091c\u0930\u093f\u090f \u0906\u092a\u0915\u0947 \u0936\u0930\u0940\u0930 \u0915\u094b \u0936\u0915\u094d\u200d\u0924\u093f \u0924\u094b \u092e\u093f\u0932 \u091c\u093e\u0924\u0940 \u0939\u0948 \u0932\u0947\u0915\u093f\u0928 \u0909\u0938\u0947 \u092c\u0930\u0915\u0930\u093e\u0930 \u0930\u0916\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u092b\u093f\u091c\u093f\u092f\u094b\u0925\u0947\u0930\u0948\u092a\u0940 \u0915\u0940 \u092c\u0939\u0941\u0924 \u091c\u0930\u0942\u0930\u0924 \u0939\u094b\u0924\u0940 \u0939\u0948 
