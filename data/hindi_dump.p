aV\u092a\u0936\u094d\u200d\u091a\u093f\u092e\u0940 \u0924\u0925\u093e \u0905\u0928\u094d\u092f \u090f\u0936\u093f\u092f\u093e\u0908 \u0932\u094b\u0917\u094b\u0902 \u0915\u0940 \u0924\u0941\u0932\u0928\u093e \u092e\u0947\u0902 \u092d\u093e\u0930\u0924\u0940\u092f\u094b\u0902 \u092e\u0947\u0902 \u0938\u0940.\u090f.\u0921\u0940. \u0915\u093e \u092a\u094d\u0930\u092d\u093e\u0935 \u0926\u094b \u0938\u0947 \u091a\u093e\u0930 \u0917\u0941\u0928\u093e \u091c\u094d\u092f\u093e\u0926\u093e \u0939\u0948 \u0924\u0925\u093e \u091a\u093e\u0932\u0940\u0938 \u0938\u0947 \u0915\u092e \u0909\u092e\u094d\u0930 \u0915\u0947 \u0935\u094d\u092f\u0915\u094d\u200d\u0924\u093f\u092f\u094b\u0902 \u092e\u0947\u0902 \u0907\u0938\u0915\u093e \u092a\u094d\u0930\u092d\u093e\u0935 \u092a\u093e\u0901\u091a \u0938\u0947 \u0926\u0938 \u0917\u0941\u0928\u093e \u091c\u094d\u092f\u093e\u0926\u093e \u0939\u0948 
p560
aV\u0905\u0928\u0941\u092e\u093e\u0928 \u0932\u0917\u093e\u092f\u093e \u0917\u092f\u093e \u0939\u0948 \u0915\u093f \u0909\u0924\u094d\u0924\u0930 \u092d\u093e\u0930\u0924 \u0915\u0947 \u092c\u0921\u093c\u0947 \u0936\u0939\u0930\u094b\u0902 \u092e\u0947\u0902 \u0930\u0939\u0928\u0947 \u0935\u093e\u0932\u0947 7 - 10 \u092a\u094d\u0930\u0924\u093f\u0936\u0924 \u0914\u0930 \u0926\u0915\u094d\u0937\u093f\u0923 \u092d\u093e\u0930\u0924 \u0915\u0947 \u092c\u0921\u093c\u0947 \u0936\u0939\u0930\u094b\u0902 \u092e\u0947\u0902 \u0930\u0939\u0928\u0947 \u0935\u093e\u0932\u0947 14 \u092a\u094d\u0930\u0924\u093f\u0936\u0924 \u0932\u094b\u0917 , \u0938\u0940.\u090f.\u0921\u0940. \u0938\u0947 \u092a\u0940\u0921\u093c\u093f\u0924 \u0939\u0948\u0902 
p561
aV\u0926\u093f\u0932 \u0915\u0947 \u0930\u094b\u0917\u094b\u0902 \u092e\u0947\u0902 \u0938\u0940.\u090f.\u0921\u0940. \u0938\u092c\u0938\u0947 \u0905\u0927\u093f\u0915 \u092a\u093e\u092f\u0940 \u091c\u093e\u0928\u0947 \u0935\u093e\u0932\u0940 \u092c\u0940\u092e\u093e\u0930\u0940 \u0939\u0948 \u091c\u094b \u091b\u093e\u0924\u0940 \u092e\u0947\u0902 \u0926\u0930\u094d\u0926 , \u0926\u093f\u0932 \u0915\u0947 \u0926\u094c\u0930\u0947 \u092f\u093e \u0905\u091a\u093e\u0928\u0915 \u092e\u094c\u0924 \u0915\u0947 \u0930\u0942\u092a \u092e\u0947\u0902 \u0935\u094d\u092f\u0915\u094d\u200d\u0924 \u0939\u094b\u0924\u0940 \u0939\u0948 
p562
aV\u0926\u093f\u0932 \u0928\u093e\u0921\u093c\u093f\u092f\u094b\u0902 \u092e\u0947\u0902 \u0927\u094c\u0902\u0915\u0928\u0940 \u0915\u0940 \u0924\u0930\u0939 \u0916\u0942\u0928 \u092a\u092e\u094d\u092a \u0915\u0930\u0924\u093e \u0939\u0948 
p563
aV\u0907\u0938\u0915\u0947 \u0926\u094d\u0935\u093e\u0930\u093e \u0916\u0942\u0928 \u092e\u0947\u0902 \u092e\u093f\u0932\u0947 \u0939\u0941\u092f\u0947 \u092a\u094c\u0937\u094d\u200d\u091f\u093f\u0915 \u0924\u0924\u094d\u0924\u094d\u0935 \u0914\u0930 \u0911\u0915\u094d\u0938\u0940\u091c\u0928 \u0936\u0930\u0940\u0930 \u0915\u0947 \u0938\u093e\u0930\u0947 \u0939\u093f\u0938\u094d\u0938\u094b\u0902 \u092e\u0947\u0902 \u092a\u0939\u0941\u0901\u091a\u0924\u0947 \u0939\u0948\u0902 
p564
aV\u0915\u0939\u0924\u0947 \u0939\u0948\u0902 \u092a\u0940\u0930 \u092c\u093e\u092c\u093e \u0915\u0940 \u0926\u0930\u0917\u093e\u0939 \u0905\u092a\u0928\u0947 \u0932\u094b\u0917\u094b\u0902 \u0915\u094b \u092c\u0941\u0930\u0940 \u0906\u0924\u094d\u092e\u093e\u0913\u0902 \u0914\u0930 \u0926\u0941\u0930\u094d\u0918\u091f\u0928\u093e\u0913\u0902 \u0938\u0947 \u092c\u091a\u093e\u0924\u0940 \u0939\u0948 
p565
aV\u0917\u0941\u0930\u0941 \u0917\u094b\u0935\u093f\u0902\u0926 \u0938\u093f\u0902\u0939 \u0915\u0947 \u092e\u093f\u0924\u094d\u0930 \u092a\u0940\u0930 \u092c\u093e\u092c\u093e \u0928\u0947 \u091c\u0940\u0935\u0928\u092d\u0930 \u0938\u093f\u0930\u094d\u092b \u0926\u0942\u0927 \u0915\u093e \u0939\u0940 \u0938\u0947\u0935\u0928 \u0915\u093f\u092f\u093e \u0925\u093e \u0914\u0930 500 \u0935\u0930\u094d\u0937 \u0915\u0940 \u0905\u0935\u0938\u094d\u0925\u093e \u092e\u0947\u0902 \u0905\u092a\u0928\u0940 \u0926\u0947\u0939 \u091b\u094b\u095c\u0940 \u0925\u0940 
p566
aV\u0909\u0926\u093e\u0939\u0930\u0923 \u0915\u0947 \u0932\u093f\u090f \u0905\u092b\u094d\u0930\u0940\u0915\u0940 \u091c\u0947\u092c\u094d\u0930\u093e , \u091c\u093f\u0930\u093e\u092b , \u0936\u0941\u0924\u0941\u0930\u092e\u0941\u0930\u094d\u0917 , \u0907\u0902\u092a\u093e\u0932\u093e , \u091a\u0940\u0924\u093e , \u0936\u0947\u0930 , \u0939\u093e\u0925\u0940 , \u0917\u0948\u0902\u0921\u093e \u0906\u0926\u093f \u0915\u0947 \u0928\u093e\u092e \u0917\u093f\u0928\u093e\u090f \u091c\u093e \u0938\u0915\u0924\u0947 \u0939\u0948\u0902 
p567
aV\u0907\u0928\u094d\u0939\u0947\u0902 \u0926\u0947\u0916\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u092f\u0942\u0930\u094b\u092a , \u0905\u092e\u0947\u0930\u093f\u0915\u093e , \u0915\u0928\u093e\u0921\u093e , \u0911\u0938\u094d\u091f\u094d\u0930\u0947\u0932\u093f\u092f\u093e \u0915\u0947 \u092a\u0930\u094d\u092f\u091f\u0915 \u091f\u0942\u091f \u0938\u0947 \u092a\u0921\u093c\u0924\u0947 \u0939\u0948\u0902 
p568
aV\u0939\u093f\u0902\u0926\u0940 \u0924\u0925\u093e \u0917\u0941\u091c\u0930\u093e\u0924\u0940 \u0924\u094b \u2019 \u0928\u0948\u0930\u094b\u092c\u0940 \u2019 \u0915\u0940 \u0917\u0932\u0940-\u0917\u0932\u0940 \u092e\u0947\u0902 \u0938\u0941\u0928\u093e\u0908 \u092a\u095c\u0924\u0940 \u0939\u0948 \u0914\u0930 \u092d\u093e\u0930\u0924\u0940\u092f \u0938\u093e\u095c\u0940 , \u0932\u0939\u0901\u0917\u093e \u0906\u0926\u093f \u0935\u0939\u093e\u0901 \u090f\u0915 \u0906\u092e \u092a\u0939\u0928\u093e\u0935\u093e \u0939\u0948 
p569
aV\u0928\u0948\u0930\u094b\u092c\u0940 \u0915\u0947 \u092c\u093e\u0930\u0947 \u092e\u0947\u0902 \u0925\u094b\u095c\u0940 \u092c\u0939\u0941\u0924 \u0938\u093e\u0935\u0927\u093e\u0928\u0940 \u092c\u0930\u0924\u0928\u0947 \u0915\u0940 \u092d\u0940 \u0938\u0932\u093e\u0939 \u0926\u0940 \u091c\u093e\u0924\u0940 \u0939\u0948 
p570
aV\u0928\u0948\u0930\u094b\u092c\u0940 \u092e\u0947\u0902 \u091a\u094b\u0930\u0940 , \u091b\u0940\u0928\u093e-\u091d\u092a\u091f\u0940 , \u0927\u094b\u0916\u093e\u0927\u095c\u0940 \u0924\u0925\u093e \u0932\u0942\u091f\u092a\u093e\u091f \u0906\u0926\u093f \u0915\u0940 \u0905\u0928\u0947\u0915 \u0918\u091f\u0928\u093e\u090f\u0901 \u0939\u094b\u0924\u0940 \u0930\u0939\u0924\u0940 \u0939\u0948\u0902 
p571
aV\u0905\u0924\u0903 \u092a\u0930\u094d\u092f\u091f\u0915\u094b\u0902 \u0915\u094b \u0905\u0915\u0947\u0932\u0947 \u0915\u0939\u0940\u0902 \u0905\u0928\u091c\u093e\u0928\u0947 \u0938\u094d\u0925\u093e\u0928 \u092a\u0930 \u0906\u0928\u0947-\u091c\u093e\u0928\u0947 \u0938\u0947 \u092c\u091a\u0928\u093e \u091a\u093e\u0939\u093f\u090f 
p572
aV\u0928\u0948\u0930\u094b\u092c\u0940 \u0915\u0940 \u0938\u092c \u0938\u0947 \u092c\u095c\u0940 \u0935\u093f\u0936\u0947\u0937\u0924\u093e \u0935\u0939\u093e\u0901 \u0915\u0947 \u0930\u093e\u0937\u094d\u200d\u091f\u094d\u0930\u0940\u092f \u092a\u093e\u0930\u094d\u0915 \u0939\u0948\u0902 , \u091c\u094b \u0932\u0917\u092d\u0917 \u0928\u0917\u0930 \u0915\u0947 \u0926\u0915\u094d\u0937\u093f\u0923 \u092e\u0947\u0902 \u0915\u0947\u0935\u0932 7 \u0915\u093f\u0932\u094b\u092e\u0940\u091f\u0930 \u0915\u0940 \u0926\u0942\u0930\u0940 \u092a\u0930 \u0938\u094d\u0925\u093f\u0924 \u0939\u0948\u0902 
p573
aV\u0935\u093e\u0938\u094d\u0924\u0935 \u092e\u0947\u0902 \u091c\u092c \u0928\u0948\u0930\u094b\u092c\u0940 \u090f\u0905\u0930\u092a\u094b\u0930\u094d\u091f \u092a\u0930 \u0935\u093f\u092e\u093e\u0928 \u0909\u0924\u0930\u0924\u093e \u0939\u0948 \u0924\u094b \u092f\u0926\u093f \u092f\u093e\u0924\u094d\u0930\u0940 \u0916\u093f\u095c\u0915\u0940 \u0938\u0947 \u092c\u093e\u0939\u0930 \u0926\u0947\u0916\u0947 \u0924\u094b \u0939\u094b \u0938\u0915\u0924\u093e \u0915\u093f \u0909\u0928\u094d\u0939\u0947\u0902 \u0915\u094b\u0908 \u091c\u093f\u0930\u093e\u092b \u0938\u0930 \u0909\u0920\u093e\u090f \u092d\u093e\u0917\u0924\u093e \u0926\u093f\u0916 \u091c\u093e\u090f 
p574
aV\u0928\u0948\u0930\u094b\u092c\u0940 \u0915\u093e \u090f\u0915 \u092a\u094d\u0930\u092e\u0941\u0916 \u0930\u093e\u0937\u094d\u200d\u091f\u094d\u0930\u0940\u092f \u092a\u093e\u0930\u094d\u0915 \u0939\u0948 
p575
aV\u092f\u0939 \u0930\u093e\u0937\u094d\u200d\u091f\u094d\u0930\u0940\u092f \u092a\u093e\u0930\u094d\u0915 \u0915\u0947\u0928\u094d\u092f\u093e \u0915\u093e \u092a\u094d\u0930\u092e\u0941\u0916 \u0909\u0926\u094d\u092f\u094b\u0917 \u092a\u0930\u094d\u092f\u091f\u0928 \u0939\u0948 , \u091c\u094b \u0907\u0928\u094d\u0939\u0940\u0902 \u0905\u0928\u094b\u0916\u0947 \u092a\u0936\u0941\u0913\u0902 \u092a\u0930 \u0928\u093f\u0930\u094d\u092d\u0930 \u0915\u0930\u0924\u093e \u0939\u0948 \u0914\u0930 \u092f\u0947 \u092a\u0936\u0941 \u0910\u0938\u0947 \u0939\u0948\u0902 \u091c\u094b \u0915\u0947\u0935\u0932 \u0905\u092b\u094d\u0930\u0940\u0915\u093e \u092e\u0947\u0902 \u0939\u0940 \u092a\u093e\u090f \u091c\u093e\u0924\u0947 \u0939\u0948\u0902 
p576
aV\u0926\u094b\u0928\u094b\u0902 \u092a\u0902\u0916\u094b\u0902 \u0915\u094b \u092b\u0948\u0932\u093e\u092f\u0947 \u0917\u093f\u0926\u094d\u0927-\u092a\u0915\u094d\u0937\u0940 \u0915\u0947 \u0938\u092e\u093e\u0928 \u0926\u093f\u0916\u0928\u0947 \u0935\u093e\u0932\u0940 \u0917\u0943\u0926\u094d\u0927-\u0915\u0942\u091f \u092a\u0939\u093e\u095c\u0940 \u092a\u0930 \u092c\u0941\u0926\u094d\u0927 \u0928\u0947 \u0938\u092e\u094d\u0930\u093e\u091f \u092c\u093f\u092e\u094d\u092c\u093f\u0938\u093e\u0930 \u0915\u094b \u092c\u094c\u0926\u094d\u0927 \u0927\u0930\u094d\u092e \u0915\u0940 \u0926\u0940\u0915\u094d\u0937\u093e \u092a\u094d\u0930\u0926\u093e\u0928 \u0915\u0940 \u0925\u0940 
p577
aV\u0917\u0943\u0926\u094d\u0927-\u0915\u0942\u091f \u0938\u0947 \u0938\u091f\u0947 , \u0930\u0924\u094d\u200d\u0928 \u0917\u093f\u0930\u0940 \u0928\u093e\u092e\u0915 \u092a\u0939\u093e\u095c\u0940 \u0915\u0947 \u0936\u093f\u0916\u0930 \u092a\u0930 \u091c\u093e\u092a\u093e\u0928 \u0926\u094d\u0935\u093e\u0930\u093e \u090f\u0915 \u0938\u0941\u0928\u094d\u0926\u0930 \u0935\u093f\u0936\u094d\u200d\u0935 \u0936\u093e\u0902\u0924\u093f \u0938\u094d\u0924\u0942\u092a \u0928\u093f\u0930\u094d\u092e\u093f\u0924 \u0939\u0948 
p578
aV\u0917\u0943\u0926\u094d\u0927-\u0915\u0942\u091f \u092a\u0939\u093e\u095c\u0940 \u0915\u0947 \u0924\u0932 \u0938\u0947 \u0936\u093f\u0916\u0930 \u092a\u0930 \u092a\u0939\u0941\u0901\u091a\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u092c\u093f\u0939\u093e\u0930 \u092a\u0930\u094d\u092f\u091f\u0928 \u0926\u094d\u0935\u093e\u0930\u093e \u090f\u0915 \u0906\u0915\u093e\u0936\u0940\u092f \u0930\u091c\u094d\u091c\u0941\u092a\u0925 \u0938\u0947\u0935\u093e \u092d\u0940 \u0938\u0902\u091a\u093e\u0932\u093f\u0924 \u0939\u0948 
p579
aV\u092c\u093f\u092e\u094d\u092c\u093f\u0938\u093e\u0930 \u0915\u093e\u0930\u093e\u0917\u093e\u0930 - \u0905\u092a\u0928\u0947 \u092a\u093f\u0924\u093e \u0938\u092e\u094d\u0930\u093e\u091f \u092c\u093f\u092e\u094d\u092c\u093f\u0938\u093e\u0930 \u0915\u094b \u0938\u093f\u0902\u0939\u093e\u0938\u0928-\u091a\u094d\u092f\u0941\u0924 \u0915\u0930 \u0905\u091c\u093e\u0924\u0936\u0924\u094d\u0930\u0941 \u0928\u0947 \u0907\u0938\u0940 \u0938\u094d\u0925\u093e\u0928 \u092a\u0930 \u092c\u0928\u094d\u0926\u0940 \u092c\u0928\u093e\u0915\u0930 \u0930\u0916\u093e \u0925\u093e 
p580
aV\u092e\u0923\u093f\u092f\u093e\u0930\u092e\u0920 - \u092a\u0941\u0930\u093e\u0924\u0924\u094d\u0924\u094d\u0935\u0947\u0924\u094d\u0924\u093e\u0913\u0902 \u0926\u094d\u0935\u093e\u0930\u093e \u0915\u0942\u092a \u0915\u0947 \u0906\u0915\u093e\u0930 \u0915\u0940 \u092f\u0939 \u0938\u0902\u0930\u091a\u0928\u093e \u092a\u094d\u0930\u093e\u091a\u0940\u0928 \u091c\u0948\u0928-\u092a\u0942\u091c\u0928 \u0938\u094d\u0925\u0932\u0940 \u092e\u093e\u0928\u0940 \u091c\u093e\u0924\u0940 \u0939\u0948 
p581
aV\u0938\u094b\u0928 \u092d\u0923\u094d\u0921\u093e\u0930 - \u092a\u0939\u093e\u095c\u0940 \u0915\u094b \u0915\u093e\u091f \u0915\u0930 \u092c\u0928\u093e\u0908 \u0917\u092f\u0940 \u0907\u0938 \u0917\u0941\u092b\u093e , \u091c\u093f\u0938\u0947 \u0938\u094d\u0935\u0930\u094d\u0923-\u092d\u0923\u094d\u0921\u093e\u0930 \u092d\u0940 \u0915\u0939\u093e \u091c\u093e\u0924\u093e \u0939\u0948 , \u092e\u0947\u0902 \u091c\u0948\u0928 \u0924\u0940\u0930\u094d\u0925\u0915\u093e\u0930\u094b\u0902 \u0915\u0940 \u092a\u094d\u0930\u093e\u091a\u0940\u0928 \u092e\u0942\u0930\u094d\u0924\u093f\u092f\u093e\u0901 \u0938\u094d\u0925\u093e\u092a\u093f\u0924 \u0939\u0948\u0902 
p582
aV\u0938\u094d\u0935\u0930\u094d\u0923-\u092d\u0923\u094d\u0921\u093e\u0930 \u0938\u0947 \u092a\u094d\u0930\u093e\u092f\u0903 1.6 \u0915\u093f.\u092e\u0940. \u0926\u0915\u094d\u0937\u093f\u0923-\u092a\u0936\u094d\u091a\u093f\u092e \u0926\u093f\u0936\u093e \u092e\u0947\u0902 \u090f\u0915 \u0938\u094d\u0925\u093e\u0928 `` \u0905\u0916\u093e\u095c\u093e `` \u092f\u093e \u0930\u0923-\u092d\u0942\u092e\u093f \u0915\u0947 \u0928\u093e\u092e \u0938\u0947 \u091c\u094d\u091e\u093e\u0924 \u0939\u0948 
p583
aV\u0915\u0939\u093e \u091c\u093e\u0924\u093e \u0939\u0948 \u0915\u093f , \u092a\u094c\u0930\u093e\u0923\u093f\u0915 \u092e\u0939\u093e\u092d\u093e\u0930\u0924-\u0915\u093e\u0932 \u0915\u0947 \u0926\u094b \u092a\u093e\u0924\u094d\u0930\u094b\u0902 - \u092d\u0940\u092e \u0914\u0930 \u091c\u0930\u093e\u0938\u0928\u094d\u0927 \u0915\u0947 \u092c\u0940\u091a \u0907\u0938\u0940 \u0905\u0916\u093e\u095c\u0947 \u092a\u0930 \u0926\u094d\u0935\u0928\u094d\u0926 \u092f\u0941\u0926\u094d\u0927 \u0939\u0941\u0906 \u0925\u093e 
p584
aV\u0935\u0948\u092d\u093e\u0930-\u0917\u093f\u0930\u0940 \u0928\u093e\u092e \u0938\u0947 \u091c\u094d\u091e\u093e\u0924 \u092a\u0939\u093e\u095c\u0940 \u0915\u0940 \u0924\u0932\u0939\u091f\u0940 \u092e\u0947\u0902 \u0905\u0935\u0938\u094d\u0925\u093f\u0924 \u0917\u0930\u094d\u092e-\u091c\u0932 \u0915\u0947 \u091d\u0930\u0928\u094b\u0902 \u0938\u0947 \u0939\u094b\u0915\u0930 \u090a\u092a\u0930 \u0915\u0940 \u0913\u0930 \u091a\u095d\u093e\u0908 \u0915\u0930\u0924\u0947 \u0938\u092e\u092f \u092a\u0924\u094d\u0925\u0930 \u0915\u0947 \u091f\u0941\u0915\u095c\u094b\u0902 \u0938\u0947 \u0928\u093f\u0930\u094d\u092e\u093f\u0924 \u090f\u0915 \u092a\u094d\u0930\u093e\u091a\u0940\u0928 \u092c\u0943\u0939\u0926 \u0938\u0902\u0930\u091a\u0928\u093e \u0926\u093f\u0916\u093e\u0908 \u0926\u0947\u0924\u0940 \u0939\u0948 
p585
aV\u0915\u0939\u093e \u091c\u093e\u0924\u093e \u0939\u0948 \u0915\u093f \u092f\u0939 \u0938\u094d\u0925\u093e\u0928 \u092a\u094c\u0930\u093e\u0923\u093f\u0915 \u092e\u0939\u093e\u092d\u093e\u0930\u0924-\u0915\u093e\u0932 \u0915\u0947 \u092a\u093e\u0924\u094d\u0930 \u091c\u0930\u093e\u0938\u0928\u094d\u0927 \u0915\u0940 \u092c\u0948\u0920\u0915 \u0925\u0940 
p586
aV\u092e\u093e\u0928\u0938 \u0930\u093e\u0937\u094d\u200d\u091f\u094d\u0930\u0940\u092f \u0909\u0926\u094d\u092f\u093e\u0928 \u0915\u0947 \u2019 \u0935\u0928\u0936\u093e\u0930\u0940 \u2019 \u0914\u0930 \u2019 \u092e\u094b\u0924\u093e\u0917\u0941\u095c\u0940 \u2019 \u092e\u0947\u0902 \u0935\u093f\u0936\u094d\u0930\u093e\u092e \u0917\u0943\u0939 \u0939\u0948\u0902 
p587
aV\u092e\u093e\u0928\u0938 \u0930\u093e\u0937\u094d\u200d\u091f\u094d\u0930\u0940\u092f \u0909\u0926\u094d\u092f\u093e\u0928 \u0915\u0947 \u0926\u0943\u0936\u094d\u092f\u094b\u0902 \u0915\u093e \u0905\u0935\u0932\u094b\u0915\u0928 \u0915\u0930\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u092f\u0939\u093e\u0901 \u0915\u0948\u0902\u092a \u092d\u0940 \u092c\u0928\u093e\u090f \u0917\u090f \u0939\u0948\u0902 
p588
aV\u092a\u094d\u0930\u093e\u092f\u0903 \u092e\u091a\u094d\u091b\u0930 \u0930\u093e\u0924\u094d\u0930\u093f \u092e\u0947\u0902 \u0915\u093e\u091f\u0924\u0947 \u0939\u0948\u0902 
p589
aV\u092e\u0932\u0947\u0930\u093f\u092f\u093e \u092f\u0915\u0943\u0924 \u0924\u0925\u093e \u0915\u0947\u0928\u094d\u0926\u094d\u0930\u0940\u092f \u0938\u094d\u0928\u093e\u092f\u0941 \u0938\u0902\u0938\u094d\u0925\u093e\u0928 \u0915\u094b \u092a\u094d\u0930\u092d\u093e\u0935\u093f\u0924 \u0915\u0930 \u092e\u0942\u0930\u094d\u091a\u094d\u091b\u093e \u0914\u0930 \u0915\u092d\u0940-\u0915\u092d\u0940 \u092e\u0943\u0924\u094d\u092f\u0941 \u0915\u093e\u0930\u0923 \u092d\u0940 \u092c\u0928\u0924\u093e \u0939\u0948 
p590
aV\u092e\u0932\u0947\u0930\u093f\u092f\u093e \u0915\u0947 \u0932\u0915\u094d\u0937\u0923\u094b\u0902 \u092e\u0947\u0902 \u0924\u0940\u0935\u094d\u0930 \u091c\u094d\u0935\u0930 , \u091c\u0940 \u092e\u091a\u0932\u0928\u093e , \u0938\u093f\u0930\u0926\u0930\u094d\u0926 \u0924\u0925\u093e \u092c\u0939\u0941\u0924 \u0924\u0947\u091c \u092a\u0938\u0940\u0928\u093e \u0939\u094b\u0924\u093e \u0939\u0948 
p591
aV\u092e\u0932\u0947\u0930\u093f\u092f\u093e \u0930\u094b\u0917 \u090f\u0915 \u0935\u094d\u092f\u0915\u094d\u0924\u093f \u0938\u0947 \u0926\u0942\u0938\u0930\u0947 \u0935\u094d\u092f\u0915\u094d\u0924\u093f \u092e\u0947\u0902 \u090f\u0928\u093e\u092b\u093f\u0932\u0940\u091c \u0915\u0941\u0932 \u0915\u0940 \u091a\u093e\u0930 \u0909\u092a\u091c\u093e\u0924\u093f \u0915\u0947 \u092e\u091a\u094d\u091b\u0930\u094b\u0902 , \u092f\u0925\u093e \u092a\u094d\u0932\u093e\u091c\u092e\u094b\u0921\u093f\u092f\u092e \u092b\u093e\u0932\u0938\u0940\u092a\u0947\u0930\u092e , \u092a\u094d\u0932\u093e\u091c\u092e\u094b\u0921\u093f\u092f\u092e \u0935\u093e\u0908\u0935\u0947\u0915\u094d\u0938 , \u092a\u094d\u0932\u093e\u091c\u092e\u094b\u0921\u093f\u092f\u092e \u0913\u0935\u0947\u0932 \u0924\u0925\u093e \u092a\u094d\u0932\u093e\u091c\u092e\u094b\u0921\u093f\u092f\u092e \u092e\u0932\u0947\u0930\u0940 \u0928\u093e\u092e \u0915\u0947 \u092e\u091a\u094d\u091b\u0930\u094b\u0902 \u0926\u094d\u0935\u093e\u0930\u093e \u092b\u0948\u0932\u0924\u093e \u0939\u0948 
p592
aV\u0907\u0938 \u0930\u094b\u0917 \u0938\u092e\u0942\u0939 \u092e\u0947\u0902 \u0935\u093f\u092d\u093f\u0928\u094d\u0928 \u092a\u094d\u0930\u0915\u093e\u0930 \u0915\u0947 \u092e\u0932\u0947\u0930\u093f\u092f\u093e , \u092b\u093e\u0907\u0932\u0947\u0930\u093f\u092f\u093e , \u0921\u0947\u0902\u0917\u0942 \u090f\u0935\u0902 \u092a\u0940\u0924-\u091c\u094d\u0935\u0930 \u0906\u0926\u093f \u0938\u092e\u094d\u092e\u093f\u0932\u093f\u0924 \u0939\u0948\u0902 
p593
aV\u091c\u0932 \u0938\u0947 \u0938\u0902\u092c\u0902\u0927\u093f\u0924 \u0915\u0940\u091f\u093e\u0923\u0941\u0913\u0902 \u0926\u094d\u0935\u093e\u0930\u093e \u092b\u0948\u0932\u0928\u0947 \u0935\u093e\u0932\u0947 \u0930\u094b\u0917 -
p594
aV\u0928\u093e\u0930\u0942 \u0930\u094b\u0917 \u0915\u093e \u0928\u093f\u0926\u093e\u0928 \u0905\u0902\u0924\u0903 \u0936\u093f\u0930\u093e \u092e\u0947\u0902 \u0915\u0941\u091b \u0930\u0938\u093e\u092f\u0928\u092f\u0941\u0915\u094d\u0924 \u091f\u0940\u0915\u093e \u0932\u0917\u0935\u093e\u0915\u0930 \u092d\u0940 \u0915\u093f\u092f\u093e \u091c\u093e \u0938\u0915\u0924\u093e \u0939\u0948 
p595
aV\u0928\u093e\u0930\u0942 \u0930\u094b\u0917 \u0915\u0940 \u0930\u094b\u0915\u0925\u093e\u092e \u0939\u0947\u0924\u0941 \u092c\u093e\u0935\u095c\u093f\u092f\u094b\u0902 , \u0924\u093e\u0932\u093e\u092c\u094b\u0902 \u090f\u0935\u0902 \u091d\u0940\u0932\u094b\u0902 \u0906\u0926\u093f \u0915\u093e \u092a\u093e\u0928\u0940 \u091b\u093e\u0928\u0915\u0930 \u0939\u0940 \u0915\u093e\u092e \u092e\u0947\u0902 \u0932\u0947\u0928\u093e \u091a\u093e\u0939\u093f\u090f 
p596
aV\u0907\u0938 \u0915\u093e\u0930\u0923 \u0930\u094b\u0917\u0940 \u0915\u094b \u092c\u0941\u0916\u093e\u0930 , \u0938\u093f\u0930 \u0926\u0930\u094d\u0926 \u0924\u0925\u093e \u0909\u0932\u094d\u091f\u0940 \u0915\u0947 \u0932\u0915\u094d\u0937\u0923 \u092d\u0940 \u0939\u094b \u091c\u093e\u0924\u0947 \u0939\u0948\u0902 
p597
aV\u0936\u0930\u0940\u0930 \u0915\u0947 \u091c\u093f\u0938 \u092d\u093e\u0917 \u092e\u0947\u0902 \u0915\u0943\u092e\u093f \u0938\u0947 \u0915\u0940\u091f \u0928\u093f\u0915\u0932\u0924\u0947 \u0939\u0948\u0902 , \u0935\u0939\u093e\u0901 \u092a\u0939\u0932\u0947 \u0924\u0947\u091c \u0916\u0941\u091c\u0932\u0940 \u0939\u094b\u0924\u0940 \u0939\u0948 , \u092b\u093f\u0930 \u092b\u092b\u094b\u0932\u093e \u092c\u0928\u0924\u093e \u0939\u0948 \u0914\u0930 \u091b\u0947\u0926 \u0939\u094b \u091c\u093e\u0924\u093e \u0939\u0948 
p598
aV\u0928\u0947\u0924\u0930\u0939\u093e\u091f \u092a\u0920\u093e\u0930 \u0915\u0947 \u0928\u093f\u0915\u091f \u0915\u0940 \u092a\u0939\u093e\u0921\u093c\u093f\u092f\u093e\u0901 \u092a\u093e\u0924 ( \u092a\u093e\u091f ) \u0915\u0939\u0932\u093e\u0924\u0940 \u0939\u0948\u0902 - \u0928\u0947\u0924\u0930\u0939\u093e\u091f \u092a\u093e\u091f , \u092a\u0938\u0947\u0930\u0940 \u092a\u093e\u091f , \u0921\u0942\u092e\u0930 \u092a\u093e\u091f , \u091c\u094b\u092d\u0940 \u092a\u093e\u091f , \u091c\u092e\u0947\u0921\u0942\u0930\u093e \u092a\u093e\u091f , \u0915\u094b\u092f\u0932 \u0914\u0930 \u0936\u0902\u0916 \u0928\u0926\u0940 \u0915\u0947 \u0905\u092a\u0930\u0926\u0928 \u0938\u0947 \u0939\u0941\u0906 \u0939\u0948 
p599
aV\u092e\u0948\u091f \u0914\u0930 \u0915\u0949\u092f\u0932 \u092d\u0940 \u092c\u091a\u094d\u091a\u094b\u0902 \u0915\u0947 \u0932\u093f\u090f \u0928\u0941\u0915\u0938\u093e\u0928\u0926\u093e\u092f\u0915 \u0938\u093e\u092c\u093f\u0924 \u0939\u094b \u0938\u0915\u0924\u0947 \u0939\u0948  
p600
aV\u092c\u0947\u0939\u0924\u0930 \u0939\u094b \u092c\u091a\u094d\u091a\u0947 \u0915\u094b \u092e\u091a\u094d\u091b\u0930\u0926\u093e\u0928\u0940 \u092e\u0947\u0902 \u0938\u0941\u0932\u093e\u090f\u0901  
p601
aV\u092a\u094d\u0930\u0938\u0935 \u0915\u0947 \u092c\u093e\u0926 \u0905\u0917\u0930 \u090f\u0915 \u092e\u0939\u0940\u0928\u0947 \u0915\u0947 \u0905\u0902\u0926\u0930 \u091c\u091a\u094d\u091a\u093e \u0915\u094b \u092c\u0941\u0916\u093e\u0930 \u0906\u090f \u092f\u093e \u092c\u093e\u0930-\u092c\u093e\u0930 \u092c\u0941\u0916\u093e\u0930 \u0909\u0924\u0930 \u091c\u093e\u090f , \u091a\u0922\u093c \u091c\u093e\u090f \u092f\u093e \u092b\u093f\u0930 \u0909\u0938\u0915\u093e \u0916\u0942\u0928 \u092c\u0939\u0928\u093e \u0928 \u0930\u0941\u0915\u0947 \u0924\u094b \u092c\u093f\u0928\u093e \u092a\u0932 \u092c\u0930\u094d\u092c\u093e\u0926 \u0915\u093f\u090f \u0909\u0938\u0947 \u0924\u0941\u0930\u0902\u0924 \u092c\u0921\u093c\u0947 \u0905\u0938\u094d\u092a\u0924\u093e\u0932 \u092a\u0939\u0941\u0901\u091a\u093e\u090f\u0901 
p602
aV\u0938\u093e\u0939\u0938\u093f\u0915 \u092f\u093e\u0924\u094d\u0930\u093e\u090f\u0901 \u092a\u0930\u094d\u092f\u093e\u0935\u0930\u0923 \u092a\u0930\u094d\u092f\u091f\u0928 \u0915\u093e \u090f\u0915 \u0905\u092d\u093f\u0928\u094d\u0928 \u0905\u0902\u0917 \u0939\u0948\u0902 \u090f\u0935\u0902 \u0917\u0924 \u0926\u094b \u0926\u0936\u0915\u094b\u0902 \u092e\u0947\u0902 \u0907\u0928 \u0938\u093e\u0939\u0938\u093f\u0915 \u092f\u093e\u0924\u094d\u0930\u093e\u0913\u0902 \u0915\u0947 \u0905\u092d\u093f\u092f\u093e\u0928\u094b\u0902 \u092e\u0947\u0902 \u092c\u0940\u0938 \u0917\u0941\u0928\u093e \u0935\u0943\u0926\u094d\u0927\u093f \u0939\u0941\u0908 \u0939\u0948 
p603
aV\u0907\u0928 \u0938\u093e\u0939\u0938\u093f\u0915 \u092f\u093e\u0924\u094d\u0930\u093e\u0913\u0902 \u092e\u0947\u0902 \u0939\u093f\u092e\u093e\u0932\u092f\u0940 , \u092e\u0948\u0926\u093e\u0928\u0940 \u092c\u093f\u092f\u093e\u0935\u093e\u0928 , \u0928\u0926\u093f\u092f\u094b\u0902 \u0915\u0947 \u0924\u091f , \u0930\u0947\u0917\u093f\u0938\u094d\u0924\u093e\u0928\u0940 \u0915\u094d\u0937\u0947\u0924\u094d\u0930 , \u0938\u092e\u0941\u0926\u094d\u0930 \u090f\u0935\u0902 \u0938\u092e\u0941\u0926\u094d\u0930\u0940 \u0924\u091f \u0936\u093e\u092e\u093f\u0932 \u0939\u0948\u0902 
p604
aV\u0907\u0928 \u0938\u092c\u0915\u093e \u0935\u094d\u092f\u093e\u092a\u0915 \u0909\u092a\u092f\u094b\u0917 \u092a\u0930\u094d\u092f\u091f\u0915\u094b\u0902 \u0915\u0947 \u092d\u094d\u0930\u092e\u0923 \u0915\u0947 \u0932\u093f\u090f \u0915\u093f\u092f\u093e \u091c\u093e\u0928\u093e \u091a\u093e\u0939\u093f\u090f 
p605
aV\u0909\u0928\u094d\u0939\u0947\u0902 \u0939\u0930 \u0910\u0924\u093f\u0939\u093e\u0938\u093f\u0915 \u0907\u092e\u093e\u0930\u0924 \u0915\u0947 \u092c\u093e\u0930\u0947 \u092e\u0947\u0902 \u092e\u0939\u0924\u094d\u0924\u094d\u0935\u092a\u0942\u0930\u094d\u0923 \u091c\u093e\u0928\u0915\u093e\u0930\u0940 \u0909\u092a\u0932\u092c\u094d\u0927 \u0915\u0940 \u091c\u093e\u0928\u0940 \u091a\u093e\u0939\u093f\u090f 
p606
aV\u0907\u0928\u094d\u0939\u0947\u0902 \u092a\u0930\u094d\u092f\u093e\u092a\u094d\u200d\u0924 \u0938\u0902\u0930\u0915\u094d\u0937\u0923 \u0926\u0947\u0915\u0930 \u0907\u0928\u0915\u0947 \u0930\u0916 - \u0930\u0916\u093e\u0935 \u092a\u0930 \u0935\u093f\u0936\u0947\u0937 \u0927\u094d\u092f\u093e\u0928 \u0926\u0947\u0928\u093e \u091a\u093e\u0939\u093f\u090f 
p607
