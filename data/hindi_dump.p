aV\u0905\u0915\u0938\u0930 \u092f\u0939 \u0930\u094b\u0917 \u0909\u0928 \u0935\u094d\u092f\u0915\u094d\u200d\u0924\u093f\u092f\u094b\u0902 \u0915\u094b \u0939\u094b\u0924\u093e \u0939\u0948 , \u091c\u093f\u0928\u094d\u0939\u0947\u0902 \u0939\u0943\u0926\u092f \u092f\u093e \u0935\u0943\u0915\u094d\u0915 \u0915\u0947 \u0930\u094b\u0917 \u0939\u094b\u0902 \u092f\u093e \u091c\u093f\u0928\u0915\u0940 \u0927\u092e\u0928\u093f\u092f\u093e\u0901 ( rteries ) \u0917\u0920\u093f\u092f\u093e , \u092e\u0926\u094d\u092f\u092a\u093e\u0928 \u092f\u093e \u0909\u092a\u0926\u0902\u0936 \u0915\u0947 \u0915\u093e\u0930\u0923 \u0938\u0916\u094d\u0924 \u092c\u0928 \u0917\u0908 \u0939\u094b\u0902 
p472
aV\u091c\u093f\u0928\u0915\u0947 \u092e\u0938\u094d\u0924\u093f\u0937\u094d\u200d\u0915 \u0915\u0940 \u0927\u092e\u0928\u093f\u092f\u094b\u0902 \u092e\u0947\u0902 \u090f\u0928\u094d\u092f\u0941\u0930\u093f\u091c\u094d\u092e ( neurism ) , \u090f\u092e\u094d\u092c\u094b\u0932\u093f\u091c\u094d\u092e ( mbolism ) \u092f\u093e \u0915\u094b\u0908 \u0930\u0938\u094c\u0932\u0940 ( umour ) \u092c\u0928 \u0917\u0908 \u0939\u094b 
p473
aV\u090f\u0928\u094d\u092f\u0941\u0930\u093f\u091c\u094d\u092e \u0914\u0930 \u090f\u092e\u094d\u092c\u094b\u0932\u093f\u091c\u094d\u092e \u0915\u0947 \u0928\u093f\u0915\u091f \u0927\u092e\u0928\u0940 \u0915\u094b\u092e\u0932 \u0914\u0930 \u0915\u092e\u091c\u094b\u0930 \u092a\u095c \u091c\u093e\u0924\u0940 \u0939\u0948 
p474
aV\u0909\u0938 \u091b\u094b\u091f\u0947 \u0938\u0947 \u0926\u0947\u0936 \u092e\u0947\u0902 \u092e\u093e\u0928\u094b \u0907\u0924\u0928\u0940 \u0916\u0942\u092c\u0938\u0942\u0930\u0924\u0940 \u0938\u092e\u093e\u0924\u0940 \u0928\u0939\u0940\u0902 
p475
aV\u092c\u093e\u091c\u093e\u0930 \u0915\u0940 \u0909\u0938 \u0938\u0948\u0930 \u0938\u0947 \u0935\u093e\u092a\u0938 \u0905\u092a\u0928\u0947 \u0915\u094d\u0930\u0942\u091c \u092a\u0930 \u091c\u093e\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u091c\u0947\u091f\u094d\u200d\u091f\u0940 \u0915\u0940 \u0924\u0930\u092b \u0932\u094c\u091f\u0924\u0947 \u0939\u0941\u090f , \u091c\u092c \u0928\u093f\u0917\u093e\u0939\u0947\u0902 \u092c\u0930\u092c\u0938 \u0938\u095c\u0915 \u0915\u0947 \u0938\u093e\u0925-\u0938\u093e\u0925 \u091a\u0932\u0924\u0947 \u0938\u092e\u0902\u0926\u0930 \u0915\u0947 \u092a\u093e\u0928\u0940 \u092e\u0947\u0902 \u0917\u0908 \u0924\u094b \u0905\u0939\u0938\u093e\u0938 \u0939\u0941\u0906 \u0915\u093f \u092f\u0939\u093e\u0901 \u0915\u0947 \u0932\u094b\u0917 \u0907\u0938 \u0916\u0942\u092c\u0938\u0942\u0930\u0924\u0940 \u0915\u0940 \u0915\u0940\u092e\u0924 \u0915\u093f\u0938 \u0939\u0926 \u0924\u0915 \u0938\u092e\u091d\u0924\u0947 \u0939\u0948\u0902 
p476
aV\u0908\u092e\u093e\u0928\u0926\u093e\u0930\u0940 \u0938\u0947 \u0915\u0939\u0942\u0901 \u0924\u094b \u092e\u0948\u0902\u0928\u0947 \u0915\u093f\u0938\u0940 \u0939\u093f\u0902\u0926\u0941\u0938\u094d\u0924\u093e\u0928\u0940 \u0915\u094b \u0930\u093e\u0937\u094d\u200d\u091f\u094d\u0930\u092a\u0924\u093f\u092f\u094b\u0902 \u092f\u093e \u092a\u094d\u0930\u0927\u093e\u0928\u092e\u0902\u0924\u094d\u0930\u0940 \u0915\u0947 \u092d\u093e\u0937\u0923\u094b\u0902 \u0915\u094b \u0907\u0924\u0928\u0947 \u0927\u094d\u092f\u093e\u0928 \u0938\u0947 \u0938\u0941\u0928\u0924\u0947 \u0928\u0939\u0940\u0902 \u0926\u0947\u0916\u093e 
p477
aV\u0932\u0947\u0915\u093f\u0928 \u091c\u092c \u0907\u0938 \u0927\u0930\u0924\u0940 \u0915\u0940 \u090f\u0915 \u0928\u093e\u092f\u093e\u092c \u0930\u091a\u0928\u093e \u0915\u0947 \u0905\u0938\u094d\u0924\u093f\u0924\u094d\u0924\u094d\u0935 \u0915\u093e \u0939\u0940 \u0938\u0935\u093e\u0932 \u0939\u094b \u0924\u094b , \u0907\u0924\u0928\u0940 \u0917\u0902\u092d\u0940\u0930\u0924\u093e \u0932\u093e\u091c\u093f\u092e\u0940 \u0939\u0940 \u0925\u0940 
p478
aV\u0915\u0908 \u0932\u094b\u0917\u094b\u0902 \u0928\u0947 \u0905\u0915\u094d\u091f\u0942\u092c\u0930 \u092e\u0947\u0902 \u092e\u093e\u0932\u0926\u0940\u0935 \u0915\u0940 \u0915\u0948\u092c\u093f\u0928\u0947\u091f \u0926\u094d\u0935\u093e\u0930\u093e \u092a\u093e\u0928\u0940 \u0915\u0947 \u0905\u0902\u0926\u0930 \u0915\u0940 \u0917\u0908 \u092c\u0948\u0920\u0915 \u0915\u094b \u092d\u0932\u0947 \u0939\u0940 \u091f\u094b\u091f\u0915\u093e \u0938\u092e\u091d\u093e \u0939\u094b , \u0932\u0947\u0915\u093f\u0928 \u092e\u093e\u0932\u0926\u0940\u0935 \u091c\u093e\u0915\u0930 \u0932\u0917\u0924\u093e \u0939\u0948 \u0915\u093f \u0905\u0917\u0930 \u092a\u094d\u0930\u0915\u0943\u0924\u093f \u0915\u093e \u0915\u094b\u092a \u0907\u0938\u0947 \u0939\u092e\u0938\u0947 \u091b\u0940\u0928 \u0932\u0947 \u0924\u094b \u0926\u0941\u0928\u093f\u092f\u093e \u0915\u093f\u0924\u0928\u0940 \u0926\u0930\u093f\u0926\u094d\u0930 \u0939\u094b \u091c\u093e\u090f\u0917\u0940 
p479
aV\u0935\u0939\u093e\u0901 \u0915\u0940 \u0938\u0941\u0902\u0926\u0930\u0924\u093e \u092e\u0947\u0902 \u0915\u0941\u091b \u0910\u0938\u093e \u0939\u0940 \u0916\u093e\u0938 \u0939\u0948 
p480
aV\u0914\u0930 \u0935\u094b \u090f\u0915 \u0910\u0938\u093e \u0938\u092b\u0930 \u0939\u094b\u0924\u093e \u0939\u0948 \u091c\u093f\u0938\u0915\u0947 \u0916\u0924\u094d\u092e \u0939\u094b\u0928\u0947 \u0915\u093e \u092e\u0928 \u0936\u093e\u092f\u0926 \u0906\u092a\u0915\u093e \u0915\u092d\u0940 \u0928 \u0915\u0930\u0947 
p481
aV\u0938\u0940 \u092a\u094d\u0932\u0947\u0928 12 \u0915\u093f\u0932\u094b\u092e\u0940\u091f\u0930 \u0938\u0947 \u0932\u0947\u0915\u0930 183 \u0915\u093f\u0932\u094b\u092e\u0940\u091f\u0930 ( \u0909\u0924\u094d\u0924\u092e \u092e\u0947\u0902 \u092e\u093e\u0935\u0940\u0932\u0947\u0935\u093e\u0930\u0942 ) \u0924\u0915 \u0915\u093e \u0938\u092b\u0930 \u092e\u093e\u0932\u0926\u0940\u0935 \u0915\u0947 \u0926\u094d\u0935\u0940\u092a\u094b\u0902 \u092e\u0947\u0902 \u0915\u0930\u0924\u0947 \u0939\u0948\u0902 
p482
aV\u0939\u092e \u092e\u093e\u0932\u0947 \u0915\u0947 \u0905\u0939\u092e\u0926\u0940 \u092c\u093e\u091c\u093e\u0930 \u092e\u0947\u0902 \u0930\u093e\u091c\u0927\u093e\u0928\u0940 \u0915\u0940 \u0938\u092c\u0938\u0947 \u092c\u095c\u0940 \u090f\u0902\u091f\u0940\u0915 \u0935 \u0938\u094b\u0935\u0947\u0928\u093f\u092f\u0930 \u0926\u0941\u0915\u093e\u0928 \u092e\u0947\u0902 \u0925\u0947 
p483
aV\u0938\u093e\u0930\u0947 \u0938\u0947\u0932\u094d\u0938\u092e\u0948\u0928 \u092e\u093e\u0932\u0926\u0940\u0935 \u0915\u0947 \u0930\u093e\u0937\u094d\u200d\u091f\u094d\u0930\u092a\u0924\u093f \u092e\u094b\u0939\u092e\u094d\u092e\u0926 \u0928\u093e\u0936\u0940\u0926 \u0926\u094d\u0935\u093e\u0930\u093e \u090f\u0915 \u0926\u093f\u0928 \u092a\u0939\u0932\u0947 \u0915\u094b\u092a\u0947\u0928\u0939\u0947\u0917\u0928 \u092e\u0947\u0902 \u0935\u093f\u0936\u094d\u200d\u0935 \u091c\u0932\u0935\u093e\u092f\u0941 \u0938\u092e\u094d\u092e\u0947\u0932\u0928 \u092e\u0947\u0902 \u0926\u093f\u090f \u0917\u090f \u092d\u093e\u0937\u0923 \u0915\u094b \u092c\u095c\u0947 \u0927\u094d\u092f\u093e\u0928 \u0938\u0947 \u0938\u0941\u0928 \u0930\u0939\u0947 \u0925\u0947 
p484
aV\u0938\u094d\u0924\u0928 \u0915\u0948\u0902\u0938\u0930 \u092e\u0947\u0902 \u091c\u094d\u092f\u093e\u0926\u093e\u0924\u0930 \u092e\u093e\u0928\u0938\u093f\u0915 \u0935 \u0906\u0930\u094d\u0925\u093f\u0915 \u0930\u0942\u092a \u0938\u0947 \u091f\u0942\u091f \u091c\u093e\u0924\u0947 \u0939\u0948\u0902 
p485
aV\u0926\u0947\u0930\u0940 \u0938\u0947 \u092a\u0924\u093e \u0932\u0917\u0928\u0947 \u092a\u0930 \u0936\u0932\u094d\u092f-\u091a\u093f\u0915\u093f\u0924\u094d\u0938\u093e \u0915\u0940 \u0917\u0902\u092d\u0940\u0930\u0924\u093e \u092c\u095d\u0928\u0947 \u0938\u0947 \u091c\u093e\u0928 \u092c\u091a\u093e\u0928\u0947 \u092a\u0930 \u092d\u0940 \u092e\u0930\u0940\u091c \u0936\u093e\u0930\u0940\u0930\u093f\u0915 \u0935\u093f\u0915\u0943\u0924\u093f \u0915\u0947 \u092d\u092f \u0935 \u092c\u0940\u092e\u093e\u0930\u0940 \u0915\u0947 \u092b\u093f\u0930 \u0938\u0947 \u0909\u092d\u0930\u0928\u0947 \u0915\u0940 \u092d\u092f\u093e\u0935\u0939 \u0938\u094d\u0925\u093f\u0924\u093f \u0938\u0947 \u0924\u094d\u0930\u0938\u094d\u0924 \u0930\u0939\u0924\u093e \u0939\u0948 
p486
aV\u092f\u0939 \u0915\u093f\u0928 \u0935\u093f\u0937\u092f\u094b\u0902 \u092e\u0947\u0902 \u0905\u0927\u093f\u0915 \u0905\u0902\u0915 \u092a\u094d\u0930\u093e\u092a\u094d\u0924 \u0915\u0930\u0924\u0947 \u0939\u0948\u0902 ? \u092f\u093e \u0915\u093f\u0928 \u092e\u0947\u0902 \u0905\u0927\u093f\u0915 \u0930\u0942\u091a\u093f \u0932\u0947\u0924\u0947 \u0939\u0948\u0902 ? \u0907\u0928\u0915\u0940 \u092c\u094b\u0932\u0928\u0947 \u0915\u0940 \u0915\u094d\u0937\u092e\u0924\u093e \u0915\u0948\u0938\u0940 \u0939\u0948 ?
p487
aV\u092a\u0942\u0930\u0947 \u0938\u094d\u0915\u0942\u0932 \u0915\u094b \u0926\u0947\u0916\u0947\u0902 \u0924\u094b \u0915\u094d\u092f\u093e \u0932\u095c\u0915\u094b\u0902 \u0914\u0930 \u0932\u095c\u0915\u093f\u092f\u094b\u0902 \u092e\u0947\u0902 , \u091b\u094b\u091f\u0947 \u092c\u091a\u094d\u091a\u094b\u0902 \u0914\u0930 \u092c\u095c\u094b\u0902 \u092e\u0947\u0902 , \u0907\u0938 \u0926\u0943\u0937\u094d\u091f\u093f \u0938\u0947 \u0915\u094b\u0908 \u0905\u0928\u094d\u0924\u0930 \u0939\u0948 ?
p488
aV\u0915\u094d\u0930\u093f\u0915\u0947\u091f \u092e\u0947\u0902 \u0924\u094b \u0907\u0928 \u0916\u092c\u094d\u092c\u0942 , \u092f\u093e \u0932\u0948\u092b\u094d\u091f-\u0939\u0948\u0928\u094d\u0921\u0930 \u0916\u093f\u0932\u093e\u095c\u093f\u092f\u094b\u0902 \u0915\u0940 \u0905\u0932\u0917 \u0939\u0940 \u0936\u093e\u0928 \u0939\u0948 
p489
aV\u0906\u092a\u0915\u0947 \u0938\u094d\u0915\u0942\u0932 \u092f\u093e \u0915\u0915\u094d\u0937\u093e \u092e\u0947\u0902 \u0907\u0928\u0915\u093e \u0905\u0928\u0941\u092a\u093e\u0924 \u0915\u094d\u092f\u093e \u0939\u0948 ?
p490
aV\u0938\u094d\u0924\u0928 \u0915\u0948\u0902\u0938\u0930 \u092e\u0939\u093f\u0932\u093e\u0913\u0902 \u0915\u0947 \u0932\u093f\u090f \u090f\u0915 \u092d\u092f\u093e\u0935\u0939 \u0928\u093e\u092e \u0939\u0948 
p491
aV\u0936\u0941\u0930\u0941\u0906\u0924\u0940 \u0926\u093f\u0928\u094b\u0902 \u092e\u0947\u0902 \u0938\u094d\u0924\u0928 \u0915\u0948\u0902\u0938\u0930 \u0915\u0940 \u092a\u0939\u091a\u093e\u0928 \u0928 \u0939\u094b\u0928\u0947 \u0938\u0947 \u0939\u091c\u093e\u0930\u094b\u0902 \u092e\u094c\u0902\u0924\u0947\u0902 \u0939\u094b\u0924\u0940 \u0939\u0948 
p492
aV\u0932\u0947\u0915\u093f\u0928 \u092a\u0939\u0932\u0947 \u0939\u0930 \u091a\u0940\u095b \u0915\u094b \u0928\u093e\u092a\u0928\u0947 \u0915\u0947 \u0932\u093f\u090f \u0915\u0941\u091b \u0928 \u0915\u0941\u091b \u092d\u094c\u0924\u093f\u0915 \u092e\u093e\u092a\u0926\u0923\u094d\u0921 \u0924\u092f \u0915\u0930\u0928\u093e \u0939\u094b\u0917\u093e 
p493
aV\u092c\u093f\u0928\u093e \u0907\u0938\u0915\u0947 \u0915\u093f\u0938\u0940 \u092d\u0940 \u0938\u0939\u0940 \u0935\u0948\u091c\u094d\u091e\u093e\u0928\u093f\u0915 \u0928\u093f\u0937\u094d\u0915\u0930\u094d\u0937 \u092a\u0930 \u092a\u0939\u0941\u0901\u091a\u0928\u093e \u0915\u0920\u093f\u0928 \u0939\u0948 
p494
aV\u0938\u0915\u0932 \u092a\u094d\u0930\u091c\u0928\u0928 \u0926\u0930 ( \u0915\u093f\u0938\u0940 \u092e\u0939\u093f\u0932\u093e \u0926\u094d\u0935\u093e\u0930\u093e \u0905\u092a\u0928\u0947 \u092a\u094d\u0930\u091c\u0928\u0928 \u091c\u0940\u0935\u0928 \u0915\u093e\u0932 \u092e\u0947\u0902 \u091c\u0928\u094d\u092e\u0947\u0902 \u0917\u090f \u0936\u093f\u0936\u0941\u0913\u0902 \u0915\u0940 \u0938\u0902\u0916\u094d\u092f\u093e ) \u091c\u094b \u0915\u093f 1971-76 \u0915\u0940 \u0905\u0935\u0927\u093f \u0915\u0947 \u0932\u093f\u090f 5 \u0938\u0947 \u0905\u0927\u093f\u0915 \u0905\u0928\u0941\u092e\u093e\u0928\u093f\u0924 \u0925\u0940 , \u0928\u093f\u0930\u0928\u094d\u0924\u0930 \u0918\u091f \u0930\u0939\u0940 \u0939\u0948 \u0924\u0925\u093e \u0935\u0930\u094d\u0937 2001 \u092e\u0947\u0902 \u092f\u0939 \u0938\u0902\u0916\u094d\u092f\u093e 3.3 \u0925\u0940 
p495
aV\u0930\u093e\u091c\u093e \u092c\u0926\u0928\u0938\u093f\u0902\u0939 \u0915\u0947 \u092c\u093e\u0926 \u0909\u0928\u0915\u0947 \u092a\u0941\u0924\u094d\u0930 \u0938\u0942\u0930\u091c\u092e\u0932 \u0928\u0947 \u0921\u0940\u0917 \u0915\u093e \u0935\u093f\u0938\u094d\u0924\u093e\u0930 \u0915\u093f\u092f\u093e \u0914\u0930 \u092f\u0939\u093e\u0901 \u0905\u0928\u0947\u0915 \u092d\u0935\u094d\u092f \u0907\u092e\u093e\u0930\u0924\u0947\u0902 \u092c\u0928\u0935\u093e\u0908 
